    <DataSet Name="DataSet2">
      <Fields>
        <Field Name="Platform">
          <DataField>Platform</DataField>
        </Field>
        <Field Name="LocPlatform">
          <Value>=SrsResources.Localization.GetDBString(Fields!Platform.Value, User!Language, "SMS_SupportedPlatforms-DisplayText")</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          BEGIN
          IF (@filterwildcard = '')
          SELECT DISTINCT DisplayText AS [Platform] FROM fn_rbac_SupportedPlatforms(@UserSIDs) 
          WHERE OSMinVersion != OSMaxVersion AND ClientSupported = 1
          ORDER BY DisplayText
          ELSE
          SELECT DISTINCT DisplayText AS [Platform] FROM fn_rbac_SupportedPlatforms(@UserSIDs) 
          WHERE DisplayText LIKE @filterwildcard AND OSMinVersion != OSMaxVersion AND ClientSupported = 1
          ORDER BY DisplayText
          END</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
