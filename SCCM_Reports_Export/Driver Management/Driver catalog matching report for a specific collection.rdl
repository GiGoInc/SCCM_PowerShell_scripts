      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>BEGIN 
 IF (@filterwildcard = '') 
  SELECT DISTINCT CollectionID, Name FROM fn_rbac_Collection(@UserSIDs)  ORDER BY Name 
 ELSE 
  SELECT DISTINCT CollectionID, Name FROM fn_rbac_Collection(@UserSIDs)  
  WHERE CollectionID like @filterwildcard 
  ORDER BY Name 
END</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
        </QueryParameters>
      </Query>
