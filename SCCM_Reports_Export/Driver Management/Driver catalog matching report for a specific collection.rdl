<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <rdl:Language xmlns:rdl="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition">=User!Language</rdl:Language>
  <DataSources>
    <DataSource Name="AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_">
      <DataSourceReference>default_reference</DataSourceReference>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet0">
      <Fields>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
        </Field>
        <Field Name="SystemDevice0">
          <DataField>SystemDevice0</DataField>
        </Field>
        <Field Name="InstalledCounts">
          <DataField>InstalledCounts</DataField>
        </Field>
        <Field Name="TargetOperatingSystem0">
          <DataField>TargetOperatingSystem0</DataField>
        </Field>
        <Field Name="CompatibleDriverInCatalog0">
          <DataField>CompatibleDriverInCatalog0</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>SELECT @targetCollection AS CollectionID 
     , SystemDevice0 
     , COUNT(DISTINCT ResourceID) AS InstalledCounts 
	 , @targetPlatform AS TargetOperatingSystem0 
     , CASE WHEN SUM(HasDriver)=0 THEN ''  
            WHEN COUNT(ResourceID) = SUM(HasDriver) THEN 'Yes'   
            ELSE 'No'  
            END AS CompatibleDriverInCatalog0 
FROM	  
	( 
		SELECT DeviceTable.Name0 AS SystemDevice0  
			   , DeviceTable.ResourceID       
			   , CASE WHEN MAX(DriverTable.CI_ID) IS NULL THEN 0 ELSE 1 END AS HasDriver 
		FROM 
			(	 
				SELECT DISTINCT Devices.Name0 
	   				   , Devices.ResourceID 
					   , LTRIM(SubString) AS HardwareID 
				FROM fn_rbac_GS_SYSTEM_DEVICES(@UserSIDs)  AS Devices 
				JOIN fn_rbac_FullCollectionMembership(@UserSIDs)  AS Collections  
					ON Collections.ResourceID = Devices.ResourceID 
				CROSS APPLY dbo.fn_SplitString(IsNull(HardwareIDs0+','+CompatibleIDs0,IsNull(HardwareIDs0,CompatibleIDs0)), ',') 
				WHERE CollectionID = @targetCollection 
			) DeviceTable 
			LEFT JOIN  
			(	 
				SELECT Drivers.CI_ID, HardwareIds.HardwareID 
				FROM fn_rbac_CI_DriversCIs(@UserSIDs)  AS Drivers  
				LEFT JOIN fn_rbac_CI_ApplicablePlatforms(@UserSIDs)  AS Platforms  
					ON Drivers.CI_ID=Platforms.CI_ID 
				INNER JOIN fn_rbac_CI_DriverHardwareIDs(@UserSIDs)  AS HardwareIds  
					ON Drivers.CI_ID=HardwareIds.CI_ID 
				WHERE Platforms.OSDisplayName=@targetPlatform OR  
					  Platforms.OSDisplayName IS NULL  
			) DriverTable	ON DeviceTable.HardwareID = DriverTable.HardwareID 
		GROUP BY DeviceTable.Name0,DeviceTable.ResourceID 
	) Results 
GROUP BY SystemDevice0 
ORDER BY SystemDevice0</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="targetCollection">
            <Value>=Parameters!targetCollection.Value</Value>
          </QueryParameter>
          <QueryParameter Name="targetPlatform">
            <Value>=Parameters!targetPlatform.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
