      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          declare @CIID int = (select CI_ID from fn_rbac_UpdateInfo(@lcid,@UserSIDs)  where CI_UniqueID=@UpdateID)
          declare @StateID int = case when IsNumeric(@Status)=1 then cast(@Status as int) else (select StateID from fn_rbac_StateNames(@UserSIDs)  where TopicType=500 and StateName=@Status) end

          if @StateID is not null
          select
            ucs.ResourceID,
            m.Name0 as ComputerName0,
            m.User_Domain0+'\'+User_Name0 as LastLoggedOnUser,
            asite.SMS_Assigned_Sites0 as AssignedSite,
            m.Client_Version0 as ClientVersion,
            LastStatusCheckTime as LastStateReceived,
            LastStatusChangeTime as LastStateChange
          from fn_rbac_Update_ComplianceStatusAll(@UserSIDs)  ucs
          join fn_rbac_UpdateInfo(@lcid,@UserSIDs)  ui on ucs.CI_ID=ui.CI_ID
          join fn_rbac_ClientCollectionMembers(@UserSIDs)  cm on cm.ResourceID=ucs.ResourceID
          join fn_rbac_R_System(@UserSIDs)  m on m.ResourceType=5 and m.ResourceID=ucs.ResourceID and isnull(m.Obsolete0,0)&lt;&gt;1
          left join fn_rbac_RA_System_SMSAssignedSites(@UserSIDs)  asite on m.ResourceID=asite.ResourceID
          where ucs.CI_ID=@CIID 
          and cm.CollectionID=@CollID 
          and ucs.Status=@StateID
          order by m.Name0
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollID">
            <Value>=Parameters!CollID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UpdateID">
            <Value>=Parameters!UpdateID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
