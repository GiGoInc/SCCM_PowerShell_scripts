  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@xdays">
            <Value>=Parameters!xdays.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Title">
            <Value>=Parameters!Title.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollectionName">
            <Value>=Parameters!CollectionName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
        -------------------------------
        -- Drop any temp table if exists
        -------------------------------

        IF OBJECT_ID('tempdb.dbo.#sColl', 'U') IS NOT NULL
            DROP TABLE #sColl;
        IF OBJECT_ID('tempdb.dbo.#HC', 'U') IS NOT NULL
            Drop Table #HC;
        IF OBJECT_ID('tempdb.dbo.#IsMem', 'U') IS NOT NULL
            Drop Table #IsMem ;

        -------------------------------
        -- Identify Clients in Collection and Healthy Machines
        -------------------------------

        select ResourceID Into #IsMem from v_ClientCollectionMembers where CollectionID in (Select SiteID  from V_Collections Where collectionname like @CollectionName)

        SELECT
            sys.ResourceID
          , Case When (LastAgenttime &gt;= Getdate()-@xdays OR lastonline &gt;= Getdate()-@xdays OR lastddr &gt;= Getdate()-@xdays) Then 1 Else 0 End As IsOn
          ,Case When sys.ResourceID in (select ResourceID from #IsMem) Then 1 Else 0 End As MemberInCollection
        Into
            #HC
        FROM
            v_r_system sys
        Left Join
        (
            select ResourceID, max(AgentTime) LastAgenttime from v_AgentDiscoveries WHERE AgentName = 'Heartbeat Discovery' group by ResourceID
        ) A ON A.ResourceId = sys.ResourceID
        Left Join
            v_CH_ClientSummary cs on cs.ResourceId = sys.ResourceID
        WHERE
            ISNULL(Obsolete0, 0) &lt;&gt; 1 AND ISNULL(Decommissioned0, 0) &lt;&gt; 1 AND Client0 = 1 and Active0 = 1 and Client_Type0 = 1
        order by sys.ResourceID

        -------------------------------
        -- Update Summary by BulletinID
        -------------------------------

        -- Get Update ID or Bundled ID for the Report to render data

        DECLARE @CI_ID INT =
        (
            select CI_ID as AuthorizationListID
            from v_AuthListInfo where
            Title = @Title
        );

        -- GET CI_IDS for the bulletin(s) passed in

        DECLARE @BulletinTbl TABLE (
           CI_ID INT,
           CIType_ID INT,
           BulletinID VARCHAR(64),
           ArticleID VARCHAR(64),
           Severity INT,
           Title varchar(264),
           PRIMARY KEY (CI_ID,BulletinID)
        );

        INSERT INTO @BulletinTbl
        SELECT
           ui.CI_ID,
           ui.CIType_ID,
           ui.BulletinID,
           ui.ArticleID,
           ui.Severity,
           ui.Title
        FROM
           dbo.v_UpdateInfo ui
        WHERE
           IsSuperseded = 0
           AND IsExpired = 0
           AND Not(CIType_ID='9')
           And ui.CI_ID in (select BundledCI_ID from v_BundledConfigurationItems  where CI_ID= @CI_ID)  --Get the sublist of CI from BundledCI
        select
                LocalCollectionID=c.CollectionID,
                c.CollectionID,  c.Name as CollectionName,
                c.Name,
                CI_ID,
                NumTotal,
                NumKnown,
                LastSummaryTime=getutcdate(),
                NumberInCollection=(Select Sum(MemberInCollection) from #HC),
                HealthyClients=ISNULL(NumTotal, 0),
                NumNotApplicable=ISNULL(NumKnown, 0)-isnull(NumMissing, 0)-isnull(NumPresent, 0),
                NumMissing=ISNULL(NumMissing, 0),
                NumPresent=ISNULL(NumPresent, 0),
                NumInstalled=ISNULL(NumInstalled, 0),
                NumFailed=ISNULL(NumFailed, 0),
                NumPending=ISNULL(NumEnforced, 0)-ISNULL(NumFailed, 0),
                NumUnknown=ISNULL(NumTotal, 0)-ISNULL(NumKnown, 0)
            Into #SColl
            from v_collection c
            outer apply
            (
                select
                    ci.CI_ID,
                    NumTotal=count(*),
                    NumKnown=isnull(sum(case when cs.Status&gt;0 or ss.ResourceID is not null then 1 else 0 end),0),
                    NumMissing=isnull(sum(case when cs.Status=2 then 1 else 0 end), 0),
                    NumPresent=ISNULL(sum(case when cs.Status=3 then 1 else 0 end), 0),
                    NumInstalled=ISNULL(sum(case when cs.Status=3 and cs.EnforcementSource in (1,3) then 1 else 0 end), 0),
                    NumEnforced=isnull(sum(case when cs.Status=2 and cs.LastEnforcementMessageID is not null then 1 else 0 end), 0),
                    NumFailed=isnull(sum(case when cs.Status=2 and cs.LastEnforcementMessageID in (6,11,14) then 1 else 0 end), 0)
                from v_ClientCollectionMembersWithAgentEdition cm -- v_ClientCollectionMembers cm
                cross join v_UpdateCIs ci
                left join v_UpdateScanStatus ss on ss.ResourceID=cm.ResourceID and ss.UpdateSource_ID=ci.UpdateSource_ID and ss.ScanPackageVersion&gt;=ci.MinSourceVersion
                left join v_Update_ComplianceStatusAll cs on cs.ResourceID=cm.ResourceID and cs.CI_ID=ci.CI_ID and cs.Status&gt;0
                where
                    cm.CollectionID=(Select CollectionID from v_Collection where Name = @CollectionName)
                    and cm.ResourceID in (select distinct resourceid from #HC  WHERE IsOn = 1  and MemberinCollection &gt; 0)
                    and ci.CI_ID= @CI_ID
                    and c.Name = @CollectionName
                group by ci.CI_ID
            ) cs

        DECLARE @COLCount INT = (SELECT NumberInCollection FROM #sColl WHERE CollectionName IN (@CollectionName) and NumberInCollection &gt; 0 );
        DECLARE @HCnt INT = (SELECT HealthyClients FROM #sColl WHERE CollectionName IN (@CollectionName) and NumberInCollection &gt; 0 );
        DECLARE @ScanErrorMissing INT = (SELECT NumUnknown FROM #sColl WHERE CollectionName IN (@CollectionName) and NumberInCollection &gt; 0 );

        --------------------------------

        WITH 
            UpdateSummaryCTE AS
            (
            SELECT
                b.CI_ID,
                b.CIType_ID,
                b.BulletinID,
                b.ArticleID,
                b.Severity,
                b.Title,
			    COUNT(DISTINCT ucs.ResourceID) [Missing],
			    @CollectionName as CollectionName
            FROM
                v_Update_ComplianceStatusAll ucs
                Left Join v_UpdateScanStatus vs on ucs.ResourceID = vs.ResourceID
                JOIN @BulletinTbl b
                    ON ucs.CI_ID = b.CI_ID
                    WHERE
                        ucs.Status in ('0','2') and ----[0]=NumUnknown, [1]=NumNotApplicable, [2]=NumMissing, [3]=NumPresent
                        ucs.ResourceID in (select distinct resourceid from #HC WHERE IsOn = 1 and MemberinCollection &gt; 0)
            GROUP BY
                b.CI_ID,
                b.CIType_ID,
                b.BulletinID,
                b.ArticleID,
                b.Severity,
                b.Title
            ),
            Vendor as
            (
                select cti.CategoryTypeName, cti.CategoryInstanceID, cti.CategoryInstanceName as Vendor, cta.CI_ID
                    FROM v_CategoryInfo cti
                    JOIN v_CICategories_All cta
                    on cti.CategoryInstanceID=cta.CategoryInstanceID
                WHERE cti.CategoryTypeName='Company'
                AND cti.CategoryInstanceName='Microsoft'
            ),
            Product as
            (
                select cti.CategoryTypeName, cti.CategoryInstanceID, cti.CategoryInstanceName as Product, cta.CI_ID
                    FROM v_CategoryInfo cti
                    JOIN v_CICategories_All cta
                    on cti.CategoryInstanceID=cta.CategoryInstanceID
                WHERE cti.CategoryTypeName='Product'
            ),
            UpdateClassification as
            (
                select cti.CategoryTypeName, cti.CategoryInstanceID, cti.CategoryInstanceName as UpdateClassification, cta.CI_ID
                    FROM v_CategoryInfo cti
                    JOIN v_CICategories_All cta
                    on cti.CategoryInstanceID=cta.CategoryInstanceID
                WHERE cti.CategoryTypeName='UpdateClassification'
            )

        SELECT DISTINCT
            Vendor,
            Product,
            UpdateClassification,
            BulletinID,
            ArticleID,
            cte.CI_ID,
            Severity,
            type.TypeName,
            Title,
            cte.CollectionName,
		    Case
                When @HCnt &gt; 0
                    Then CAST(100.0*(@HCnt-[Missing]-@ScanErrorMissing)/@HCnt AS DECIMAL(5,2))
		        Else 0
            End AS [CompliantPercent],
            @COLCount AS [Total],
            @HCnt As [HealthyClients],
            Case
                When @HCnt &gt; 0
                    Then @HCnt-[Missing]-@ScanErrorMissing
                Else 0
            End AS Compliant,
            [Missing] AS [NonCompliant],
            @ScanErrorMissing  as [Unknowns]
        FROM
            UpdateSummaryCTE cte
            JOIN Vendor vend
            ON cte.CI_ID=vend.CI_ID
            JOIN Product prod
            ON cte.CI_ID=prod.CI_ID
            JOIN UpdateClassification upc
            ON cte.CI_ID=upc.CI_ID
            JOIN v_CITypes type
            ON cte.CIType_ID=type.CIType_ID
        ORDER BY BulletinID
</CommandText>
      </Query>
