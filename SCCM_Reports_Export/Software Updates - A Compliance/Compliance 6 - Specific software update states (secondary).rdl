  <DataSets>
    <DataSet Name="DataSet0">
      <Fields>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
        </Field>
        <Field Name="StateID">
          <DataField>StateID</DataField>
        </Field>
        <Field Name="Status">
          <DataField>Status</DataField>
        </Field>
        <Field Name="NumberOfComputers">
          <DataField>NumberOfComputers</DataField>
        </Field>
        <Field Name="PComputers">
          <DataField>PComputers</DataField>
        </Field>
        <Field Name="UniqueUpdateID">
          <DataField>UniqueUpdateID</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
            declare @UpdateID int = (select CI_ID from fn_rbac_UpdateCIs(@UserSIDs)  where CI_UniqueID=@UpdateGuid)
            select
                CollectionID = @CollID,
                StateID = sn.StateID,
                Status = sn.StateName,
                NumberOfComputers,
                PComputers = convert(float, NumberOfComputers)/isnull(nullif(NumTotal, 0), 1),
                UniqueUpdateID = @UpdateGuid
            from (select NumTotal, [0]=NumUnknown, [1]=NumNotApplicable, [2]=NumMissing, [3]=NumPresent from fn_rbac_UpdateSummaryPerCollection(@UserSIDs)  where CollectionID=@CollID and CI_ID=@UpdateID) stc
            unpivot (NumberOfComputers for StateID in ([0], [1], [2], [3]) ) upvt
            join fn_rbac_StateNames(@UserSIDs)  sn on sn.TopicType=500 and sn.StateID=upvt.StateID
            order by 4 desc
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollID">
            <Value>=Parameters!CollID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UpdateGuid">
            <Value>=Parameters!UpdateID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet2">
