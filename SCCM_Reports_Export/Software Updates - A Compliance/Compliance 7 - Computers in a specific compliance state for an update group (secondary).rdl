        </Field>
        <Field Name="StateName">
          <Value>=SrsResources.Localization.GetDBString(Fields!UnlocStateName.Value, User!Language)</Value>
        </Field>
        <Field Name="UnlocStateDescription">
          <DataField>StateDescription</DataField>
        </Field>
        <Field Name="StateDescription">
          <Value>=SrsResources.Localization.GetDBString(Fields!UnlocStateDescription.Value, User!Language)</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
              begin
              if (@filterwildcard = '')
              select StateName, StateDescription from fn_rbac_StateNames(@UserSIDs)  sn where TopicType=300 and sn.StateID&lt;3
              else
              select StateName, StateDescription from fn_rbac_StateNames(@UserSIDs)  sn where TopicType=300 and sn.StateID&lt;3
              and ((sn.StateName like @filterwildcard) or (sn.StateID like @filterwildcard))
              end
            </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
        </QueryParameters>
      </Query>
