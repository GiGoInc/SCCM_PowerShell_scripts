        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
              declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
              select CI_ID as AuthListID, Title as Title 
              from fn_rbac_AuthListInfo(@lcid, @UserSIDs)
              where (@filterwildcard='' or (CI_UniqueID like @filterwildcard) or (Title like @filterwildcard))
              order by Title
            </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
