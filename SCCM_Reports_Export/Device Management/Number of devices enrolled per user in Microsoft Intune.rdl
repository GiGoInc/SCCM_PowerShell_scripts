      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
        -- [1.] get users from collection members joined with user_disc (on itemkey)
        CREATE TABLE 
        #users
        (
            DiscUserID INT, 
            UserName nvarchar(256),
            UserID uniqueidentifier,
            UPN nvarchar(256)
        )
        CREATE INDEX ix_users ON #users (DiscUserID) 

        INSERT INTO #users(DiscUserID, UserName, UserID, UPN)
        SELECT DISTINCT ud.ResourceID,  ud.Unique_User_Name0, ud.CloudUserId, ud.User_Principal_Name0
        FROM fn_rbac_Collections(@UserSIDs)  co 
        JOIN fn_rbac_CollectionExpandedUserMembers(@UserSIDs)  cm ON cm.SiteID = co.SiteID
        JOIN fn_rbac_R_User(@UserSIDs)  ud ON ud.ResourceID = cm.UserItemKey
        WHERE co.SiteID = @CollectionID
                        

        -- [2.] join #users with UMR (on unique username) and then with SYSTEM_DISC (on ResourceID/ItemKey) to populate this table
        CREATE TABLE 
        #machines
        (
            DiscUserID INT, 
            DeviceID uniqueidentifier -- Remove GUID: FROM SMSID to get this
        )
        CREATE INDEX ix_machines1 ON #machines (DiscUserID)
        CREATE INDEX ix_machines2 ON #machines (DeviceID)

        INSERT INTO #machines(DiscUserID, DeviceID)
        SELECT users.DiscUserID, CONVERT(uniqueidentifier, CASE  LEN(machines.SMS_Unique_Identifier0) WHEN 36 then machines.SMS_Unique_Identifier0 ELSE SUBSTRING(machines.SMS_Unique_Identifier0,6,LEN(machines.SMS_Unique_Identifier0)-5) end)
        FROM #users users
        JOIN fn_rbac_usermachinerelation(@UserSIDs) relations ON relations.UniqueUserName = users.UserName
        JOIN fn_rbac_R_System(@UserSIDs) machines ON machines.ResourceID = relations.MachineResourceID
        WHERE ISNULL(machines.Obsolete0, 0) &lt;&gt; 1 AND ISNULL(machines.Decommissioned0, 0) &lt;&gt; 1 AND machines.Client0 = 1 


        --join #machines with MDMClientIdentity and MDMDeviceManagementStates and do GROUP BY on DiscUserID to get count of valid devices
        CREATE TABLE 
        #userdevices
        (
            DiscUserID INT, 
            NumDevices INT
        )
        CREATE INDEX ix_userdevices ON #userdevices (DiscUserID)

        INSERT INTO #userdevices(DiscUserID, NumDevices)
        SELECT machines.DiscUserID, COUNT(mdmci.DeviceID)
        FROM #machines machines
        JOIN fn_rbac_mdmclientidentity(@UserSIDs) mdmci ON mdmci.DeviceID = machines.DeviceID
        JOIN fn_rbac_MDMDeviceManagementStates(@UserSIDs) mdmdms ON mdmdms.DeviceID = machines.DeviceID
        WHERE
        mdmci.ClientState IN (2,5) AND
        (mdmdms.Wipe = 0 OR mdmdms.Wipe IS NULL) AND
        (mdmdms.Retire = 0 OR mdmdms.Retire IS NULL)
        GROUP BY machines.DiscUserID


        -- finally here join #userdevices back with #users to get UserName, UserID and UPN and display them

        SELECT @CollectionID as CollectionID, users.DiscUserID, users.UPN, users.UserID, users.UserName, ISNULL(userdevices.NumDevices,0) as NumDevices, CASE WHEN users.UserID IS NULL THEN '' ELSE '*' END AS Enabled
        FROM #users users
        LEFT JOIN #userdevices userdevices ON userdevices.DiscUserID = users.DiscUserID
        ORDER BY NumDevices DESC
        DROP TABLE #users
        DROP TABLE #machines
        DROP TABLE #userdevices
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionName_Param.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
