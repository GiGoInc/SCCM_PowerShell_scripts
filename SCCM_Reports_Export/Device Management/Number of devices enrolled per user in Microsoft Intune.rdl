      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
        Select distinct coll.CollectionID, coll.Name as CollectionName  
        from fn_rbac_Collection(@UserSIDs)  coll 
        where coll.CollectionType = 1
        order by coll.Name
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
