    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet0">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WindowsPhone">
            <Value>=Parameters!WindowsPhone.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WindowsMobile">
            <Value>=Parameters!WindowsMobile.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@iOS">
            <Value>=Parameters!iOS.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Android">
            <Value>=Parameters!Android.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WinRT">
            <Value>=Parameters!WinRT.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OtherDevices">
            <Value>=Parameters!OtherDevices.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OthersReportParam">
            <Value>=Parameters!OthersReportParam.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          DECLARE @temp_t table (MobileCount bigint, DeviceType nvarchar(max))


          INSERT INTO @temp_t
          SELECT    COUNT(*) as MobileCount, DeviceType as DeviceType
          FROM      fn_rbac_EAS_Property_s(@UserSIDs)  where Client_Type0 is NULL
          GROUP BY DeviceType

          UNION ALL

          SELECT    COUNT(*) as MobileCount, Platform0 as DeviceType
          FROM      fn_rbac_GS_DEVICE_OSINFORMATION(@UserSIDs)  as d INNER JOIN fn_rbac_R_System(@UserSIDs)  as s on d.ResourceID = s.ResourceID
          GROUP BY Platform0

          UNION ALL

          SELECT    COUNT(*) as MobileCount, CI.DeviceOperatingSystem as DeviceType
          from fn_rbac_mdmclientidentity(@UserSIDs) CI where CI.DeviceOperatingSystem is not null and CI.DeviceType = 1
          GROUP BY CI.DeviceOperatingSystem

          ORDER BY DeviceType

          select
          SUM(MobileCount) as Number, 'Windows Phone' as MobilePlatform
          from
          @temp_t
          where
          DeviceType in (@WindowsPhone)

          UNION ALL

          select
          SUM(MobileCount) as Number, 'Windows Embedded Handheld' as MobilePlatform
          from
          @temp_t
          where
          DeviceType in ('Windows Embedded Handheld')

          UNION ALL

          select
          SUM(MobileCount) as Number,
          'Windows Mobile' as MobilePlatform
          from
          @temp_t
          where
          DeviceType in (@WindowsMobile)

          UNION ALL

          select
          SUM(MobileCount) as Number,
          'iOS' as MobilePlatform
          from
          @temp_t
          where
          DeviceType in (@iOS)

          UNION ALL

          select
          SUM(MobileCount) as Number,
          'Android' as MobilePlatform
          from
          @temp_t
          where
          DeviceType in (@Android)

          UNION ALL

          select
          SUM(MobileCount) as Number,
          'Microsoft Windows RT' as MobilePlatform
          from
          @temp_t
          where
          DeviceType in (@WinRT)

          UNION ALL

          select
          SUM(MobileCount) as Number,
          @OthersReportParam as MobilePlatform
          from
          @temp_t
          where
          DeviceType in (@OtherDevices)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Number">
          <DataField>Number</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="MobilePlatform">
          <DataField>MobilePlatform</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
