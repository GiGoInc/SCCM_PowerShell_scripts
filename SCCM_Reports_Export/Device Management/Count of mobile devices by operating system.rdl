                 and DeviceType not in (@WinRT)
                 and DeviceType not in ('Windows Embedded Handheld')
          )
          
          select DeviceType, DeviceType as DeviceTypeLoc
          from @temp_t
          where
               DeviceType not in (@WindowsPhone)
               and DeviceType not in (@WindowsMobile)
               and DeviceType not in (@iOS)
               and DeviceType not in (@Android)
               and DeviceType not in (@WinRT)
               and DeviceType not in ('Windows Embedded Handheld')
 ELSE
  
 Select 'None' as DeviceType, @NoneLoc as DeviceTypeLoc</CommandText>
