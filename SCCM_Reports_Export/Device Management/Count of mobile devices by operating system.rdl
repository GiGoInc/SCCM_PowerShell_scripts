    <DataSet Name="DistinctDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
          select distinct DeviceType
          from

          (
          SELECT    distinct DeviceType
          FROM      fn_rbac_EAS_Property_s(@UserSIDs)  where Client_Type0 is NULL

          UNION ALL

          SELECT    distinct Platform0 as DeviceType
          FROM            fn_rbac_GS_DEVICE_OSINFORMATION(@UserSIDs)   as d INNER JOIN fn_rbac_R_System(@UserSIDs)  as s on d.ResourceID = s.ResourceID where Platform0 is not null

          UNION ALL

          SELECT    distinct CI.DeviceOperatingSystem as DeviceType
          from fn_rbac_mdmclientidentity(@UserSIDs) CI where CI.DeviceOperatingSystem is not null

          ) as temp</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
