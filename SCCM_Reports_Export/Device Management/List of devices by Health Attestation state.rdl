    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@collectionId">
            <Value>=Parameters!CollectionID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@HASNotRequired">
            <Value>=Parameters!HASNotRequired.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OnlyHASSupportParameter">
            <Value>=Parameters!OnlyHASSupportParameter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AttributesStatusParameter">
            <Value>=Parameters!AttributesStatusParameter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OperatingSystemParameter">
            <Value>=Parameters!OperatingSystemParameter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllStatus">
            <Value>=Parameters!AllStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NoBitLocker">
            <Value>=Parameters!NoBitLocker.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NoSecureBoot">
            <Value>=Parameters!NoSecureBoot.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NoCodeIntegrity">
            <Value>=Parameters!NoCodeIntegrity.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NoELAMDriver">
            <Value>=Parameters!NoELAMDriver.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
		  SELECT DISTINCT
			  HAReport.DeviceName AS "Device Name"
			  ,HAReport.OperatingSystem AS "Operating System"
			  ,HAReport.CodeIntegrityEnabled AS "Code Integrity"
			  ,HAReport.EarlyLaunchAntiMalware AS "Early-launch Antimalware"
			  ,HAReport.SecureBoot AS "Secure Boot"
			  ,HAReport.AttestationIdentityKey AS "Attestation Identity Key (AIK)"
			  ,HAReport.DepPolicy AS "Data Execution Prevention Policy"
			  ,HAReport.SafeMode AS "Safe Mode"
			  ,HAReport.WinPeEnvironment AS "WinPE Environment"
			  ,HAReport.VSM AS "Virtual Secure Mode (VSM)"
			  ,HAReport.BootDebugging AS "Boot Debugging"
			  ,HAReport.KernelDebugging AS "Kernel Debugging"
			  ,HAReport.TestSigning AS "Test Signing"
			  ,HAReport.BootManagerVersion AS "Boot Manager Version"
			  ,HAReport.CodeIntegrityVersion AS "Code Integrity Check Version"
			  ,HAReport.PCR0 AS "PCR Value"
          FROM
			  dbo.fn_rbac_MDMHealthAttestation(@UserSIDs) HAReport
		  INNER JOIN fn_rbac_FullCollectionMembership(@UserSIDs) c on c.ResourceID = HAReport.DeviceItemKey
		  WHERE
          c.CollectionID = @collectionId
		  AND (HAReport.HASSupported = 1 OR @OnlyHASSupportParameter = @HASNotRequired)
		  AND (@OperatingSystemParameter = 'All Platforms' 
            OR (@OperatingSystemParameter = 'Windows Phone' AND HAReport.OperatingSystem LIKE '%Windows Phone%') 
            OR (@OperatingSystemParameter = 'Windows' AND HAReport.OperatingSystem LIKE '%Windows%' AND HAReport.OperatingSystem NOT LIKE '%Phone%'))
		  AND (@AttributesStatusParameter = @AllStatus
			OR (@AttributesStatusParameter = @NoBitLocker AND HAReport.BitLockerStatus &lt;&gt; 1)
			OR (@AttributesStatusParameter = @NoSecureBoot AND HAReport.SecureBoot &lt;&gt; 1)
			OR (@AttributesStatusParameter = @NoCodeIntegrity AND HAReport.CodeIntegrityEnabled &lt;&gt; 1)
			OR (@AttributesStatusParameter = @NoELAMDriver AND HAReport.EarlyLaunchAntiMalware &lt;&gt; 1))
			</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
