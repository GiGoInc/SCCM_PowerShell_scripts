        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          declare @temp_t table (ProductName  nvarchar(256), ProductLoc  nvarchar(256))
          insert into @temp_t
          select '*' as ProductName, @AllLoc as ProductLoc

          insert into  @temp_t
          select distinct product0 as ProductName,  product0 as ProductLoc from fn_rbac_GS_DEVICE_COMPUTERSYSTEM(@UserSIDs)  where product0 is not NULL
          order by product0 desc
          
          insert into @temp_t
          select distinct CI.DeviceOperatingSystem  as ProductName, CI.DeviceOperatingSystem  as ProductLoc
          from fn_rbac_mdmclientidentity(@UserSIDs) CI where CI.DeviceOperatingSystem is not null

          select ProductName, ProductLoc from @temp_t
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllLoc">
            <Value>=SrsResources.Localization.GetString("All", User!Language)</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
