      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          CREATE TABLE
          #machines
          (
          DeviceID uniqueidentifier, -- Remove GUID: FROM SMSID to get this
          ResourceID int,
          SMS_Unique_Identifier0 nvarchar(255),
          Name0 nvarchar(255),
          Netbios_Name0 nvarchar(255),
          Client_Version0 nvarchar(256),
          Client_Type0 int,
          Operating_System_Name_and0  nvarchar(256),
		  DeviceOwner0 int
          )
          CREATE INDEX ix_machines2 ON #machines (DeviceID)

          INSERT INTO #machines(DeviceID, ResourceID, SMS_Unique_Identifier0, Name0, Netbios_Name0,
          Client_Version0, Client_Type0, Operating_System_Name_and0, DeviceOwner0)
          SELECT CONVERT(uniqueidentifier, CASE  LEN(machines.SMS_Unique_Identifier0)
          WHEN 36 then machines.SMS_Unique_Identifier0
          ELSE SUBSTRING(machines.SMS_Unique_Identifier0,6,LEN(machines.SMS_Unique_Identifier0)-5) end),
          machines.ResourceID,
          machines.SMS_Unique_Identifier0,
          machines.Name0,
          machines.Netbios_Name0,
          machines.Client_Version0,
          machines.Client_Type0,
          machines.Operating_System_Name_and0,
          machines.DeviceOwner0
          FROM fn_rbac_R_System(@UserSIDs) machines
          WHERE ISNULL(machines.Obsolete0, 0) &lt;&gt; 1 AND ISNULL(machines.Decommissioned0, 0) &lt;&gt; 1 AND machines.Client0 = 1
          AND ISNULL(machines.AgentEdition0, 0) &lt;&gt; 5 AND machines.Client_Type0 = 3

          Select
          (case when RS.Name0 is not null then RS.Name0 else RS.Netbios_Name0 end) as DeviceName,
          (case when DEV.OEM0 is not null then DEV.OEM0 else CI.Manufacturer end) as DeviceOEMInfo,
          (case when DEV.PlatformType0 is not null then DEV.PlatformType0 else CS.SystemType0 end) as DeviceType,
          (case when RS.Operating_System_Name_and0 = 'Windows Embedded Handheld' then RS.Operating_System_Name_and0 when DEV.Product0 is not null then DEV.Product0 else CI.DeviceOperatingSystem end) as Product,
          (case when ver.Version0 is not null then ver.Version0 else RS.Client_Version0 end) as Version,
          (case when DEV.ProcessorLevel0 is not null then DEV.ProcessorLevel0 else P.Level0 end) as DeviceProcessorLevel,
          (case when DEV.ProcessorRevision0 is not null then DEV.ProcessorRevision0 else P.Revision0 end) as DeviceProcessorRevision,
          (case when OS.Platform0 is not null then RS.Operating_System_Name_and0 else RS.Operating_System_Name_and0 end) as DeviceOSPlatform,
          (case when OS.Version0 is not null then OS.Version0 else WOS.Version0 end) as DeviceOSVersion,
          RS.DeviceOwner0
          from #machines  RS
          Left outer join fn_rbac_GS_DEVICE_COMPUTERSYSTEM(@UserSIDs)  DEV on DEV.ResourceID=RS.ResourceID
          Left outer join fn_rbac_GS_DEVICE_OSINFORMATION(@UserSIDs)  OS on OS.ResourceID=RS.ResourceID
          left outer join fn_rbac_GS_DEVICE_CLIENTAGENTVERSION(@UserSIDs)  ver on  ver.ResourceID=RS.ResourceID
          left outer join fn_rbac_mdmclientidentity(@UserSIDs) CI on RS.DeviceID = CI.DeviceID
          left outer join fn_rbac_GS_COMPUTER_SYSTEM(@UserSIDs) CS on CS.ResourceID = RS.ResourceID
          left outer join fn_rbac_GS_OPERATING_SYSTEM(@UserSIDs) WOS on WOS.ResourceID = RS.ResourceID
          LEFT OUTER JOIN fn_rbac_GS_PROCESSOR(@UserSIDs) P on P.ResourceID = RS.ResourceID
          where (@ProductName = '*' or  DEV.Product0 = @ProductName or CI.DeviceOperatingSystem = @ProductName)
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProductName">
            <Value>=Parameters!ProductName.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
