<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <rdl:Language xmlns:rdl="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition">=User!Language</rdl:Language>
  <Description>This report displays the mobile devices that are jailbroken or rooted</Description>
  <DataSources>
    <DataSource Name="ID5C6358F24BB64a1bA16E8D96795D8602">
      <DataSourceReference>default_reference</DataSourceReference>
      <rd:DataSourceID>feaac5ad-a180-4202-bea2-e69bf51b73fb</rd:DataSourceID>
      <rd:SecurityType>None</rd:SecurityType>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="DeviceName">
          <DataField>DeviceName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="PrimaryUser">
          <DataField>PrimaryUser</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="JailbrokenStatus">
          <DataField>JailbrokenStatus</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="OperatingSystem">
          <DataField>OperatingSystem</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>
        
            IF OBJECT_ID('tempdb..#machines') IS NOT NULL  DROP TABLE #machines
            CREATE TABLE #machines(ItemKey int)
            CREATE INDEX IX on #machines (ItemKey) 

            insert into #machines
            select cm.ResourceID from v_FullCollectionMembership cm where cm.CollectionID = @CollectionID and cm.IsClient=1 

            IF @@ROWCOUNT &lt;&gt; 0
            begin
            Select 
            DeviceName = tmp2.Netbios_Name0, 
            PrimaryUser = tmp.UniqueUserName, 
            JailbrokenStatus = tmp2.JailbrokenStatus,
            OperatingSystem = tmp2.Operating_System_Name_and0
            from #machines machines
            join 
            (
                select 
                devices.ItemKey,
                devices.Netbios_Name0,
                devices.Operating_System_Name_and0,
                JailbrokenStatus = CASE WHEN details.JailBroken0 IS NULL THEN 3 ELSE details.JailBroken0 END
                from SYSTEM_DISC devices
                left join fn_rbac_GS_DEVICE_COMPUTERSYSTEM(@UserSIDs) details on details.ResourceID = devices.ItemKey
            ) tmp2 on tmp2.ItemKey = machines.ItemKey
            left join 
            (
                select pri.UniqueUserName, pri.MachineResourceID from fn_rbac_usermachinerelation(@UserSIDs) pri 
                join v_R_User userdisc on userdisc.Unique_User_Name0 = pri.UniqueUserName
            ) tmp ON tmp.MachineResourceID = machines.ItemKey			
            where (tmp2.JailbrokenStatus = @StateId or @StateId = '')
            end
            IF OBJECT_ID('tempdb..#machines') IS NOT NULL  DROP TABLE #machines
            
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionName_Param.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@StateId">
            <Value>=Parameters!StateName_Param.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet_JailbreakStatus">
      <Fields>
        <Field Name="StateDisplayName">
          <DataField>StateDisplayName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="StateId">
          <DataField>StateId</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>
         ="
        select  N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as StateDisplayName, '' as StateID 
        UNION ALL
        select  N'" &amp;SrsResources.Localization.GetString("JBState_1", User!Language)&amp; "' as StateDisplayName, '1' as StateId
        UNION ALL
        select  N'" &amp;SrsResources.Localization.GetString("JBState_2", User!Language)&amp; "' as StateDisplayName, '2' as StateId
        UNION ALL
        select  N'" &amp;SrsResources.Localization.GetString("JBState_3", User!Language)&amp; "' as StateDisplayName, '3' as StateId
        UNION ALL
        select  N'" &amp;SrsResources.Localization.GetString("JBState_4", User!Language)&amp; "' as StateDisplayName, '4' as StateId
        "
         </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet_Collection">
      <Fields>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
