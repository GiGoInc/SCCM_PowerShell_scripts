        </Field>
        <Field Name="Management_Site_Code">
          <DataField>Management Site Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Email_User_Name">
          <DataField>Email/User Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Certificate_Expiration_Date">
          <DataField>Certificate Expiration Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Last_Certificate_Renewal_Attempt">
          <DataField>Last Certificate Renewal Attempt</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>="select r.DeviceName as [Device Name], 
ISNULL(r.Name, N'" &amp; SrsResources.Localization.GetString("EN_NA", User!Language) &amp; "') as [Enrollment Profile], 
ssaa.SMS_Assigned_Sites0  as [Management Site Code], 
r.RequesterName as [Email/User Name], 
r.CertExpiryTime as [Certificate Expiration Date], 
r.LastCertRenAttempt as [Last Certificate Renewal Attempt] 
from fn_rbac_EN_ClientCertificateRecords(@UserSIDs)  as r 
join fn_rbac_R_System(@UserSIDs)  as sd on r.SMSID = sd.SMS_Unique_Identifier0 
join fn_rbac_System_SMS_Assign_ARR(@UserSIDs)  as ssaa on sd.ResourceID = ssaa.ItemKey 
where r.EnrollmentRecordId = -1 and r.RenewStartTime &lt; GETUTCDATE() and r.Status != 3 and (@SiteCode='' or ssaa.SMS_Assigned_Sites0 =@SiteCode)"</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="SiteCode">
            <Value>=Parameters!SiteCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Label">
          <DataField>Label</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SiteCode">
          <DataField>SiteCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select @AllSitesCode as [Label], '' as [SiteCode]
union all
select SiteCode as [Label], SiteCode as [SiteCode] from fn_rbac_Site(@UserSIDs) </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllSitesCode">
            <Value>=SrsResources.Localization.GetString("AllSitesCode", User!Language)</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
