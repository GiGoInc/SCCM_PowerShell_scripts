  <Description>Mobile devices not applied Exchange default policy</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>default_reference</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>48c1b973-a8c2-4797-82c7-ff3e7db0a72c</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet0">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DeviceTypeList">
            <Value>=Parameters!DeviceTypeList.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          SELECT
          DeviceType,
          DeviceModel,
          COUNT(*) as Counts
          FROM
          (
          SELECT DeviceType as DeviceType,
          DeviceModel as DeviceModel
          FROM fn_rbac_EAS_Property_S(@UserSIDs)  where Client_Type0 is NULL

          UNION ALL

          SELECT os.Platform0 as DeviceType,
          ISNULL(cs.DeviceModel0,'') as DeviceModel
          FROM fn_rbac_GS_DEVICE_OSINFORMATION(@UserSIDs)  os INNER JOIN fn_rbac_R_System(@UserSIDs)  as s on os.ResourceID = s.ResourceID
          LEFT JOIN fn_rbac_GS_DEVICE_COMPUTERSYSTEM(@UserSIDs)  cs
          ON cs.ResourceID = os.ResourceID
          
          UNION ALL
          
          SELECT CI.DeviceOperatingSystem as DeviceType,
          CI.Model as DeviceModel
          from fn_rbac_mdmclientidentity(@UserSIDs) CI where CI.DeviceOperatingSystem is not null and CI.DeviceType = 1

          ) as temp


          WHERE DeviceType in (@DeviceTypeList)
          GROUP BY
          DeviceType, DeviceModel

        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DeviceType">
          <DataField>DeviceType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DeviceModel">
          <DataField>DeviceModel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Counts">
          <DataField>Counts</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DeviceTypeListDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
          select distinct DeviceType
          from

          (
          SELECT DeviceType as DeviceType
          FROM fn_rbac_EAS_Property_S(@UserSIDs)  where Client_Type0 is NULL

          UNION ALL

          SELECT Platform0 as DeviceType
          FROM fn_rbac_GS_DEVICE_OSINFORMATION(@UserSIDs)  as d INNER JOIN fn_rbac_R_System(@UserSIDs)   as s on d.ResourceID = s.ResourceID
          
          UNION ALL
          
          SELECT CI.DeviceOperatingSystem as DeviceType
          from fn_rbac_mdmclientidentity(@UserSIDs) CI where CI.DeviceOperatingSystem is not null
          ) as temp
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="DeviceType">
          <DataField>DeviceType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSetAdminID">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserTokenSIDs">
            <Value>=Parameters!UserTokenSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select dbo.fn_rbac_GetAdminIDsfromUserSIDs(@UserTokenSIDs) as UserSIDs</CommandText>
      </Query>
      <Fields>
        <Field Name="UserSIDs">
          <DataField>UserSIDs</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
