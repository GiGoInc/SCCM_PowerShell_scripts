<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <rdl:Language xmlns:rdl="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition">=User!Language</rdl:Language>
  <Description>This reports displays all mobile devices clients. Mobile devices that are discovered by the Exchange Server connector are not included.</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>default_reference</DataSourceReference>
      <rd:DataSourceID>48c1b973-a8c2-4797-82c7-ff3e7db0a72c</rd:DataSourceID>
      <rd:SecurityType>None</rd:SecurityType>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet0">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@collectionId">
            <Value>=Parameters!CollectionID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DeviceAccessState">
            <Value>=Parameters!DeviceAccessState.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DeviceAccessStateReason">
            <Value>=Parameters!DeviceAccessStateReason.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AADRegistered">
            <Value>=Parameters!AADRegistered.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ManagementChannel">
            <Value>=Parameters!ManagementChannel.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Compliant">
            <Value>=Parameters!Compliant.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> CREATE TABLE
          #devices
          (
          EasID nvarchar(256), 
		  EasDeviceName nvarchar(256),
          DeviceAccessStateReason nvarchar(64),
          DeviceAccessState nvarchar(32),
          EasDeviceOS nvarchar(64),
		  MDMDeviceName nvarchar(256),
          MDMDeviceOS nvarchar(255),
          MDMDeviceId uniqueidentifier,
          IsManaged nvarchar(32),
          IsCompliant nvarchar(32),
          IsEASActivated nvarchar(32),
          MDMStatus nvarchar(32),
          ManagementChannel int,
		  UserNames nvarchar(max),
		  ComplianceTime nvarchar(max)
          )

          INSERT INTO #devices (EasId, EasDeviceName, DeviceAccessState, DeviceAccessStateReason, EasDeviceOS, MDMDeviceName, MDMDeviceOS,
          MDMDeviceId, IsCompliant, IsManaged, IsEASActivated, MDMStatus, ManagementChannel, UserNames, ComplianceTime)
          SELECT eas.DeviceID,
			eas.FriendlyName,
			eas.DeviceAccessState, 
			eas.DeviceAccessStateReason,
			eas.DeviceOS,
			ci.NetBiosName,
			ci.DeviceOperatingSystem,
			props.DeviceID, 
			CASE 
			    WHEN props.IsCompliant = '0' THEN '2'
				ELSE props.IsCompliant END,
            ISNULL(props.IsManaged,'0'),			
			props.IsEASActivated,
			CASE 
			     WHEN props.MDMStatus IS NULL OR props.MDMStatus = 0 THEN '0'
				 WHEN props.MDMStatus = '1' THEN '1'
				 ELSE '2' END AS MDMStatus,
			CASE 
				 WHEN eas.DeviceID IS NOT NULL AND SMS_Unique_Identifier0 IS NOT NULL THEN 2
				 WHEN eas.DeviceID IS NOT NULL  THEN 1
				 ELSE 0
				 END as ManagementChannel,
			rel.UserNames,
			GraphIsCompliantLastUpdateTime
			FROM fn_rbac_R_System(@UserSIDs) as sd
				LEFT JOIN (
						   SELECT DeviceID, [MDMStatus],[IsEASActivated], [IsManaged], [IsCompliant], [GraphIsCompliantLastUpdateTime]
						   FROM (
							   SELECT 
								   DeviceId, PropertyValue, PropertyName
							   FROM fn_rbac_mdmdeviceproperty(@UserSIDs)
						   ) as s
						   PIVOT
						   (
							   Max(PropertyValue)
							   FOR PropertyName IN ([MDMStatus], [IsEASActivated], [IsManaged], [IsCompliant], [GraphIsCompliantLastUpdateTime])
						   )AS pvt) as props on CAST (props.DeviceID as nvarchar(255))= sd.SMS_Unique_Identifier0
				LEFT JOIN fn_rbac_EAS_Property_S(@UserSIDs) as eas on eas.ItemKey = sd.ResourceID
				LEFT JOIN (SELECT MachineResourceID,
								  STUFF(
										(SELECT DISTINCT ', ' + UniqueUserName
										FROM fn_rbac_usermachinerelation(@UserSIDs) 
										WHERE MachineResourceID = a.MachineResourceID AND RelationActive = 1
										FOR XML PATH (''))
										, 1, 1, '')  AS UserNames
								   FROM fn_rbac_usermachinerelation(@UserSIDs)  AS a
								   GROUP BY MachineResourceID
						   ) as rel on rel.MachineResourceID = sd.ResourceID 
		        LEFT JOIN fn_rbac_mdmclientidentity(@UserSIDs) as ci on CAST (ci.DeviceID as nvarchar(255))= sd.SMS_Unique_Identifier0  
				INNER JOIN fn_rbac_FullCollectionMembership(@UserSIDs) as c on c.ResourceID = sd.ResourceID
			WHERE (eas.DeviceID IS NOT NULL OR (props.DeviceID IS NOT NULL AND ci.DeviceID is not null)) AND sd.ResourceID IS NOT NULL
			AND (@DeviceAccessState = '*' or eas.DeviceAccessState = @DeviceAccessState) AND (@DeviceAccessStateReason = '*' OR
			eas.DeviceAccessStateReason = @DeviceAccessStateReason) 
			AND c.CollectionID = @collectionId 

			SELECT ISNULL(MDMDeviceName, EasDeviceName) as DeviceName,
			       ISNULL(MDMDeviceOS, EASDeviceOS) as DeviceOS,
				   EasId,
                   UserNames as Users,
				   DeviceAccessState,
				   DeviceAccessStateReason,
				   ManagementChannel,
				   IsManaged as AADRegistered,
				   ISNULL(IsEasActivated, '0') as EasActivated,
				   ISNULL(IsCompliant, MDMStatus) as IsCompliant,
ComplianceTime
            FROM #devices
			WHERE (@ManagementChannel = -1 OR @ManagementChannel = ManagementChannel) AND
                              (@AADRegistered = '*' OR IsManaged = @AADRegistered) AND
                              (@Compliant = '*' OR (IsCompliant IS NULL AND MDMStatus = @Compliant) OR IsCompliant = @Compliant)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
