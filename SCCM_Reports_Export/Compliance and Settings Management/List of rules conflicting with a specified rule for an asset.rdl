      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>


          declare @contributing as varchar(max)
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          declare @contributingXML as xml
          set @contributingXML = CONVERT(xml,@contributing)

          declare @countRule as int
          set @countRule = 0
        
          declare @valueRule as int
          set @valueRule = 1

          select
          ConflictCode,
          dbo.fn_GetLocalizedCIName(@lcid,ContributingRule.Rule_CI_ID) as ConfigurationItemName,
          dbo.fn_GetLocalizedProperty(@lcid,ContributingRule.ConfigurationItemDescriptionRID,ContributingRule.Rule_CI_ID) as ConfigurationItemDescription,
          CIVersion,
          dbo.fn_GetLocalizedProperty(@lcid,ContributingRule.RuleNameRID,ContributingRule.Rule_CI_ID) as RuleDisplayName,
          dbo.fn_GetLocalizedProperty(@lcid,ContributingRule.RuleDescriptionRID,ContributingRule.Rule_CI_ID) as RuleDescription,
          case when IsValueRule=1 then @valueRule else @countRule end as RuleType,
          dbo.fn_GetLocalizedProperty(@lcid,ContributingRule.SettingNameRID,ContributingRule.Setting_CI_ID) as SettingDisplayName,
          dbo.fn_GetLocalizedProperty(@lcid,ContributingRule.SettingDescriptionRID,ContributingRule.Setting_CI_ID) as SettingDescription,
          SourceType as SettingType,
          ValidationRule,
          Rule_UniqueID as RuleLogicalName,
          CIType_ID as CIType
          from
          (
          select distinct
          T.X.value('(../../@ConflictCode)[1]', 'nvarchar(255)') as ConflictCode,
              case when convert(nvarchar(255), T.X.value('(./InstanceData/Instance/@RuleType)[1]', 'nvarchar(255)'))='Existential' then 0 else 1 end  as IsValueRule,
              ruleci.CIVersion,
              ruleci.CIType_ID,
              cirules.CI_ID as Rule_CI_ID,
              cirules.Criteria as ValidationRule,
              cirules.RuleNameRID,
              cirules.RuleDescriptionRID,
              cisettings.CI_ID as Setting_CI_ID,
              cisettings.SettingNameRID,
              cisettings.SettingDescriptionRID,
              cirules.Rule_UniqueID,
              cisettings.SourceType,
              cirules.RuleType,
              
              T.X.value('(./CIDescription/@RID)[1]', 'nvarchar(255)') as ConfigurationItemDescriptionRID

              from v_CI_CurrentComplianceStatus  curr 
              inner join v_CICurrentRuleDetail  details on details.CurrentComplianceStatusID = curr.CI_CurrentComplianceStatusID
              cross apply details.ContributingRules.nodes('/ContributingRules/ContributingRule/SettingInformation') as T(X)
              inner join fn_rbac_ConfigurationItems(@UserSIDs)  cis on cis.CI_ID = curr.CI_ID
              inner join v_LocalizedCIProperties  vlcip on vlcip.CI_ID = curr.CI_ID
              inner join v_R_System_Valid  sys on sys.ResourceID=curr.ItemKey
              inner join v_SMSCICurrentComplianceStatus  ciccs on ciccs.CI_CurrentComplianceStatusID = details.CurrentComplianceStatusID and ciccs.CIVersion = cis.CIVersion and ciccs.UserID = curr.UserID
              inner join fn_rbac_SMSConfigurationItems(@UserSIDs)  ruleci on ruleci.ModelName=T.X.value('(../@AuthoringScope)[1]', 'nvarchar(512)') + '/' + T.X.value('(../@LogicalName)[1]', 'nvarchar(512)') and ruleci.CIVersion=T.X.value('(../@Version)[1]', 'nvarchar(512)')
              inner join fn_rbac_SMSConfigurationItems(@UserSIDs)  settingci on settingci.ModelName=T.X.value('(./SettingConfigurationItem/@ModelName)[1]', 'nvarchar(512)') and settingci.CIVersion=T.X.value('(./SettingConfigurationItem/@Version)[1]', 'nvarchar(512)')                     
              inner join v_CIRules  cirules on cirules.Rule_UniqueID=T.X.value('(./../RuleLogicalName)[1]/./text()[1]', 'nvarchar(512)') and cirules.CI_ID=ruleci.CI_ID                      
              inner join v_CISettings  cisettings on cisettings.Setting_UniqueID =  T.X.value('(./SettingLogicalName)[1]/./text()[1]', 'nvarchar(512)') and cisettings.CI_ID=settingci.CI_ID 
              inner join v_CIRules  rules on rules.Rule_ID=details.RuleID and dbo.fn_GetLocalizedProperty(@lcid,rules.RuleNameRID,rules.CI_ID)=@RuleName
              inner join v_CISettings  settings on settings.Setting_ID=details.SettingID and dbo.fn_GetLocalizedProperty(@lcid,settings.SettingNameRID,settings.CI_ID)=@SettingName
          inner join v_Users  usrs on usrs.UserID = ciccs.UserID
          where vlcip.DisplayName = @CIname and
          cis.CIVersion = @CIversion and
              sys.Netbios_Name0 = @computer and
              details.DetailType &lt;&gt; 2 and 
              (usrs.FullName = @UserName or @UserName='') and
              details.DetailType=1
          ) as ContributingRule
        </CommandText>
