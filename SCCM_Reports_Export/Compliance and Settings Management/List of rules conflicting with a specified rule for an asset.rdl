      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          ="
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          if (@filterwildcard = '')
          begin
          select * from
          (
              select '' as RuleName,
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as RuleDisplayName
              UNION ALL
              select distinct dbo.fn_GetLocalizedProperty(@lcid,cir.RuleNameRID,cir.CI_ID) as RuleName,  dbo.fn_GetLocalizedProperty(@lcid,cir.RuleNameRID,cir.CI_ID) as RuleLogicalName from
              v_CIRules  cir
              inner join v_CISettingReferences  cisr on cisr.Rule_ID = cir.Rule_ID
              inner join v_CISettings  cis on cis.Setting_ID    = cisr.Setting_ID
              inner join fn_rbac_SMSConfigurationItems(@UserSIDs)  ruleci on ruleci.CI_ID=cir.CI_ID
              where ((@CIName = '') or ((dbo.fn_GetLocalizedCIName(@lcid,ruleci.CI_ID) = @CIName and (ruleci.CIVersion = @CIversion or  @CIversion=''))))
              and ((@SettingName = '') or ((dbo.fn_GetLocalizedProperty(@lcid,cis.SettingNameRID,cis.CI_ID) = @SettingName)))
          ) temp
          order by RuleDisplayName
          end
          else
          begin
          select * from
          (
              select '' as RuleName,
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as RuleDisplayName
              UNION ALL
              select distinct dbo.fn_GetLocalizedProperty(@lcid,cir.RuleNameRID,cir.CI_ID) as RuleName,  dbo.fn_GetLocalizedProperty(@lcid,cir.RuleNameRID,cir.CI_ID) as RuleLogicalName from
              v_CIRules  cir
              inner join v_CISettingReferences  cisr on cisr.Rule_ID = cir.Rule_ID
              inner join v_CISettings  cis on cis.Setting_ID    = cisr.Setting_ID
              inner join fn_rbac_SMSConfigurationItems(@UserSIDs)  ruleci on ruleci.CI_ID=cir.CI_ID
              where ((@CIName = '') or ((dbo.fn_GetLocalizedCIName(@lcid,ruleci.CI_ID) = @CIName and (ruleci.CIVersion = @CIversion or  @CIversion=''))))
              and ((@SettingName = '') or ((dbo.fn_GetLocalizedProperty(@lcid,cis.SettingNameRID,cis.CI_ID) = @SettingName)))
              and dbo.fn_GetLocalizedProperty(@lcid,cir.RuleNameRID,cir.CI_ID) like @filterwildcard
          ) temp
          order by RuleDisplayName
          end
          "
