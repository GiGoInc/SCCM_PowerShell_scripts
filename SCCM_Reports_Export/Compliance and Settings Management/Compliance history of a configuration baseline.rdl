        </Field>
        <Field Name="LastComplianceMessageTime">
          <DataField>LastComplianceMessageTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CI_UniqueID">
          <DataField>CI_UniqueID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="UserName">
          <DataField>UserName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          set @end=DATEADD(dd,1,@end)

          declare @bls table
          (
          CI_ID int,
          DisplayName nvarchar(512),
          CI_UniqueID nvarchar(512),
          CollectionID nvarchar(100)
          )
          insert into @bls (CI_ID, DisplayName, CI_UniqueID, CollectionID)
          select target.CI_ID, dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID), ci.CI_UniqueID,assign.CollectionID from
          v_AssignmentTargetedCIs  target
          inner join v_CIAssignment  assign on assign.AssignmentID=target.AssignmentID
          inner join fn_rbac_Collection(@UserSIDs) coll on coll.CollectionID = assign.CollectionID
          inner join fn_rbac_ConfigurationItems(@UserSIDs)  ci on ci.CI_ID=target.CI_ID
          where
          (dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) = @name or @name = '') and ci.CIType_ID=2 and
          (assign.CollectionID = @collid or @collid='')

          declare @UserIDs  table (UserID int)
          insert into @UserIDs(UserID) select UserID from v_Users  where FullName = @UserName
          declare @MachineID as int
          set @MachineID = (select ResourceID from v_R_System_Valid  where Netbios_Name0 = @computer)


          set nocount on
          declare @error int
          declare @notapplicable int
          declare @notdetected int
          declare @compliant int
          declare @noncompliant int

          select @error=4
          select @notapplicable=5
          select @notdetected=6
          select @compliant=1
          select @noncompliant=2


          select distinct
          bls.DisplayName as BaselineName,
          hist.SDMPackageVersion as BaselineVersion,
          sys.Netbios_Name0 as MachineName,
          hist.FullName,
          hist.UserName,
          case when hist.IsError=1 then @error else
          case when hist.IsApplicable=0 then @notapplicable else
          case when hist.IsDetected=0 then @notdetected else
          case when hist.IsCompliant=1 then @compliant else @noncompliant end
          end end
          end as ComplianceState,
          hist.MaxNoncomplianceCriticality,
          hist.ComplianceStartDate as LastComplianceMessageTime,
          bls.CI_UniqueID
          from @bls bls
          inner join v_dcmdeploymentresourcesuser  res on res.CollectionID = bls.CollectionID
          inner join fn_rbac_Collection(@UserSIDs) coll on coll.CollectionID = res.CollectionID
          inner join v_Users  users on users.FullName=res.UserName
          inner join v_CIComplianceHistory  hist on hist.UserID=users.UserID and hist.CI_ID = bls.CI_ID
          inner join v_R_System_Valid  sys on sys.ResourceID=hist.ResourceID
          where  (sys.ResourceID = @MachineID or @MachineID ='' or @MachineID is null)
          and hist.ComplianceStartDate&gt;=@start
          and hist.ComplianceStartDate&lt;=@end
          and (hist.UserID  in (select UserID from @UserIDs) or @UserName = '' or @UserName is null)
          
          
          UNION ALL          
          
          select distinct
          bls.DisplayName as BaselineName,
          hist.SDMPackageVersion as BaselineVersion,
          sys.Netbios_Name0 as MachineName,
          hist.FullName,
          hist.UserName,
          case when hist.IsError=1 then @error else
          case when hist.IsApplicable=0 then @notapplicable else
          case when hist.IsDetected=0 then @notdetected else
          case when hist.IsCompliant=1 then @compliant else @noncompliant end
          end end
          end as ComplianceState,
          hist.MaxNoncomplianceCriticality,
          hist.ComplianceStartDate as LastComplianceMessageTime,
          bls.CI_UniqueID
          from @bls bls
          inner join v_ClientCollectionMembers  cm on cm.CollectionID=bls.CollectionID
          inner join fn_rbac_Collection(@UserSIDs) coll on coll.CollectionID = cm.CollectionID
          inner join v_CIComplianceHistory  hist on hist.ResourceID=cm.ResourceID and hist.CI_ID=bls.CI_ID
          inner join v_R_System_Valid  sys on sys.ResourceID=hist.ResourceID
          where  (sys.ResourceID = @MachineID or @MachineID ='' or @MachineID is null)
          and hist.ComplianceStartDate&gt;=@start
          and hist.ComplianceStartDate&lt;=@end
          and (hist.UserID  in (select UserID from @UserIDs) or @UserName = '' or @UserName is null)
          order by hist.ComplianceStartDate desc          
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
