        <CommandText>
          ="
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          if (@filterwildcard = '')
          begin
          select * from
          (
              select distinct
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as BaselineDisplayName, 
              '' as BaselineName, 
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Baseline_UniqueID
              UNION ALL
              select distinct
              dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineDisplayName,dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineName, ci.CI_UniqueID as Baseline_UniqueID
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci
              where ci.CIType_ID=2
          ) temp
          order by BaselineDisplayName
          end
          else
          begin
          select * from
          (
              select distinct
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as BaselineDisplayName,
              '' as BaselineName, 
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Baseline_UniqueID
              UNION ALL
              select distinct
              dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineDisplayName,dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineName, ci.CI_UniqueID as Baseline_UniqueID
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci
              where ci.CIType_ID=2      and dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) like @filterwildcard
          ) temp
          order by BaselineDisplayName
          end
          "
        </CommandText>
