    <DataSet Name="Parameter_DataSet_User">
      <Fields>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="FullDisplayName">
          <DataField>FullDisplayName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          ="
          declare @CI_ID  table (CI_ID int, ModelID int, CIVersion int)
          insert into @CI_ID (CI_ID,ModelID,CIVersion)
          select cis.CI_ID,ModelID, CIVersion from fn_rbac_SMSConfigurationItems(@UserSIDs)  cis
          inner join v_LocalizedCIProperties  cilp on cilp.CI_ID = cis.CI_ID
          where (cilp.DisplayName = @name or @name = '' or @name is NULL)


          if (@filterwildcard = '')
          begin
          select * from
          (
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as FullDisplayName, '' as FullName
              UNION ALL
              select distinct usrs.FullName as FullDisplayName, usrs.FullName
              from fn_rbac_Users(@UserSIDs) usrs
              inner join v_SMSCICurrentComplianceStatus  cics on cics.UserID=usrs.UserID
              inner join @CI_ID cis on cis.ModelID=cics.ModelID and cis.CIVersion=cics.CIVersion
              inner join v_R_System_Valid  sys on sys.ResourceID=cics.ItemKey
              where
              (sys.Netbios_Name0 = @computer or @computer = '' or @computer is NULL)
          ) temp
          order by FullDisplayName
          end
          else
          begin
          select * from
          (
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as FullDisplayName, '' as FullName
              UNION ALL
              select distinct usrs.FullName as FullDisplayName, usrs.FullName
              from fn_rbac_Users(@UserSIDs) usrs
              inner join v_SMSCICurrentComplianceStatus  cics on cics.UserID=usrs.UserID
              inner join @CI_ID cis on cis.ModelID=cics.ModelID and cis.CIVersion=cics.CIVersion
              inner join v_R_System_Valid  sys on sys.ResourceID=cics.ItemKey
              where
              (sys.Netbios_Name0 = @computer or @computer = '' or @computer is NULL)
              and usrs.FullName like @filterwildcard
          ) temp
          order by FullDisplayName
          end
          "
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value>=Parameters!UserFilter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@collid">
            <Value>=Parameters!collid.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@computer">
            <Value>=Parameters!computer.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@name">
            <Value>=Parameters!name.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
