          <Value>=SrsResources.Localization.GetDBString(Fields!UnlocCollectionName.Value, User!Language)</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
            begin
            if (@filterwildcard = '')
            select distinct coll.Name, coll.CollectionID from 
            v_CIAssignment  assign
            join v_CIAssignmentToCI  asstoci on assign.AssignmentID=asstoci.AssignmentID
            join fn_rbac_Collection(@UserSIDs)  coll on coll.CollectionID = assign.CollectionID
            join v_CIRelation_All  rel on rel.CI_ID=asstoci.CI_ID
            join fn_rbac_ConfigurationItems(@UserSIDs) cis on cis.CI_ID = rel.ReferencedCI_ID
            join v_LocalizedCIProperties  loc on loc.CI_ID=rel.ReferencedCI_ID
            where (loc.DisplayName=@BLName OR @BLName='')
            else
            select distinct coll.Name, coll.CollectionID from 
            v_CIAssignment  assign
            join v_CIAssignmentToCI  asstoci on assign.AssignmentID=asstoci.AssignmentID
            join fn_rbac_Collection(@UserSIDs)  coll on coll.CollectionID = assign.CollectionID
            join v_CIRelation_All  rel on rel.CI_ID=asstoci.CI_ID
            join fn_rbac_ConfigurationItems(@UserSIDs) cis on cis.CI_ID = rel.ReferencedCI_ID
            join v_LocalizedCIProperties  loc on loc.CI_ID=rel.ReferencedCI_ID
            where (loc.DisplayName=@BLName OR @BLName='') and coll.CollectionID like @filterwildcard
            ORDER BY Name
            end
      </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@BLName">
            <Value>=Parameters!name.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
