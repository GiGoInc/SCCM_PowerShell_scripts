      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>          
          declare @allassignments table
          (
          AssignmentID int,
          CollectionID  nvarchar(512),
          CollecionName nvarchar(512),
          CI_ID       int,
          DisplayName nvarchar(512),
          CIType_ID   int,
          CI_UniqueID nvarchar(512),
          CIVersion   int,
          Assignment_UniqueID nvarchar(512)         
          )
          
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          
          insert into @allassignments (AssignmentID,CollectionID,CollecionName,CI_ID, DisplayName,CIType_ID,CI_UniqueID,CIVersion,Assignment_UniqueID)
          select assign.AssignmentID,coll.CollectionID as CollectionID,coll.name as CollecionName,bls.CI_ID, dbo.fn_GetLocalizedCIName(@lcid,bls.CI_ID),bls.CIType_ID,bls.CI_UniqueID,bls.CIVersion,assign.Assignment_UniqueID
          from v_CIAssignment  assign
          inner join fn_rbac_Collection(@UserSIDs)  coll on coll.CollectionID=assign.CollectionID
          inner join v_CIAssignmentToCI  targ on targ.AssignmentID=assign.AssignmentID
          inner join fn_rbac_SMSConfigurationItems(@UserSIDs)  bls on bls.CI_ID=targ.CI_ID
          where CIType_ID=2
          and (assign.AssignmentID = @assignmentID or @assignmentID='' or @assignmentID is NULL)
          and (coll.CollectionID=@collid or @collid='')
          and (dbo.fn_GetLocalizedCIName(@lcid,bls.CI_ID) = @name or @name = '')
          
          select
          AllAssignments.AssignmentID,
          AllAssignments.DisplayName as BaselineName,
          ci.SDMPackageVersion as BaselineContentVersion,
          vAl.Value_Int  as SLA,
          summary.NumberSuccess as CountCompliant,
          summary.NumberOther as CountNoncompliant,
          summary.NumberUnknown as CountNotReporting,
          summary.NumberErrors as FailureCount,
          summary.NumberUnknown as CountUnknown,
          (summary.NumberUnknown + summary.NumberSuccess + summary.NumberOther + summary.NumberErrors) as CountTargeted,
          case when (summary.NumberUnknown + summary.NumberSuccess + summary.NumberOther + summary.NumberErrors) &gt; 0
          then convert(numeric(5,2), 100.0*summary.NumberSuccess/(summary.NumberUnknown + summary.NumberSuccess + summary.NumberOther + summary.NumberErrors))
          else 0 end as CompliancePercentage,
          dbo.fn_BuildLocalizedCICategories(ci.CI_ID, @lcid) as Categories,
          @collid as CollectionID,
          col.CollectionName,
          ci.CI_UniqueID as Baseline_UniqueID
          from
          @allassignments AllAssignments
          inner join v_DeploymentSummary  summary on summary.AssignmentID=AllAssignments.AssignmentID
          inner join fn_rbac_ConfigurationItems(@UserSIDs)  ci on ci.ModelId=summary.ModelID
          left join  fn_rbac_AlertVariable_G(@UserSIDs)  vAl  on AllAssignments.Assignment_UniqueID=vAl.TypeInstanceID and vAl.TypeID = 1008
          left join fn_rbac_Collections(@UserSIDs)  col on col.SiteID=@collid
          
          order by
          CountNoncompliant desc,
          BaselineName,
          BaselineContentVersion
             
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@assignmentID">
            <Value>=Parameters!assignmentID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@collid">
            <Value>=Parameters!collid.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@name">
            <Value>=Parameters!name.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
