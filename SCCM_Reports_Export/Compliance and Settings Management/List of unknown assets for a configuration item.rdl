      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          declare @allassignments table
          (
          AssignmentID int,
          CollectionID  nvarchar(512),
          CollecionName nvarchar(512),
          ModelID      int,
          CI_ID        int,
          DisplayName nvarchar(512),
          CIType_ID   int,
          CI_UniqueID nvarchar(512),
          CIVersion   int
          )
          
          declare @AllCIs table
          (
          BL_CI_ID      int,
          CI_ID         int,
          ModelID       int,
          CIVersion     int,
          ModelName     nvarchar(512),
          CI_UniqueID   nvarchar(512),
          CIType_ID     int,
          RelationType  int,
          DisplayName   nvarchar(512)
          )          

          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)

          insert into @allassignments (AssignmentID,CollectionID,CollecionName,CI_ID,ModelID,DisplayName,CIType_ID,CI_UniqueID,CIVersion)
          select assign.AssignmentID,coll.CollectionID as CollectionID,coll.name as CollecionName,bls.CI_ID,bls.ModelId,dbo.fn_GetLocalizedCIName(@lcid,bls.CI_ID),bls.CIType_ID,bls.CI_UniqueID,bls.CIVersion
          from v_CIAssignment  assign
          inner join fn_rbac_Collection(@UserSIDs) coll on coll.CollectionID=assign.CollectionID
          inner join v_CIAssignmentToCI  targ on targ.AssignmentID=assign.AssignmentID
          inner join v_CIRelation_All  rel on rel.CI_ID=targ.CI_ID
          inner join fn_rbac_SMSConfigurationItems(@UserSIDs)  bls on bls.CI_ID=rel.ReferencedCI_ID
          where CIType_ID in (2,50) and bls.IsTombstoned=0
          and assign.AssignmentType in (0,8)
          and (coll.CollectionID=@collid or @collid='')

          Insert into @AllCIs
          select distinct rel.CI_ID, rel.ReferencedCI_ID,ciref.ModelID,ciref.CIVersion,ciref.ModelName,ciref.CI_UniqueID,ciref.CIType_ID,rel.RelationType,dbo.fn_GetLocalizedCIName(@lcid,ciref.CI_ID)
          from
          @allassignments bl
          inner join v_CIRelation_All  rel on rel.CI_ID=bl.CI_ID and rel.RelationType&lt;&gt;5 and rel.RelationType&lt;&gt;50 and rel.RelationType&lt;&gt;8 and rel.RelationType&lt;&gt;80
          inner join fn_rbac_ConfigurationItems(@UserSIDs) ciref on ciref.CI_ID=rel.ReferencedCI_ID
          where
          ((rel.RelationType in (0, 1, 2, 3, 4)) OR (rel.RelationType = 7 and ciref.CIType_ID=50))
          and ciref.CIType_ID&lt;&gt;2 and ciref.CIType_ID &lt;&gt; 11--no baselines or global cis
          and ciref.IsTombstoned=0
          and (dbo.fn_GetLocalizedCIName(@lcid,ciref.CI_ID) = @CIname or @CIname='' or @CIName is null)
          and (ciref.CIVersion = @CIVersion or @CIVersion='' or @CIVersion is null)
          
          create table #machines(ResourceID int, Netbios_Name0 nvarchar(256))
          insert into #machines(ResourceID, Netbios_Name0)
          select distinct cm.ResourceID, sysdisc.Netbios_Name0 from v_ClientCollectionMembers  cm join fn_rbac_Collection(@UserSIDs) coll on coll.CollectionID = cm.CollectionID
          join @allassignments allbl on allbl.CollectionID=cm.CollectionID
		  join @AllCIs cis on cis.BL_CI_ID = allbl.CI_ID
          join v_R_System_Valid  sysdisc on sysdisc.ResourceID = cm.ResourceID
          
          CREATE INDEX MX on #machines (ResourceID) 
          
          create table #users (UserID int, DiscUserID int, UserName nvarchar(256))
          insert into #users(UserID, DiscUserID, UserName)
          select distinct users.UserID,resources.DiscoveredUserID,resources.UserName 
          from v_dcmdeploymentresourcesuser  resources 
          join fn_rbac_Collection(@UserSIDs) coll on coll.CollectionID = resources.CollectionID 
          join @allassignments allbl on allbl.AssignmentID=resources.AssignmentID
		  join @AllCIs cis on cis.BL_CI_ID = allbl.CI_ID
          left join v_Users  users on users.FullName = resources.UserName
          
          CREATE INDEX UX on #users (UserID) 
                    
          select
          tm.Netbios_Name0 as TargetName,
          ClientState as State,
          Case When ClientState=1 or ClientState=4 then NULL else eval.HealthCheckDescription end as Description
          from  (select distinct ModelID,CIVersion from @AllCIs) AllCIs
          cross join #machines tm
          left join v_SMSCICurrentComplianceStatus  cs  on cs.ModelID=AllCIs.ModelID and cs.CIVersion=AllCIs.CIVersion and cs.ItemKey=tm.ResourceID
          inner join v_CH_ClientSummary  chcs on chcs.ResourceID = tm.ResourceID
          left join v_CH_EvalResults  eval on eval.ResourceID=tm.ResourceID
          where ((cs.ComplianceState is null) or (cs.ComplianceState not in (1,2,4)))

          UNION ALL
          select
          resources.UserName as TargetName,
          NULL as State,
          NULL as Description
          from  (select distinct ModelID,CIVersion from @AllCIs) AllCIs
          cross join #users resources
          left join v_SMSCICurrentComplianceStatus  cs  on cs.ModelID=AllCIs.ModelID and cs.CIVersion=AllCIs.CIVersion and cs.UserID=resources.UserID
          where ((cs.ComplianceState is null) or (cs.ComplianceState not in (1,2,4)))

          drop table #machines
          drop table #users
        </CommandText>
