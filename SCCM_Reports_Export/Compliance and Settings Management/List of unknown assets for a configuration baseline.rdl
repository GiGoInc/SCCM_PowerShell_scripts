    <DataSet Name="Parameter_DataSet1">
      <Fields>
        <Field Name="BaselineName">
          <DataField>BaselineName</DataField>
        </Field>
        <Field Name="Baseline_UniqueID">
          <DataField>Baseline_UniqueID</DataField>
        </Field>
        <Field Name="BaselineDisplayName">
          <DataField>BaselineDisplayName</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          ="
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          if (@filterwildcard = '')
          begin
          select * from
          (
              select distinct
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as BaselineDisplayName, 
              '' as BaselineName, 
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Baseline_UniqueID
              UNION ALL
              select distinct
              dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineDisplayName,dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineName,  CI_UniqueID  as Baseline_UniqueID
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci
              where ci.CIType_ID=2
          ) temp
          order by BaselineDisplayName
          end
          else
          begin
          select * from
          (
              select distinct
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as BaselineDisplayName,
              '' as BaselineName, 
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Baseline_UniqueID
              UNION ALL
              select distinct
              dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineDisplayName,dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineName,  CI_UniqueID as Baseline_UniqueID
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci
              where ci.CIType_ID=2      and dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) like @filterwildcard
          ) temp
          order by BaselineDisplayName
          end
          "
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_CollectionDataSet">
      <Fields>
        <Field Name="UnlocCollectionName">
          <DataField>Name</DataField>
        </Field>
        <Field Name="Name">
          <Value>=SrsResources.Localization.GetDBString(Fields!UnlocCollectionName.Value, User!Language)</Value>
        </Field>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>              
        ="
              begin
              if (@filterwildcard = '')
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Name, '' as CollectionID
              UNION ALL 
              SELECT DISTINCT  Name, CollectionID FROM fn_rbac_Collection(@UserSIDs)  ORDER BY Name
              else
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Name, '' as CollectionID
              UNION ALL   
              SELECT DISTINCT  Name, CollectionID FROM fn_rbac_Collection(@UserSIDs) 
              WHERE CollectionID like @filterwildcard
              ORDER BY Name
              end
        "
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@filterwildcard">
            <Value />
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="AssignmentID_Dataset">
      <Fields>
        <Field Name="AssignmentID">
          <DataField>AssignmentID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
        select '' as AssignmentID
        UNION ALL
        select CONVERT(NVARCHAR(MAX),assign.AssignmentID) from 
        (
            select distinct assign.AssignmentID from 
            v_CIAssignment  assign
            join fn_rbac_Collection(@UserSIDs)  coll on coll.CollectionID = assign.CollectionID
            join v_CIAssignmentToCI  asstoci on assign.AssignmentID=asstoci.AssignmentID
            join v_CIRelation_All  rel on rel.CI_ID=asstoci.CI_ID
            join fn_rbac_ConfigurationItems(@UserSIDs) cis on cis.CI_ID = rel.ReferencedCI_ID
            join v_LocalizedCIProperties  loc on loc.CI_ID=rel.ReferencedCI_ID
            where (loc.DisplayName=@BLName OR @BLName='')
        ) assign
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BLName">
            <Value>=Parameters!BLname.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
