          <DataField>FailureCount</DataField>
        </Field>
        <Field Name="ActivatedCount">
          <DataField>ActivatedCount</DataField>
        </Field>
        <Field Name="EnforcedCount">
          <DataField>EnforcedCount</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Categories">
          <DataField>Categories</DataField>
        </Field>
        <Field Name="Baseline_UniqueID">
          <DataField>Baseline_UniqueID</DataField>
        </Field>
        <Field Name="LastSummaryTime">
          <DataField>LastSummaryTime</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>             
              declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)

              select
              dbo.fn_GetLocalizedCIName(@lcid,summ.CI_ID) as BaselineName,
              ci.SDMPackageVersion as BaselineContentVersion,
              summ.Severity as MaxNoncomplianceCriticality,
              summ.CountCompliant,
              summ.CountNoncompliant,
              summ.CountTargeted - summ.CountCompliant - summ.CountNoncompliant - summ.FailureCount as CountNotReporting,
              summ.FailureCount,
              summ.CountTargeted,
              summ.ActivatedCount,
              Summ.EnforcedCount,
              case when summ.CountTargeted&gt;0 then convert(numeric(5,2), 100.0*summ.CountCompliant/summ.CountTargeted) else 0 end as CompliancePercentage,
              dbo.fn_BuildLocalizedCICategories(ci.CI_ID, @lcid) as Categories,
              Summ.LastSummaryTime,
              ci.CI_UniqueID as Baseline_UniqueID
              from (select distinct CI_ID from v_AssignmentTargetedCIs target inner join v_CIAssignment assign on assign.AssignmentID = target.AssignmentID inner join fn_rbac_Collection(@UserSIDs) coll on  coll.CollectionID = assign.CollectionID) cit
              inner join v_CIComplianceSummary  summ on summ.CI_ID=cit.CI_ID
              inner join fn_rbac_ConfigurationItems(@UserSIDs)  ci on summ.CI_ID=ci.CI_ID
              left join (select distinct CI_ID from
              fn_CICategoryInfo(@lcid)  cat
              where cat.CategoryInstanceName = @category or @category=''
              ) temp
              on temp.CI_ID=ci.CI_ID
              where CIType_ID=2
              and (dbo.fn_GetLocalizedCIName(@lcid,summ.CI_ID) = @name or @name='')
              and (summ.Severity &gt;= @severity or @severity='')
              and (temp.CI_ID is not null or @category='')
              order by summ.CountCompliant, summ.Severity, BaselineName, BaselineContentVersion
              
           </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
          <QueryParameter Name="name">
            <Value>=Parameters!name.Value</Value>
          </QueryParameter>
          <QueryParameter Name="category">
            <Value>=Parameters!category.Value</Value>
          </QueryParameter>
          <QueryParameter Name="severity">
            <Value>=Parameters!severity.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_DataSet1">
      <Fields>
        <Field Name="BaselineName">
          <DataField>BaselineName</DataField>
        </Field>
        <Field Name="Baseline_UniqueID">
          <DataField>Baseline_UniqueID</DataField>
        </Field>
        <Field Name="BaselineDisplayName">
          <DataField>BaselineDisplayName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          ="
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          if (@filterwildcard = '')
          begin
          select * from
          (
              select distinct
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as BaselineDisplayName, 
              '' as BaselineName, 
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Baseline_UniqueID
              UNION ALL
              select distinct
              dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineDisplayName,dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineName,  CI_UniqueID  as Baseline_UniqueID
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci
              where ci.CIType_ID=2
          ) temp
          order by BaselineDisplayName
          end
          else
          begin
          select * from
          (
              select distinct
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as BaselineDisplayName,
              '' as BaselineName, 
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Baseline_UniqueID
              UNION ALL
              select distinct
              dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineDisplayName,dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineName,  CI_UniqueID as Baseline_UniqueID
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci
              where ci.CIType_ID=2      and dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) like @filterwildcard
          ) temp
          order by BaselineDisplayName
          end
          "
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
