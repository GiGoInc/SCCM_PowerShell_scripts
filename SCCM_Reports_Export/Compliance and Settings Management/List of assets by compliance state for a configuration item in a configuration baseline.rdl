    <DataSet Name="Parameter_CollectionDataSet">
      <Fields>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="UnlocCollectionName">
          <DataField>Name</DataField>
        </Field>
        <Field Name="Name">
          <Value>=SrsResources.Localization.GetDBString(Fields!UnlocCollectionName.Value, User!Language)</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>   
          ="
              begin     
              if (@filterwildcard = '')
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Name, '' as CollectionID
              UNION ALL 
              SELECT DISTINCT  Name, CollectionID FROM fn_rbac_Collection(@UserSIDs)  ORDER BY Name
              else
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Name, '' as CollectionID
              UNION ALL   
              SELECT DISTINCT  Name, CollectionID FROM fn_rbac_Collection(@UserSIDs) 
              WHERE CollectionID like @filterwildcard
              ORDER BY Name
              end
          "
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="AssignmentID_Dataset">
      <Fields>
        <Field Name="AssignmentID">
          <DataField>AssignmentID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
        select '' as AssignmentID
        UNION ALL
        select CONVERT(NVARCHAR(MAX),assign.AssignmentID) from 
        (
          select distinct assign.AssignmentID from 
          v_CIAssignment  assign
          join fn_rbac_Collection(@UserSIDs)  coll on coll.CollectionID = assign.CollectionID
          join v_CIAssignmentToCI  asstoci on assign.AssignmentID=asstoci.AssignmentID
          join v_CIRelation_All  rel on rel.CI_ID=asstoci.CI_ID
          join fn_rbac_ConfigurationItems(@UserSIDs) cis on cis.CI_ID = rel.ReferencedCI_ID
          join v_LocalizedCIProperties  loc on loc.CI_ID=rel.ReferencedCI_ID
          where (loc.DisplayName=@BLName OR @BLName='')
         ) assign
    </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BLName">
            <Value>=Parameters!BLName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="TargetName_Dataset">
      <Fields>
        <Field Name="TargetName">
          <DataField>TargetName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
        select '' as TargetName
        UNION ALL
        select distinct UserName as TargetName from v_dcmdeploymentresourcesuser cm join v_Collection coll on coll.CollectionID =  cm.CollectionID where cm.CollectionID='SMS00002' 
        UNION ALL
        select distinct cm.Name as TargetName from v_ClientCollectionMembers cm join v_Collection coll on coll.CollectionID =  cm.CollectionID where cm.CollectionID='SMS00001'        
    </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
