        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          ="
          select * from
          (
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as CIDisplayVersion, '' as CIVersion
              UNION ALL
              select distinct CONVERT(varchar(max),CIVersion) as CIDisplayVersion, CONVERT(varchar(max),CIVersion) as CIVersion from fn_rbac_ConfigurationItems(@UserSIDs)  cis 
              join v_LocalizedCIProperties  cilp on cilp.CI_ID = cis.CI_ID
              where (cilp.DisplayName = @CIName or @CIName ='' or @CIName is NULL)
              and cis.CIType_ID&lt;&gt;2
          ) temp
          order by CAST(CIVersion as int)
          "
