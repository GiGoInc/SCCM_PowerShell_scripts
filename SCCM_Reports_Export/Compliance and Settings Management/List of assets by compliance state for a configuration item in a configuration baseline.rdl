    <DataSet Name="Parameter_DataSet_CI_DisplayName">
      <Fields>
        <Field Name="CIDisplayName">
          <DataField>CIDisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CIName">
          <DataField>CIName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          ="
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          if (@filterwildcard = '')
          begin
          select * from
          (
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as CIDisplayName, '' as CIName
              UNION ALL
              select distinct dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIDisplayName, dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIName
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci
              inner join v_CIRelation_All  rel on rel.ReferencedCI_ID=ci.CI_ID and rel.RelationType &lt;&gt; 7 and rel.RelationType&lt;&gt;5 and rel.RelationType&lt;&gt;50 and rel.RelationType&lt;&gt;8 and rel.RelationType&lt;&gt;80
              inner join fn_rbac_ConfigurationItems(@UserSIDs)  bl on bl.CI_ID=rel.CI_ID and bl.CIType_ID=2
              where ci.CIType_ID not in (2,11,14) and (dbo.fn_GetLocalizedCIName(@lcid,bl.CI_ID) = @BLname or @BLname = '')
              UNION ALL
              select distinct dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIDisplayName, dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIName
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci where ci.CIType_ID = 50 and @BLname = ''                 
          ) temp
          order by CIDisplayName
          end
          else
          begin
          select * from
          (
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as CIDisplayName, '' as CIName
              UNION ALL
              select distinct dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIDisplayName, dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIName
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci
              inner join v_CIRelation_All  rel on rel.ReferencedCI_ID=ci.CI_ID and rel.RelationType &lt;&gt; 7 and rel.RelationType&lt;&gt;5 and rel.RelationType&lt;&gt;50 and rel.RelationType&lt;&gt;8 and rel.RelationType&lt;&gt;80
              inner join fn_rbac_ConfigurationItems(@UserSIDs)  bl on bl.CI_ID=rel.CI_ID and bl.CIType_ID=2
              where ci.CIType_ID not in (2,11,14) and (dbo.fn_GetLocalizedCIName(@lcid,bl.CI_ID) = @BLname or @BLname = '') and dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) like @filterwildcard
              UNION ALL
              select distinct dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIDisplayName, dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIName
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci where ci.CIType_ID = 50 and @BLname = ''                 
          ) temp
          order by CIDisplayName
          end
          "
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@filterwildcard">
            <Value />
          </QueryParameter>
          <QueryParameter Name="@BLname">
            <Value>=Parameters!BLName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
