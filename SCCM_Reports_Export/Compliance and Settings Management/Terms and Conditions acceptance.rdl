    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserName">
            <Value>=Parameters!UserName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TermsAndConditions">
            <Value>=Parameters!TermsAndConditions.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllValueParameter">
            <Value>=Parameters!AllValueParameter.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Yes">
            <Value>=Parameters!Yes.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@No">
            <Value>=Parameters!No.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
		SELECT lcisl.DisplayName AS Name, ud.Name0 AS UserName, NULLIF(MAX(uta.TermVersion), 0) AS AcceptedVersion, uta.AcceptanceTime AS DateAccepted, 
			CASE CONVERT(int,RIGHT(cir.Criteria, LEN(cir.Criteria)-LEN('Equals ')))
				WHEN uta.TermVersion THEN @Yes
				ELSE @No
			END AS AcceptedLatest
		FROM dbo.fn_rbac_MDMUserPolicyAssignment(@UserSIDs) upa 
			INNER JOIN dbo.v_LocalizedCIProperties_SiteLoc lcisl ON lcisl.CI_ID = upa.CI_ID
			INNER JOIN dbo.v_CIRules cir ON cir.CI_ID = upa.CI_ID AND cir.Rule_UniqueID = 'TermsAndConditionsSettings_Version'
			INNER JOIN dbo.fn_rbac_R_User(@UserSIDs) ud ON upa.UserID = ud.CloudUserID
			LEFT OUTER JOIN dbo.v_MDMUserCompanyTermAcceptance uta ON upa.UserID = uta.UserID AND CONVERT(UNIQUEIDENTIFIER,upa.PolicyID) = CONVERT(UNIQUEIDENTIFIER,uta.TermID)
		WHERE (upa.PolicyType = 15
			AND (ud.Name0 = @UserName OR @UserName = @AllValueParameter)
			AND (lcisl.DisplayName = @TermsAndConditions OR @TermsAndConditions = @AllValueParameter)
			)
		GROUP BY lcisl.DisplayName, ud.Name0, uta.TermVersion, uta.AcceptanceTime, cir.Criteria</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UserName">
          <DataField>UserName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AcceptedVersion">
          <DataField>AcceptedVersion</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DateAccepted">
          <DataField>DateAccepted</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="AcceptedLatest">
          <DataField>AcceptedLatest</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@filterwildcard_User">
            <Value>=Parameters!UserFilter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllValueParameter">
            <Value>=Parameters!AllValueParameter.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
		begin
		 if (@filterwildcard_User = '') 
			SELECT @AllValueParameter AS "User Name"
			UNION ALL 
			SELECT distinct ud.Name0 as "User Name" FROM dbo.fn_rbac_MDMUserPolicyAssignment(@UserSIDs) upa
			INNER JOIN dbo.fn_rbac_R_User(@UserSIDs) ud ON upa.UserID = ud.CloudUserID
		else 
			SELECT @AllValueParameter AS "User Name"
			UNION ALL
			SELECT distinct ud.Name0 as "User Name" FROM dbo.fn_rbac_MDMUserPolicyAssignment(@UserSIDs) upa     
			INNER JOIN dbo.fn_rbac_R_User(@UserSIDs) ud ON upa.UserID = ud.CloudUserID
			where ud.Name0 like '%' + @filterwildcard_User + '%' 
		end
        </CommandText>
      </Query>
      <Fields>
        <Field Name="User_Name">
          <DataField>User Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet3">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllValueParameter">
            <Value>=Parameters!AllValueParameter.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
		SELECT @AllValueParameter AS "Terms And Conditions"
		UNION ALL 
		SELECT DISTINCT lcisl.DisplayName AS "Terms And Conditions"
		FROM dbo.fn_rbac_MDMUserPolicyAssignment(@UserSIDs) upa 
			INNER JOIN dbo.v_LocalizedCIProperties_SiteLoc lcisl ON lcisl.CI_ID = upa.CI_ID
		WHERE (upa.PolicyType = 15)
        </CommandText>
      </Query>
      <Fields>
        <Field Name="Terms_And_Conditions">
          <DataField>Terms And Conditions</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
