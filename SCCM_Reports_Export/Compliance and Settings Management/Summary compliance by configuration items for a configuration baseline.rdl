        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
        ="
              declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
              select distinct N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as CategoryInstanceDisplayName, '' as CategoryInstanceName
          UNION ALL
          select distinct cat.CategoryInstanceName as CategoryInstanceDisplayName, cat.CategoryInstanceName
          from fn_ListCategoryInstances(@lcid) cat
          where CategoryTypeName='UserCategories' and (@filterwildcard='' or cat.CategoryInstanceName like @filterwildcard)
          "
        </CommandText>
