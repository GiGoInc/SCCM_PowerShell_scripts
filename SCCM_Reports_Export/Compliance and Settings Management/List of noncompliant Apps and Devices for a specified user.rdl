          </QueryParameter>
        </QueryParameters>
        <CommandText>CREATE TABLE 
        #machines
        
(
	MachineID int, 
	DeviceName  nvarchar(256),
	AgentEdition int,			
	FullUserName nvarchar(256),			
	UPN nvarchar(256)
)
		

INSERT INTO #machines(MachineID, DeviceName, AgentEdition, FullUserName, UPN)

SELECT DISTINCT 	s.ResourceID, s.Name0,
	case s.AgentEdition0 when 8 then 9 else s.AgentEdition0 end ,
        	u.Full_User_Name0, u.User_Principal_Name0    
             from v_Collections co
 INNER JOIN v_FullCollectionMembership  fcm on co.SiteID = fcm.CollectionID
 
INNER JOIN fn_rbac_R_System(@UserSIDs) s on s.ResourceID = fcm.ResourceID

INNER JOIN v_usermachinerelation um on um.MachineResourceID = s.ResourceID

INNER JOIN fn_rbac_R_User(@UserSIDs) u on um.UniqueUserName = u.Unique_User_Name0  
where co.SiteID = @CollectionID and s.AgentEdition0 in (8,9,11,14,17)
and u.Unique_User_Name0 = @UserName


select distinct m.FullUserName, m.UPN, m.DeviceName as DeviceName, ISNULL(am.Name, ad.ApplicationId) as ApplicationName, CL.DisplayName as PolicyName 
 from fn_rbac_AllowOrDenyListViolationData(@UserSIDs) ad
  INNER JOIN  #machines m ON  m.MachineID = ad.MachineID
  INNER JOIN fn_LocalizedCIProperties(1033) CL on CL.CI_ID = ad.CI_ID
LEFT OUTER JOIN [dbo].[v_ApplicationIdToNameMap]am on ad.ApplicationId = am.ApplicationId and ad.CI_ID = am.CI_ID and m.AgentEdition = am.AgentEdition
   where ad.violationType in (@AppListType) and m.AgentEdition in (@OperatingSystem)  and CL.LocaleID = 1033
  
   

   drop table #machines</CommandText>
