      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          set nocount on

          declare @notapplicable int
          declare @notdetected int
          declare @unknown int
          declare @compliant int
          declare @noncompliant int
          declare @conflict int
          declare @error int
          declare @IsApplicable as bit
          declare @IsDetected as bit
          declare @ResolvedState as int


          select @unknown=0
          select @compliant=1
          select @noncompliant=2
          select @conflict=3
          select @error=4
          select @notapplicable=5
          select @notdetected=6

          set @ResolvedState=@ActualState
          if(@unknown = @ActualState)      begin set @ResolvedState=@ActualState set @IsDetected=NULL set @IsApplicable=NULL  end
          if(@compliant = @ActualState)    begin set @ResolvedState=@ActualState set @IsDetected=1    set @IsApplicable=1     end
          if(@noncompliant = @ActualState) begin set @ResolvedState=@ActualState set @IsDetected=1    set @IsApplicable=1     end
          if(@conflict = @ActualState)     begin set @ResolvedState=@ActualState set @IsDetected=1    set @IsApplicable=1     end
          if(@error = @ActualState)        begin set @ResolvedState=@ActualState set @IsDetected=NULL set @IsApplicable=NULL  end
          if(@notapplicable = @ActualState)begin set @ResolvedState=NULL         set @IsDetected=NULL set @IsApplicable=0     end
          if(@notdetected = @ActualState)  begin set @ResolvedState=NULL         set @IsDetected=0    set @IsApplicable=NULL  end

          declare @MachineID as int

          declare @UserIDs  table (UserID int)
          insert into @UserIDs(UserID) select UserID from v_Users  where FullName = @UserName

          if(@computer is NULL OR @computer= '')     set @MachineID = ''
          else                                       set @MachineID = (select ResourceID from v_R_System_Valid  where Netbios_Name0 = @computer)


          declare @allassignments table
          (
          AssignmentID int,
          CollectionID  nvarchar(512),
          CollecionName nvarchar(512),
          CI_ID         int,
          DisplayName nvarchar(512),
          CIType_ID   int,
          CI_UniqueID nvarchar(512),
          CIVersion     int,
          Level         int,
          deepsortorder nvarchar(1024)
          )

          declare @AllCIs table
          (
          BL_CI_ID      int,
          CI_ID         int,
          ModelID       int,
          CIVersion     int,
          CI_UniqueID   nvarchar(512),
          CIType_ID     int,
          RelationType  int,
          BLDisplayName nvarchar(512),
          DisplayName   nvarchar(512)
          )

          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)


          --recursively select all sub-baselines along with the sort order
          ;WITH AllBaselines(AssignmentID,CollectionID,CollecionName,CI_ID, CIVersion, CI_UniqueID, Level, deepsortorder, DisplayName)
          AS
          (
          select AssignmentID=assign.AssignmentID,CollectionID=coll.CollectionID,CollecionName=coll.Name,CI_ID=ci.CI_ID,ci.CIVersion,CI_UniqueID, Level=0, convert(varchar(1024),(ci.CI_ID)), dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID)
          from
          v_CIAssignment  assign
          inner join fn_rbac_Collection(@UserSIDs)  coll on coll.CollectionID=assign.CollectionID
          inner join v_CIAssignmentToCI  targ on targ.AssignmentID=assign.AssignmentID

          inner join v_CIRelation_All  rel on rel.CI_ID=targ.CI_ID
          inner join fn_rbac_ConfigurationItems(@UserSIDs)  ci on ci.CI_ID=rel.ReferencedCI_ID
          where CIType_ID in (2,50)
          and (dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) = @BLname  or @BLname = '')
          and (coll.CollectionID=@collid or @collid='' or @collid is NULL)
          and(assign.AssignmentID=@assignmentID or @assignmentID='' or @assignmentID is NULL)
          and assign.AssignmentType in (0,8)
          union all
          select bci.AssignmentID,bci.CollectionID,bci.CollecionName,rci.ToCIID,ci.CIVersion, ci.CI_UniqueID, Level=Level+1, convert(varchar(1024), deepsortorder+'-'+convert(varchar(32),rci.ToCIID)), dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID)
          from AllBaselines bci
          inner join v_CIRelation  rci on rci.FromCIID=bci.CI_ID and rci.RelationType=1
          inner join fn_rbac_ConfigurationItems(@UserSIDs)  ci on ci.CI_ID=rci.ToCIID
          where ci.CIType_ID in (2,50)
          )
          Insert into @allassignments (AssignmentID,CollectionID,CollecionName,CI_ID,CIVersion,CI_UniqueID, Level, deepsortorder, DisplayName)
          select AllBaselines.AssignmentID ,CollectionID,CollecionName,CI_ID,CIVersion,CI_UniqueID, Level, deepsortorder, DisplayName from AllBaselines
          group by AllBaselines.AssignmentID,CollectionID,CollecionName,CI_ID,CIVersion,CI_UniqueID, Level, deepsortorder, DisplayName


          --select all CIs along with the sort order
          Insert into @AllCIs
          select distinct rel.CI_ID,rel.ReferencedCI_ID,ModelId,ciref.CIVersion,ciref.CI_UniqueID,ciref.CIType_ID,rel.RelationType,dbo.fn_GetLocalizedCIName(@lcid,bl.CI_ID) as BLDisplayName,dbo.fn_GetLocalizedCIName(@lcid,ciref.CI_ID)
          from
          @allassignments bl
          inner join  v_CIRelation_All  rel on rel.CI_ID=bl.CI_ID
          inner join fn_rbac_ConfigurationItems(@UserSIDs)  ciref on ciref.CI_ID=rel.ReferencedCI_ID
          where
          ((rel.RelationType in (0, 1, 2, 3, 4)) OR (rel.RelationType = 7 and ciref.CIType_ID=50))
          and ciref.IsTombstoned=0
          and ciref.CIType_ID not in (11,14)
          and (dbo.fn_GetLocalizedCIName(@lcid,ciref.CI_ID) = @CIname  or @CIname = '')
          and (ciref.CIVersion = @CIVersion  or @CIVersion = '')


          DECLARE @Assignments Assignment_Type INSERT INTO @Assignments select AssignmentID from @allassignments


          select
          @assignmentID as AssignmentID,
          sys.Netbios_Name0 as MachineName,
          case when TargetCompliance.CIType_ID =50 then NULL  else REPLICATE('-',3*(TargetCompliance.Level)) +  case when (TargetCompliance.Level)&gt;0 then '&gt; ' else ''  end + case when TargetCompliance.CIType_ID=2 then TargetCompliance.DisplayName else TargetCompliance.BLDisplayName end  end as SubBaselineDisplayName,
          case when TargetCompliance.CIType_ID =2 then NULL else TargetCompliance.DisplayName end as ConfigurationItemName,
          case when (TargetCompliance.RelationType=1 AND  TargetCompliance.CIType_ID=3) then 2 else TargetCompliance.RelationType end as Policy,
          case when TargetCompliance.CIType_ID =50 then NULL else case when TargetCompliance.CIType_ID&lt;&gt;2 then TargetCompliance.BLVersion else TargetCompliance.CIVersion end end as SubBaselineContentVersion,
          case when TargetCompliance.CIType_ID = 50 then NULL else TargetCompliance.BLVersion end as ParentBaselineVersion,
          TargetCompliance.deepsortorder +  '-' + convert(varchar(32),TargetCompliance.CI_ID) as FinalOrder,
          case when TargetCompliance.ComplianceState=4 then  TargetCompliance.ComplianceState
          when TargetCompliance.IsApplicable=0 then @notapplicable
          when TargetCompliance.IsDetected=0   then @notdetected
          else TargetCompliance.ComplianceState
          end as ComplianceState,
          TargetCompliance.FullName,
          case when TargetCompliance.CIType_ID=2 then NULL else TargetCompliance.CIVersion end as CIContentVersion,
          case when TargetCompliance.CIType_ID=2 then    2 else TargetCompliance.CIType_ID end as ConfigurationItemType,
          case when TargetCompliance.CIType_ID=2 then NULL else TargetCompliance.ComplianceValidationRuleFailures end as ComplianceValidationRuleFailures,
          case when TargetCompliance.CIType_ID=2 then NULL else TargetCompliance.ReificationCount end as EnforcementCount,
          case when TargetCompliance.CIType_ID=2 then NULL else TargetCompliance.CompliantRulesCount end as CompliantRulesCount,
          case when TargetCompliance.CIType_ID=2 then NULL else TargetCompliance.ErrorCount end as ErrorCount,
          case when TargetCompliance.CIType_ID=2 then NULL else TargetCompliance.conflictCount end as conflictCount,
          TargetCompliance.MaxNoncomplianceCriticality,
          dbo.fn_GetLastAssignmentEvaluationTime(@Assignments,TargetCompliance.ItemKey,TargetCompliance.UserID) as LastComplianceMessageTime,
          TargetCompliance.CI_UniqueID,
          case when TargetCompliance.CIType_ID=50 then '' else TargetCompliance.BLDisplayName end as ParentBaselineName
          from
          (

          select AllCIs.DisplayName,allbl.CIVersion as BLVersion,allbl.CI_UniqueID as BL_UniqueID,cs.ComplianceState,cs.ReificationCount,cs.IsApplicable,cs.IsDetected,cs.MaxNoncomplianceCriticality,cs.ItemKey,cs.UserID,users.FullName,cs.IsEnforced,users.UserName,
          allbl.Level,AllCIs.CI_ID,AllCIs.CI_UniqueID,AllCIs.CIType_ID,AllCIs.CIVersion,AllCIs.RelationType,allbl.DisplayName as BLDisplayName,allbl.deepsortorder,
          cs.ConflictCount,cs.ComplianceValidationRuleFailures,cs.CompliantRulesCount,cs.ErrorCount
          from (select distinct CIVersion, CI_ID, CI_UniqueID, DisplayName, Level, deepsortorder from @allassignments) allbl
          inner join @AllCIs AllCIs on allbl.CI_ID=AllCIs.BL_CI_ID
          cross join (select distinct cm.ResourceID from v_ClientCollectionMembers  cm join fn_rbac_Collection(@UserSIDs) coll on coll.CollectionID = cm.CollectionID join @allassignments allbl on allbl.CollectionID=cm.CollectionID and cm.ResourceID=@MachineID) cm
          inner join v_SMSCICurrentComplianceStatus  cs  on cs.ModelID=AllCIs.ModelID and cs.CIVersion=AllCIs.CIVersion and cs.ItemKey=cm.ResourceID and cs.ItemKey = @MachineID and cs.UserID in (select UserID from @UserIDs)
          inner join v_Users  users on users.UserID=cs.UserID and users.UserID in (select UserID from @UserIDs)
          UNION --User assignment
          select AllCIs.DisplayName,allbl.CIVersion as BLVersion,allbl.CI_UniqueID as BL_UniqueID,cs.ComplianceState,cs.ReificationCount,cs.IsApplicable,cs.IsDetected,cs.MaxNoncomplianceCriticality,cs.ItemKey,users.UserID,users.FullName,cs.IsEnforced,users.UserName,
          allbl.Level,AllCIs.CI_ID,AllCIs.CI_UniqueID,AllCIs.CIType_ID,AllCIs.CIVersion,AllCIs.RelationType,AllCIs.BLDisplayName as BLDisplayName,allbl.deepsortorder,
          cs.ConflictCount,cs.ComplianceValidationRuleFailures,cs.CompliantRulesCount,cs.ErrorCount
          from (select distinct CIVersion, CI_ID, CI_UniqueID, DisplayName, Level, deepsortorder from @allassignments) allbl
          inner join @AllCIs AllCIs on allbl.CI_ID=AllCIs.BL_CI_ID
          cross join (select distinct resources.UserName from v_dcmdeploymentresourcesuser  resources join fn_rbac_Collection(@UserSIDs) coll on coll.CollectionID = resources.CollectionID join @allassignments allbl on allbl.AssignmentID=resources.AssignmentID and resources.UserName=@UserName) resources
          inner join v_Users  users on users.FullName=resources.UserName and users.UserID in (select UserID from @UserIDs)
          inner join v_SMSCICurrentComplianceStatus  cs  on cs.ModelID=AllCIs.ModelID and cs.CIVersion=AllCIs.CIVersion and cs.UserID=users.UserID  and cs.UserID  in (select UserID from @UserIDs) and cs.ItemKey=@MachineID
          ) TargetCompliance
          inner join v_R_System_Valid  sys on sys.ResourceID=TargetCompliance.ItemKey


          where (TargetCompliance.ReificationCount &gt; 0 or @IsEnforced = '0' or @IsEnforced is null or @IsEnforced = '')
          and (TargetCompliance.MaxNoncomplianceCriticality = @Severity or @Severity='' or @Severity is null)
          and (TargetCompliance.ComplianceState = @ResolvedState or @ResolvedState is null or @ResolvedState = '')
          and (TargetCompliance.IsApplicable = @IsApplicable or @IsApplicable is NULL)
          and (TargetCompliance.IsDetected = @IsDetected   or @IsDetected is NULL)
   
          order by  
          MachineName,
          FullName,
        FinalOrder</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
