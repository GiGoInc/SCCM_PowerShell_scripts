    <DataSet Name="Parameter_DataSet_DisplayName">
      <Fields>
        <Field Name="BaselineName">
          <DataField>BaselineName</DataField>
        </Field>
        <Field Name="Baseline_UniqueID">
          <DataField>Baseline_UniqueID</DataField>
        </Field>
        <Field Name="BaselineDisplayName">
          <DataField>BaselineDisplayName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          ="
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          if (@filterwildcard = '')
          begin
          select * from
          (
              select distinct
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as BaselineDisplayName, 
              '' as BaselineName, 
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Baseline_UniqueID
              UNION ALL
              select distinct
              dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineDisplayName,dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineName,  CI_UniqueID  as Baseline_UniqueID
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci
              where ci.CIType_ID=2
          ) temp
          order by BaselineDisplayName
          end
          else
          begin
          select * from
          (
              select distinct
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as BaselineDisplayName,
              '' as BaselineName, 
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Baseline_UniqueID
              UNION ALL
              select distinct
              dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineDisplayName,dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineName,  CI_UniqueID as Baseline_UniqueID
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci
              where ci.CIType_ID=2      and dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) like @filterwildcard
          ) temp
          order by BaselineDisplayName
          end
          "
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
