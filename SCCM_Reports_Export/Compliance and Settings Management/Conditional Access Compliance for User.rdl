      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          -- The first two variables are for the report: 1st on is defined by the user and the 2nd is an internal/hidden parameter:
          -- DECLARE @UserAlias nvarchar(256) = N'sid';
          DECLARE @Win10SupportedVersion nvarchar(50) = N'10.0.10240.0';

          DECLARE  @User nvarchar(256)
          ,@CloudUserId uniqueidentifier
          ,@Win10_First smallint
          ,@Win10_Secnd smallint
          ,@Win10_Third smallint
          ,@Win10_Forth smallint;
          SELECT  @Win10_First = PARSENAME(@Win10SupportedVersion,(LEN(@Win10SupportedVersion)-LEN(REPLACE(@Win10SupportedVersion,N'.',N'')))+1)
          ,@Win10_Secnd = PARSENAME(@Win10SupportedVersion,(LEN(@Win10SupportedVersion)-LEN(REPLACE(@Win10SupportedVersion,N'.',N''))))
          ,@Win10_Third = PARSENAME(@Win10SupportedVersion,(LEN(@Win10SupportedVersion)-LEN(REPLACE(@Win10SupportedVersion,N'.',N'')))-1)
          ,@Win10_Forth = ISNULL(PARSENAME(@Win10SupportedVersion,(LEN(@Win10SupportedVersion)-LEN(REPLACE(@Win10SupportedVersion,N'.',N'')))-2),0);

          -- Get the full UserName but ensure we only get one. For those with multiple records we'll grab the latest one based on the Creation Dates:
          SELECT  TOP 1
          @User = Unique_User_Name0
          ,@CloudUserId = CloudUserId
          FROM dbo.v_R_User
          WHERE User_Name0 = @UserAlias
          ORDER BY  AD_Object_Creation_Time0 DESC
          ,Creation_Date0 DESC;

          WITH XMLNAMESPACES ( 'http://schemas.microsoft.com/SystemsCenterConfigurationManager/2009/07/10/DesiredConfiguration' as dcm
          ,'http://schemas.microsoft.com/SystemsCenterConfigurationManager/2009/06/14/Rules' as rul)
          ,Policies AS ( -- This gets all CA policies and what they are "applicable to" along with the collections to which they are targeted:
          SELECT  DISTINCT cis.CI_ID
          ,cis.SDMPackageDigest.value(N'(/dcm:DesiredConfigurationDigest/dcm:ConfigurationPolicy/rul:Annotation/rul:DisplayName/@Text)[1]',N'nvarchar(513)') AS [CA_PolicyName]
          ,CASE
          WHEN plt.OSPlatform IN (N'x64',N'I386') AND (cis.ConfigurationFlags &amp; 3) = 3 AND plt.OSMinVersion = N'6.30.0000.0' THEN 'ConfigMgr Win8.1'
          WHEN plt.OSPlatform IN (N'x64',N'I386') AND (cis.ConfigurationFlags &amp; 3) = 3 AND plt.OSMinVersion = N'6.20.0000.0' THEN 'ConfigMgr Win8'
          WHEN plt.OSPlatform IN (N'x64',N'I386') AND (cis.ConfigurationFlags &amp; 3) = 3 AND plt.OSMinVersion = N'6.10.0000.0' THEN 'ConfigMgr Win7'
          WHEN plt.OSPlatform IN (N'x64',N'I386') AND (cis.ConfigurationFlags &amp; 3) = 3 AND plt.OSMinVersion = N'10.00.0000.0' THEN 'ConfigMgr Win10'
          WHEN plt.OSPlatform IN (N'x64',N'I386') AND (cis.ConfigurationFlags &amp; 1) = 1 AND plt.OSMinVersion = N'6.30.0000.0' THEN 'NDJ Win8.1'
          WHEN plt.OSPlatform IN (N'x64',N'I386') AND (cis.ConfigurationFlags &amp; 1) = 1 AND plt.OSMinVersion = N'10.00.0000.0' THEN 'NDJ Win10'
          WHEN plt.OSPlatform = N'Windows Phone' AND plt.OSMinVersion = N'8.10.0000.0' THEN 'WindowsPhone 8.1' -- cis.ConfigurationFlags = 1
          WHEN plt.OSPlatform = N'Windows Phone' AND plt.OSMinVersion = N'10.00.0000.0' THEN 'WindowsPhone 10' -- cis.ConfigurationFlags = 1
          WHEN plt.OSPlatform = N'Android' AND plt.CI_UniqueID LIKE '%AndroidForWork%' THEN 'AndroidForWork'
          WHEN plt.OSPlatform = N'Android' THEN 'Android'
          WHEN plt.OSPlatform = N'iOS' THEN 'iOS'
          END AS [ApplicableTo]
          ,col.CollectionID
          FROM dbo.CI_ConfigurationItems cis
          CROSS APPLY cis.SDMPackageDigest.nodes(N'//rul:PlatformApplicabilityCondition/rul:OperatingSystemExpression/rul:Operands/rul:RuleExpression') pac(RuleApplicability)
          INNER JOIN dbo.SupportedPlatforms plt
          ON pac.RuleApplicability.value(N'./@RuleId',N'nvarchar(max)') = plt.CI_UniqueID
          INNER JOIN dbo.CI_AssignmentTargetedCIs ass -- this is to get the assignment info for the policy
          ON cis.CI_ID = ass.CI_ID
          INNER JOIN dbo.v_CIAssignment asm
          ON ass.AssignmentID = asm.AssignmentID
          INNER JOIN dbo.v_Collection col -- this is to just get the assignment for the user based assignment &amp; the Collection Targeted to determine applicable devices
          ON asm.CollectionID = col.CollectionID
          AND col.CollectionType = 1 -- User = 1; Machine = 2
          WHERE cis.CIType_ID = 50
          AND cis.IsLatest = 1
          AND cis.ConfigurationFlags &amp; 3 IN (1,3)
          )
          ,UserDevices AS ( -- This gets all the devices associated with the supplied user (using UDA and ConditionalAccessCompliance for CM machines):
          SELECT  umr.UniqueUserName
          ,sis.ResourceID
          ,sis.Netbios_Name0
          ,sis.SMS_Unique_Identifier0
          ,dvc.Agent_Edition
          ,CASE sis.AgentEdition0
          WHEN 0 THEN ISNULL(REPLACE(REPLACE(REPLACE(sis.Build01,N'(',N'.'),N' ',N''),N')',N''),ops.Version0)
          ELSE sis.Client_Version0
          END AS [OS_Client_Version]
          FROM dbo.v_UserMachineRelation umr
          INNER JOIN dbo.v_R_System sis
          ON umr.MachineResourceID = sis.ResourceID
          AND ISNULL(sis.Obsolete0,0) = 0
          AND umr.RelationActive = 1
          LEFT OUTER JOIN (
          SELECT  ResourceID
          ,Version0
          ,ROW_NUMBER() OVER (PARTITION BY ResourceID ORDER BY TimeStamp DESC) AS [LatestOSRecord]
          FROM dbo.v_GS_OPERATING_SYSTEM
          ) ops
          ON sis.ResourceID = ops.ResourceID
          AND ops.LatestOSRecord = 1
          LEFT OUTER JOIN dbo.UserMachineSourceRelation usr
          ON umr.RelationshipResourceID = usr.RelationshipResourceID
          AND usr.SourceID = 5 -- "DeviceManagement" records only (for MDM)
          CROSS APPLY ( -- This is used so that we don't have to repeat the CASE multiple times...
          SELECT  CASE sis.AgentEdition0
          WHEN 0  THEN CASE WHEN ISNULL(REPLACE(REPLACE(REPLACE(sis.Build01,N'(',N'.'),N' ',N''),N')',N''),ops.Version0) = N'6.3.9600' THEN 'ConfigMgr Win8.1'
          WHEN ISNULL(REPLACE(REPLACE(REPLACE(sis.Build01,N'(',N'.'),N' ',N''),N')',N''),ops.Version0) = N'6.1.7601' THEN 'ConfigMgr Win7'
          WHEN CONVERT(smallint,ISNULL(PARSENAME(REPLACE(REPLACE(REPLACE(sis.Build01,N'(',N'.'),N' ',N''),N')',N''),3),PARSENAME(ops.Version0,(LEN(ops.Version0)-LEN(REPLACE(ops.Version0,N'.',N'')))+1))) &gt;= @Win10_First
          AND CONVERT(smallint,ISNULL(PARSENAME(REPLACE(REPLACE(REPLACE(sis.Build01,N'(',N'.'),N' ',N''),N')',N''),2),PARSENAME(ops.Version0,(LEN(ops.Version0)-LEN(REPLACE(ops.Version0,N'.',N'')))))) = @Win10_Secnd
          AND CONVERT(smallint,ISNULL(PARSENAME(REPLACE(REPLACE(REPLACE(sis.Build01,N'(',N'.'),N' ',N''),N')',N''),1),PARSENAME(ops.Version0,(LEN(ops.Version0)-LEN(REPLACE(ops.Version0,N'.',N'')))-1))) &gt;= @Win10_Third
          THEN 'ConfigMgr Win10'
          ELSE 'Unsupported ConfigMgr'
          END
          WHEN 1  THEN CASE WHEN sis.CPUType0 LIKE N'%ARM%' THEN 'WindowsRT'
          WHEN sis.CPUType0 IS NULL THEN 'Unknown OMADM'
          WHEN sis.Client_Version0 = N'6.3.9600' THEN 'NDJ Win8.1'
          WHEN CONVERT(smallint,PARSENAME(sis.Client_Version0,(LEN(sis.Client_Version0)-LEN(REPLACE(sis.Client_Version0,N'.',N'')))+1)) &gt;= @Win10_First
          AND CONVERT(smallint,PARSENAME(sis.Client_Version0,(LEN(sis.Client_Version0)-LEN(REPLACE(sis.Client_Version0,N'.',N''))))) = @Win10_Secnd
          AND CONVERT(smallint,PARSENAME(sis.Client_Version0,(LEN(sis.Client_Version0)-LEN(REPLACE(sis.Client_Version0,N'.',N'')))-1)) &gt;= @Win10_Third
          AND ISNULL(CONVERT(smallint,PARSENAME(sis.Client_Version0,(LEN(sis.Client_Version0)-LEN(REPLACE(sis.Client_Version0,N'.',N'')))-2)),0) &gt;= @Win10_Forth
          THEN 'NDJ Win10'
          ELSE 'Unsupported NDJ'
          END
          WHEN 4  THEN CASE --WHEN sis.Client_Version0 LIKE N'8.0.%' THEN 'WindowsPhone 8'
          WHEN sis.Client_Version0 LIKE N'8.1%' THEN 'WindowsPhone 8.1'
          WHEN CONVERT(smallint,PARSENAME(sis.Client_Version0,(LEN(sis.Client_Version0)-LEN(REPLACE(sis.Client_Version0,N'.',N'')))+1)) &gt;= @Win10_First
          AND CONVERT(smallint,PARSENAME(sis.Client_Version0,(LEN(sis.Client_Version0)-LEN(REPLACE(sis.Client_Version0,N'.',N''))))) = @Win10_Secnd
          AND CONVERT(smallint,PARSENAME(sis.Client_Version0,(LEN(sis.Client_Version0)-LEN(REPLACE(sis.Client_Version0,N'.',N'')))-1)) &gt;= @Win10_Third
          AND ISNULL(CONVERT(smallint,PARSENAME(sis.Client_Version0,(LEN(sis.Client_Version0)-LEN(REPLACE(sis.Client_Version0,N'.',N'')))-2)),0) &gt;= @Win10_Forth
          THEN 'WindowsPhone 10'
          ELSE 'Unsupported WP'
          END
          WHEN 8  THEN 'iPhone'
          WHEN 9  THEN 'iPad'
          WHEN 10 THEN 'iPodTouch'
          WHEN 11 THEN 'Android'
          WHEN 17 THEN 'AndroidForWork'
          END
          ) dvc(Agent_Edition)
          WHERE umr.UniqueUserName = @User
          AND CASE WHEN sis.AgentEdition0 &gt; 0 THEN usr.SourceID ELSE 5 END = 5 -- this is to get only the "DeviceManagement" records for the MDM devices
          UNION
          SELECT  usr.Unique_User_Name0
          ,sis.ResourceID
          ,sis.Netbios_Name0
          ,sis.SMS_Unique_Identifier0
          ,dvc.Agent_Edition
          ,ISNULL(REPLACE(REPLACE(REPLACE(sis.Build01,N'(',N'.'),N' ',N''),N')',N''),ops.Version0) AS [OS_Client_Version]
          FROM dbo.ConditionalAccessCompliance cac
          INNER JOIN dbo.v_R_User usr
          ON cac.UserItemKey = usr.ResourceID
          INNER JOIN dbo.v_R_System sis
          ON cac.DeviceItemKey = sis.ResourceID
          LEFT OUTER JOIN (
          SELECT  ResourceID
          ,Version0
          ,ROW_NUMBER() OVER (PARTITION BY ResourceID ORDER BY TimeStamp DESC) AS [LatestOSRecord]
          FROM dbo.v_GS_OPERATING_SYSTEM
          ) ops
          ON sis.ResourceID = ops.ResourceID
          AND ops.LatestOSRecord = 1
          CROSS APPLY ( -- This is used so that we don't have to repeat the CASE multiple times...
          SELECT  CASE sis.AgentEdition0
          WHEN 0  THEN CASE 
          WHEN ISNULL(REPLACE(REPLACE(REPLACE(sis.Build01,N'(',N'.'),N' ',N''),N')',N''),ops.Version0) = N'6.3.9600' THEN 'ConfigMgr Win8.1'
          WHEN ISNULL(REPLACE(REPLACE(REPLACE(sis.Build01,N'(',N'.'),N' ',N''),N')',N''),ops.Version0) = N'6.2.9200' THEN 'ConfigMgr Win8'
          WHEN ISNULL(REPLACE(REPLACE(REPLACE(sis.Build01,N'(',N'.'),N' ',N''),N')',N''),ops.Version0) = N'6.1.7601' THEN 'ConfigMgr Win7'
          WHEN CONVERT(smallint,ISNULL(PARSENAME(REPLACE(REPLACE(REPLACE(sis.Build01,N'(',N'.'),N' ',N''),N')',N''),3),PARSENAME(ops.Version0,(LEN(ops.Version0)-LEN(REPLACE(ops.Version0,N'.',N'')))+1))) &gt;= @Win10_First
          AND CONVERT(smallint,ISNULL(PARSENAME(REPLACE(REPLACE(REPLACE(sis.Build01,N'(',N'.'),N' ',N''),N')',N''),2),PARSENAME(ops.Version0,(LEN(ops.Version0)-LEN(REPLACE(ops.Version0,N'.',N'')))))) = @Win10_Secnd
          AND CONVERT(smallint,ISNULL(PARSENAME(REPLACE(REPLACE(REPLACE(sis.Build01,N'(',N'.'),N' ',N''),N')',N''),1),PARSENAME(ops.Version0,(LEN(ops.Version0)-LEN(REPLACE(ops.Version0,N'.',N'')))-1))) &gt;= @Win10_Third
          THEN 'ConfigMgr Win10'
          ELSE 'Unsupported ConfigMgr'
          END
          END
          ) dvc(Agent_Edition)
          WHERE usr.Unique_User_Name0 = @User
          )
          ,ApplicableDevices AS ( -- This determines which devices and CA policies are applicable for the user:
          -- Note: records found in "ConditionalAccessCompliance" for machines that don't match the "applicableto" in a policy will be removed...
          --  i.e., a record for a machine with an OS of 8.1 will be removed if there is not a policy applicable to ConfigMgr Win8.1
          SELECT  DISTINCT usd.ResourceID
          ,usd.Netbios_Name0
          ,usd.SMS_Unique_Identifier0
          ,usd.UniqueUserName
          ,usd.Agent_Edition
          ,usd.OS_Client_Version
          ,pol.CA_PolicyName
          ,pol.CI_ID
          FROM UserDevices usd
          INNER JOIN Policies pol
          ON CASE WHEN usd.Agent_Edition IN ('iPhone','iPad','iPodTouch') THEN 'iOS' ELSE usd.Agent_Edition END = pol.ApplicableTo
          INNER JOIN dbo.vCollectionMembers mem
          ON pol.CollectionID = mem.SiteID
          AND usd.UniqueUserName = mem.SMSID
          )
          ,Results AS ( -- This returns the applicable devices along with the status from CurrentComplianceStatus:
          SELECT  dvc.ResourceID
          ,dvc.Netbios_Name0
          ,dvc.SMS_Unique_Identifier0
          ,dvc.UniqueUserName
          ,dvc.Agent_Edition
          ,dvc.CA_PolicyName
          ,dvc.CI_ID
          ,dvc.OS_Client_Version
          ,snm.StateName
          ,sts.ComplianceState
          ,sts.IsApplicable
          --,ROW_NUMBER() OVER(PARTITION BY dvc.ResourceID,dvc.CA_PolicyName ORDER BY sts.UserID DESC) AS [MaxUserIDNum] -- this is used later to ensure that we are only getting one record per resource with our preference being the non "(SYSTEM)" record
          ,sts.CI_CurrentComplianceStatusID
          FROM ApplicableDevices dvc
          LEFT OUTER JOIN dbo.v_CICurrentComplianceStatus sts
          ON dvc.ResourceID = sts.ResourceID
          AND dvc.CI_ID = sts.CI_ID
          --AND dvc.UniqueUserName = sts.FullName  -- do we want to see the status returned by other users??
          -- can't put isapplicable = 1 because that will mess up the "Unknown" stats....
          LEFT OUTER JOIN dbo.v_StateNames snm
          ON sts.ComplianceState = snm.StateID
          AND snm.TopicType = 401
          )

          -- This is the final resultset which returns error and non-compliance details for each device in one record rather than multiple:
          SELECT  rst.CA_PolicyName
          ,rst.ResourceID
          ,mdm.DeviceID
          ,rst.Netbios_Name0
          ,rst.UniqueUserName
          ,@CloudUserId AS [CloudUserId]
          ,rst.Agent_Edition
          ,rst.OS_Client_Version
          ,atv.ActiveDate AS [LastConnected]
          ,ISNULL(rst.StateName,N'Unknown') AS [ComplianceState_Desc]
          ,ISNULL(rst.ComplianceState,99) AS [ComplianceState]
          ,STUFF(ers.Settings,1,2,N'') AS [ErrorSettings]
          ,STUFF(csd.NonCompliantSettings,1,2,N'') AS [NonCompliantSettings]
          --,cac.IsCompliant
          --,CASE WHEN cac.ComplianceSetTime != N'' THEN CONVERT(datetime,cac.ComplianceSetTime) END AS [ComplianceEvalTime]
          ,ISNULL(mdm.AAD_DeviceID,cac.AADDeviceID) AS [AAD_DeviceID]
          --,mdm.AAD_UpdateTime
          --,mdm.AAD_ComplianceStatus
          ,ISNULL(cac.IsCompliant,mdm.AAD_ComplianceStatus) AS [CAC_AAD_ComplianceStatus]
          ,ISNULL(CASE WHEN cac.ComplianceSetTime != N'' THEN CONVERT(datetime,cac.ComplianceSetTime) END,mdm.AAD_UpdateTime) AS [CAC_AAD_Time]
          --,cac.ComplianceEvalTime
          --,mdm.AAD_UpdateTime
          FROM Results rst
          LEFT OUTER JOIN (
          SELECT  ResourceId
          ,MAX(AgentTime) AS [ActiveDate]
          FROM dbo.v_AgentDiscoveries
          WHERE AgentName IN ('MP_ClientRegistration', 'Heartbeat Discovery','MP_INVENTORY_MANAGER')
          GROUP BY ResourceId
          ) atv
          ON rst.ResourceID = atv.ResourceId
          LEFT OUTER JOIN dbo.v_R_User usr
          ON rst.UniqueUserName = usr.Unique_User_Name0
          LEFT OUTER JOIN dbo.ConditionalAccessCompliance cac
          ON rst.ResourceID = cac.DeviceItemKey
          AND usr.ResourceID = cac.UserItemKey
          LEFT OUTER JOIN (
          SELECT  CONVERT(nvarchar(256),cli.DeviceID) AS [DeviceID]
          ,cli.ClientState
          ,MIN(CASE WHEN prp.PropertyName = N'ReferenceGUID' THEN prp.PropertyValue END) AS [AAD_DeviceID]
          ,MIN(CASE WHEN prp.PropertyName = N'GraphIsCompliantLastUpdateTime' THEN CASE WHEN prp.PropertyValue NOT IN (N'',N'01/01/0001 00:00:00.000') THEN CONVERT(datetime,prp.PropertyValue) END END) AS [AAD_UpdateTime]
          ,MIN(CASE WHEN prp.PropertyName = N'IsCompliant' THEN prp.PropertyValue END) AS [AAD_ComplianceStatus]
          FROM dbo.MDMClientIdentity cli
          LEFT OUTER JOIN dbo.v_MDMDeviceProperty prp
          ON cli.DeviceID = prp.DeviceID
          AND PropertyName IN (N'ReferenceGUID',N'GraphIsCompliantLastUpdateTime',N'IsCompliant')
          GROUP BY  CONVERT(nvarchar(256),cli.DeviceID)
          ,cli.ClientState
          ) mdm
          ON rst.SMS_Unique_Identifier0 = mdm.DeviceID
          -- Error Info
          CROSS APPLY (
          SELECT  N', '+stg.SettingName
          FROM dbo.v_CI_CurrentErrorDetails err
          LEFT OUTER JOIN dbo.v_CISettings stg
          ON err.CI_ID = stg.CI_ID
          AND err.SettingID = stg.Setting_ID
          WHERE err.CurrentComplianceStatusID = rst.CI_CurrentComplianceStatusID
          FOR XML PATH ('')
          ) ers(Settings)
          -- Non-Compliant Info
          CROSS APPLY (
          SELECT  N', '+Setting_UniqueID
          FROM dbo.v_CIComplianceStatusDetail
          WHERE CI_ID = rst.CI_ID
          AND ResourceID = rst.ResourceID
          AND FullName = rst.UniqueUserName
          FOR XML PATH ('')
          ) csd(NonCompliantSettings)
          WHERE ISNULL(rst.IsApplicable,1) = 1;
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserAlias">
            <Value>=Parameters!UserAlias.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
