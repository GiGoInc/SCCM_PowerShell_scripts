    <DataSet Name="UserThreatList">
      <Fields>
        <Field Name="ResourceID">
          <DataField>ResourceID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ComputerName">
          <DataField>ComputerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ThreatName">
          <DataField>ThreatName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FirstDetection">
          <DataField>FirstDetection</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Category">
          <DataField>Category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastDetection">
          <DataField>LastDetection</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Severity">
          <DataField>Severity</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="InfectionCount">
          <DataField>InfectionCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT
ResourceID,
ComputerName, 
ThreatName,
Category,
Severity,
MIN(DetectionTime) AS FirstDetection, 
MAX(DetectionTime) AS LastDetection, 
COUNT(*) as InfectionCount
FROM
(
	SELECT
        t.ResourceID,
        ISNULL(tc.Name,t.ThreatName) AS ThreatName,
        cat.Category,
        sev.Severity,
        ISNULL(s.Netbios_Name0 + '.' + s.Full_Domain_Name0, s.Netbios_Name0) as ComputerName,
        t.DetectionTime     
FROM fn_rbac_GS_Threats(@UserSIDs)  t
   LEFT JOIN fn_rbac_ThreatCatalog(@UserSIDs)  tc on t.ThreatID=tc.ThreatID
   LEFT JOIN fn_rbac_ThreatSeverities(@UserSIDs)  sev on tc.SeverityID=sev.SeverityID
   LEFT JOIN fn_rbac_ThreatCategories(@UserSIDs)  cat on tc.CategoryID=cat.CategoryID
   JOIN fn_rbac_R_System(@UserSIDs)  s on t.ResourceID=s.ResourceID
   WHERE 
      DATEADD(day, 0, DATEDIFF(day, 0, t.DetectionTime)) BETWEEN @StartDate AND @EndDate AND 
      UserName=@UserName AND s.Obsolete0=0
) AS Infections
GROUP BY ResourceID, ComputerName, ThreatName, Category, Severity
ORDER BY FirstDetection</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserName">
            <Value>=Parameters!UserName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="StartEndDates">
      <Fields>
        <Field Name="StartDate">
          <DataField>StartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="EndDate">
          <DataField>EndDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select DATEADD(day,-7,DATEDIFF(day, 0, GetUTCDate())) as StartDate, DATEADD(day,0,DATEDIFF(day, 0, GetUTCDate())) as EndDate</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="DataSetAdminID">
