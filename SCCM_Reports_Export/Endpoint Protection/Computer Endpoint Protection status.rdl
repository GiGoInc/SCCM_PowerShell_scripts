      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT a.* FROM   fn_rbac_FullCollectionMembership(@UserSIDs)  coll 
                LEFT OUTER JOIN fn_rbac_CombinedDeviceResources(@UserSIDs)  a on a.MachineID = coll.ResourceID
                LEFT OUTER JOIN fn_rbac_EndpointProtectionStatus(@UserSIDs)  g ON coll.ResourceID = g.ResourceID
WHERE  coll.CollectionID = @CollectionID
       AND (@OverallStatus IS NULL
            OR @OverallStatus = 1 AND g.EpProtected= 1
            OR @OverallStatus = 2 AND g.EpAtRisk = 1
            OR @OverallStatus = 3 AND g.EpNotYetInstalled = 1
            OR @OverallStatus = 4 AND g.EpUnsupported = 1
            OR @OverallStatus = 5 AND g.Inactive = 1
            OR @OverallStatus = 6 AND g.NotClient = 1) 
       AND (@RemediationStatus IS NULL
            OR @RemediationStatus = 1 AND g.AmRemediationFailed = 1
            OR @RemediationStatus = 2 AND g.AmFullscanRequired = 1
            OR @RemediationStatus = 3 AND g.AmRestartRequired = 1
            OR @RemediationStatus = 4 AND g.AmOfflineScanRequired = 1
            OR @RemediationStatus = 5 AND g.AmManualStepsRequired = 1
            OR @RemediationStatus = 6 AND g.AmRecentlyCleaned = 1)
       AND (@OperationalStatus IS NULL
            OR @OperationalStatus = 1 AND g.EpInstallFailed = 1
            OR @OperationalStatus = 2 AND g.EpEnforcementFailed = 1
            OR @OperationalStatus = 3 AND g.EpPendingReboot = 1
            OR @OperationalStatus = 4 AND g.Unhealthy = 1)
       AND (@DefinitionStatus IS NULL 
            OR @DefinitionStatus = 1 AND g.SignatureUpTo1DayOld = 1 
            OR @DefinitionStatus = 2 AND g.SignatureUpTo3DaysOld = 1
            OR @DefinitionStatus = 3 AND g.SignatureUpTo7DaysOld = 1
            OR @DefinitionStatus = 4 AND g.SignatureOlderThan7Days = 1
            OR @DefinitionStatus = 5 AND g.NoSignature = 1)
       AND (@PolicyApplicationStatus IS NULL 
            OR @PolicyApplicationStatus = 1 AND g.EpEnforcementSucceeded = 1 
            OR @PolicyApplicationStatus = 2 AND g.EpEnforcementFailed = 1)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OverallStatus">
            <Value>=Parameters!OverallStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RemediationStatus">
            <Value>=Parameters!RemediationStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OperationalStatus">
            <Value>=Parameters!OperationalStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DefinitionStatus">
            <Value>=Parameters!DefinitionStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PolicyApplicationStatus">
            <Value>=Parameters!PolicyApplicationStatus.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
