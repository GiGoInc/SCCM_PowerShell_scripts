      <Fields>
        <Field Name="DeploymentID">
          <DataField>DeploymentID</DataField>
        </Field>
        <Field Name="DeploymentName">
          <DataField>DeploymentName</DataField>
        </Field>
        <Field Name="Available">
          <DataField>Available</DataField>
        </Field>
        <Field Name="Deadline">
          <DataField>Deadline</DataField>
        </Field>
        <Field Name="LastModificationTime">
          <DataField>LastModificationTime</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select distinct 
                cia.Assignment_UniqueID as DeploymentID, 
                cia.AssignmentName as DeploymentName, 
                cia.StartTime as Available, 
                cia.EnforcementDeadline as Deadline, 
                IsUpdateGroupAssignment = case  cia.AssignmentType when 5 then '*' else '' end,
                cia.LastModificationTime as LastModificationTime 
            from v_CIAssignmentTargetedMachines atm 
            join fn_rbac_CIAssignment(@UserSIDs)  cia on atm.AssignmentID=cia.AssignmentID 
            where atm.ResourceID = @ResourceID and cia.AssignmentType in (1,5)
            order by cia.AssignmentName</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="ResourceID">
            <Value>=Parameters!MachineID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="MachineName">
          <DataField>MachineName</DataField>
        </Field>
        <Field Name="ResourceID">
          <DataField>ResourceID</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          select MachineName, ResourceID
          from fn_rbac_ClientMachines(@UserSIDs)  cm
          cross apply (select MachineName=Name+isnull('.'+Domain, '')) mn
          where (@MachineNameFilter='' or MachineName like @MachineNameFilter)
          order by 1
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="MachineNameFilter">
            <Value>=Parameters!MachineNameFilter.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSetAdminID">
      <Fields>
        <Field Name="UserSIDs">
          <DataField>UserSIDs</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select dbo.fn_rbac_GetAdminIDsfromUserSIDs(@UserTokenSIDs) as UserSIDs</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserTokenSIDs">
            <Value>=Parameters!UserTokenSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
