      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>--declare @UserSIDs nvarchar(255)='disabled',
--    @DeploymentID nvarchar(255)='',
--    @locale nvarchar(8)='en-us'
    
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          
          ---- use this to get the maximam perf load for dev test
          --select top 1 @DeploymentID = a.Assignment_UniqueID
          --  from fn_rbac_CIAssignment(@UserSIDs)  a
          --  join fn_rbac_CIAssignmentToCI(@UserSIDs)  aci on aci.AssignmentID=a.AssignmentID
          --  join fn_rbac_UpdateInfo(@lcid, @UserSIDs) ui on ui.CI_ID=aci.CI_ID
          --  group by a.Assignment_UniqueID
          --  having COUNT(ui.CI_ID) &lt; 500
          --  order by COUNT(ui.CI_ID) desc
          
          declare @asnid int, @collid varchar(8); select @asnid=Assignmentid, @collid=Collectionid from fn_rbac_CIAssignment(@UserSIDs)  where Assignment_UniqueID=@DeploymentID
          declare @ci table(CI_ID int primary key, CI_UniqueID nvarchar(256), Title nvarchar(512), ArticleID nvarchar(64), BulletinID nvarchar(64))
          declare @cilist CIIDList_Type
          
          insert @ci(CI_ID, CI_UniqueID, Title, ArticleID, BulletinID)           
          output inserted.CI_ID into @cilist
            select ui.CI_ID, ui.CI_UniqueID, ui.Title, ui.ArticleID, ui.BulletinID
            from fn_rbac_CIAssignment(@UserSIDs)  a
            join fn_rbac_CIAssignmentToCI(@UserSIDs)  aci on aci.AssignmentID=a.AssignmentID
            join fn_rbac_UpdateInfo(@lcid, @UserSIDs) ui on ui.CI_ID=aci.CI_ID
            where a.AssignmentID=@asnid
          
          select  
            BulletinID,
            ArticleID,
            Title,
            udl.NumPresent as Present,
            udl.NumMissing as Missing,
            udl.NumNotApplicable as NotApplicable,
            udl.NumUnknown as Unknown,
            udl.NumFailed as Failed,
            udl.NumPending as Pending,
            ui.CI_UniqueID as UniqueUpdateID,
            @DeploymentID as DeploymentID
          from fn_UpdateSummaryPerCollection(@cilist,@collid)  udl 
          join @ci ui on udl.CI_ID=ui.CI_ID
          order by ui.Title</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DEPLOYMENTID">
            <Value>=Parameters!DEPLOYMENTID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
