        </Field>
        <Field Name="DisplayTitle">
          <Value>=String.Format("{0} ({1})", Fields!ContentUniqueID.Value, Fields!ContentLanguage.Value)</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
            declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
            select 
                ContentUniqueID = ucis.CI_UniqueID,
				ContentLanguage = isnull(nullif(uc.ContentLocales, ''), 'Neutral')
            from fn_rbac_UpdateCIs(@UserSIDs)  ci
            join fn_rbac_UpdateContents(@UserSIDs)  uc on uc.CI_ID=ci.CI_ID
            join fn_rbac_UpdateCIs(@UserSIDs)  ucis on ucis.CI_ID = uc.ContentCI_ID
            where ci.CI_UniqueID=@UpdateID
            order by 1
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="UpdateID">
            <Value>=Parameters!UpdateID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
