        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
            declare @DeploymentLocalID int = (select AssignmentID from fn_rbac_CIAssignment(@UserSIDs)  where Assignment_UniqueID = @DeploymentID)
            declare @UpdateLocalID int = (select CI_ID from fn_rbac_UpdateCIs(@UserSIDs)  where CI_UniqueID = @UpdateID)
            declare @ContentLocalID int = (select CI_ID from fn_rbac_UpdateCIs(@UserSIDs)  where CI_UniqueID = @ContentID)
            declare @SDMTYPE varchar(256) = (select ModelName from fn_rbac_UpdateCIs(@UserSIDs)  where CI_UniqueID = @ContentID)
 
            select distinct 
                m.Name0 as ComputerName0, 
                m.User_Domain0+'\'+m.User_Name0 as LastLoggedOnUser, 
                asite.SMS_Assigned_Sites0 as AssignedSite,  
                m.Client_Version0 as ClientVersion,  
                st.StateName as Status, 
                ucs.LastEnforcementStatusMsgID&amp;0x0000FFFF as ErrorStatusID,  
                ucs.LastEnforcementMessageTime as DeploymentStateTime,
                sm.RecordID as ERecordID, 
                uc.ContentLocales as Language,
                statusinfo.MessageStateName as ErrorStatusName  
            from fn_rbac_CIAssignment(@UserSIDs)  a  
            join fn_rbac_CIAssignmentToCI(@UserSIDs)  atc on atc.AssignmentID=a.AssignmentID and (@UpdateLocalID is null or @UpdateLocalID=atc.CI_ID) 
            join fn_rbac_UpdateContents(@UserSIDs)  uc on uc.CI_ID=atc.CI_ID and (@ContentLocalID is null or @ContentLocalID=uc.ContentCI_ID) and uc.ContentProvisioned=0
            join fn_rbac_UpdateComplianceStatus(@UserSIDs)  ucs on ucs.CI_ID=uc.ContentCI_ID and ucs.Status=2 
            join fn_rbac_CIAssignmentTargetedMachines(@UserSIDs)  cm on cm.AssignmentID=a.AssignmentID and cm.ResourceID=ucs.ResourceID 
            join fn_rbac_R_System(@UserSIDs)  m on m.ResourceType=5 and m.ResourceID=ucs.ResourceID and isnull(m.Obsolete0,0)=0
            left join fn_rbac_RA_System_SMSAssignedSites(@UserSIDs)  asite on m.ResourceID=asite.ResourceID 
            left join fn_rbac_AdvertisementStatusInformation(@UserSIDs)  statusinfo on statusinfo.MessageID=ucs.LastEnforcementStatusMsgID&amp;0x0000FFFF
            left join fn_rbac_StateNames(@UserSIDs)  st on ucs.LastEnforcementMessageID=st.StateID and st.TopicType=402
            outer apply (
                select top 1 smf.RecordID 
                from v_StatusMessage  smf 
                join fn_rbac_StatMsgAttributes(@UserSIDs)  att on att.RecordID=smf.RecordID and att.AttributeID=417 and att.AttributeValue=@SDMTYPE
                where smf.MessageID=(ucs.LastEnforcementStatusMsgID&amp;0x0000FFFF) and smf.MachineName=m.Name0
                order by att.AttributeTime desc
                ) sm
            where a.AssignmentID = @DeploymentLocalID 
            order by 1
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DeploymentID">
            <Value>=Parameters!DeploymentID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UpdateID">
            <Value>=Parameters!UpdateID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ContentID">
            <Value>=Parameters!ContentID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="DeploymentID">
          <DataField>DeploymentID</DataField>
        </Field>
        <Field Name="DeploymentName">
          <DataField>DeploymentName</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          select DeploymentID = cia.Assignment_UniqueID
               , DeploymentName = isnull(grp.Title+' - ', '') + cia.AssignmentName
          from fn_rbac_CIAssignment(@UserSIDs)  cia
          left join fn_rbac_CIAssignmentToGroup(@UserSIDs)  atg on cia.AssignmentType=5 and atg.AssignmentID=cia.AssignmentID
          left join fn_rbac_AuthListInfo(@lcid, @UserSIDs) grp on cia.AssignmentType=5 and grp.CI_ID=atg.AssignedUpdateGroup
          where cia.AssignmentType in (1,5)
            and (@filterwildcard='' or cia.Assignment_UniqueID like @filterwildcard or cia.AssignmentName like @filterwildcard or grp.Title like @filterwildcard)
          order by 2
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_DataSet_UpdateID">
      <Fields>
        <Field Name="UpdateID">
          <DataField>CI_UniqueID</DataField>
        </Field>
        <Field Name="DisplayTitle">
          <DataField>DisplayTitle</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          select CI_UniqueID, DisplayTitle = ui.Title+isnull(' ('+nullif(ui.ArticleID, '')+')', '') + isnull(' ('+nullif(ui.BulletinID, '') +')', '')
          from fn_rbac_CIAssignment(@UserSIDs)  a
          join fn_rbac_CIAssignmentToCI(@UserSIDs)  atc on atc.AssignmentID=a.AssignmentID
          join fn_rbac_UpdateInfo(@lcid, @UserSIDs) ui on ui.CI_ID=atc.CI_ID
          where a.Assignment_UniqueID=@DeploymentID
          order by 2
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="DeploymentID">
            <Value>=Parameters!DeploymentID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
