          <DataField>UniqueUpdateID</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>-- leave below block for easy dev test
--declare @UserSIDs nvarchar(255)='disabled',
--    @Vendor nvarchar(255)='',
--    @UpdateClass nvarchar(255)='',
--    @CollID nvarchar(8)='SMS00001',
--    @rowcount int = 20,
--    @locale nvarchar(8)='en-us'
    
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          declare @VendorID int; if @Vendor='' set @VendorID=0 else select @VendorID=CategoryInstanceID from fn_rbac_CategoryInfo(@lcid, @UserSIDs) where CategoryTypeName='Company' and CategoryInstanceName=@Vendor 
          declare @ClassID int; if @UpdateClass='' set @ClassID=0 else select @ClassID=CategoryInstanceID from fn_rbac_CategoryInfo(@lcid, @UserSIDs) where CategoryTypeName='UpdateClassification' and CategoryInstanceName=@UpdateClass 
          declare @CI CIIDList_Type
          
          ; with CA as (select AssignmentID from fn_rbac_CIAssignmentTargetedCollections(@UserSIDs)  where CollectionID=@CollID) 
               , CM as (select distinct cs.CI_ID
                        from fn_rbac_ClientCollectionMembers(@UserSIDs) cm join fn_rbac_UpdateComplianceStatus(@UserSIDs)  cs on cs.ResourceID=CM.ResourceID
                        where cs.Status=2 and CollectionID=@CollID and IsClient=1) 
          insert @CI 
            select top(@rowcount) ci.CI_ID 
            from fn_rbac_UpdateCIs(@UserSIDs)  ci 
            inner join CM on CM.CI_ID=ci.CI_ID
            where ci.IsHidden=0 and ci.IsExpired=0 and ci.CIType_ID in (1,8)
              and (@VendorID=0 or ci.CI_ID in (select CI_ID from fn_rbac_CICategories_All(@UserSIDs)  where CategoryInstanceID=@VendorID)) 
              and (@ClassID=0 or ci.CI_ID in (select CI_ID from fn_rbac_CICategories_All(@UserSIDs)  where CategoryInstanceID=@ClassID)) 
              and not exists(select 1 from CA join fn_rbac_CIAssignmentToCI(@UserSIDs)  ac on ac.AssignmentID=CA.AssignmentID and ac.CI_ID=ci.CI_ID)
            order by ci.DateRevised desc

          select   
              ui.ArticleID as ArticleID, 
              ui.BulletinID as BulletinID, 
              ui.Title as Title, 
              NumMissing as Missing, 
              PComputers=convert(float, isnull(NumMissing, 0)*100.0)/isnull(nullif(NumTotal, 0), 1), 
              ui.InfoURL as InformationURL, 
              ui.CI_UniqueID as UniqueUpdateID 
          from fn_UpdateSummaryPerCollection(@CI,@CollID) ci 
          left join fn_UpdateInfo(@lcid) ui on ci.CI_ID=ui.CI_ID 
          where ci.NumMissing&gt;0 
          order by NumMissing desc, ui.Title</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="CollID">
            <Value>=Parameters!CollID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="Vendor">
            <Value>=Parameters!Vendor.Value</Value>
          </QueryParameter>
          <QueryParameter Name="UpdateClass">
            <Value>=Parameters!UpdateClass.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@rowcount">
            <Value>=Parameters!rowcount.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
