      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          select CollectionID, CollectionName=Name, NameSort=CollectionID+' - '+Name 
          from fn_rbac_Collection(@UserSIDs)  
          order by 2
        </CommandText>
