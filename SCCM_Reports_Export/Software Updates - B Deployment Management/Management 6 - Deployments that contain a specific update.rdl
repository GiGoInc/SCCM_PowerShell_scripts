          <DataField>Available</DataField>
        </Field>
        <Field Name="Deadline">
          <DataField>Deadline</DataField>
        </Field>
        <Field Name="NAPEnabled">
          <DataField>NAPEnabled</DataField>
        </Field>
        <Field Name="DeploymentID">
          <DataField>DeploymentID</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
              declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
              select
                DeploymentName = a.AssignmentName,
                CollectionName = a.CollectionName,
                CollectionID = a.CollectionID,
                Available = a.StartTime,
                Deadline = a.EnforcementDeadline,
                IsUpdateGroupAssignment = case  a.AssignmentType when 5 then '*' else '' end,
                NAPEnabled = case ui.IsQuarantined when 1 then '*' else '' end,
                DeploymentID = a.Assignment_UniqueID
              from fn_rbac_UpdateInfo(@lcid,@UserSIDs)  ui
              join fn_rbac_CIAssignmentToCI(@UserSIDs)  atc on ui.CI_ID=atc.CI_ID
              join fn_rbac_CIAssignment(@UserSIDs)  a on a.AssignmentID=atc.AssignmentID and a.AssignmentType in (1,5)
              where (@UPDATEID is null or ui.CI_UniqueID = @UPDATEID)
              order by AssignmentName
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UPDATEID">
            <Value>=Parameters!UpdateID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
