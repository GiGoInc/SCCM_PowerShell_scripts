      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
            declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
            select
            ui.ArticleID,
            ui.BulletinID,
            ui.Title,
            UniqueUpdateID = ui.CI_UniqueID,
            Language = ui.Locales
            from fn_rbac_UpdateInfo(@lcid, @UserSIDs) ui
            where ui.CI_UniqueID = @UPDATEID and CIType_ID in (1,8)
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UPDATEID">
            <Value>=Parameters!UpdateID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_DataSet_UpdateID">
      <Fields>
        <Field Name="UpdateID">
          <DataField>CI_UniqueID</DataField>
        </Field>
        <Field Name="DisplayTitle">
          <DataField>DisplayTitle</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
            declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
            select CI_UniqueID, DisplayTitle = ui.Title+isnull(' ('+nullif(ui.ArticleID, '')+')', '') + isnull(' ('+nullif(ui.BulletinID, '') +')', '')
            from fn_rbac_UpdateInfo(@lcid, @UserSIDs) ui
            WHERE (@filterwildcard='' or Title like @filterwildcard or ArticleID like @filterwildcard or BulletinID like @filterwildcard) and CIType_ID in (1,8)
            order by 2
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value>=Parameters!UpdateFilter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
