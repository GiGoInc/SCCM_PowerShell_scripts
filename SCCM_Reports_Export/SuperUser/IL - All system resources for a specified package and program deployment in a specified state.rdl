    <DataSet Name="DeploymentDataset">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PackageProgram">
            <Value>=Parameters!PackageProgram.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Collection">
            <Value>=Parameters!Collection.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
  advert.AdvertisementID,
  advert.PresentTime
from fn_rbac_Advertisement(@UserSIDs)  advert
where advert.PackageID = @PackageProgram and
            advert.CollectionID = @Collection</CommandText>
