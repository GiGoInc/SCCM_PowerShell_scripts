      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>begin 
 if (@filterwildcard = '') 
 
select distinct SettingIndex, CONVERT(bigint, SettingIndex)  AS SettingIndexDWORD from
(
select  ACSettingIndex0 as SettingIndex from fn_rbac_GS_POWER_MANAGEMENT_SETTINGS(@UserSIDs)  
union all
 select    DCSettingIndex0 as SettingIndex from fn_rbac_GS_POWER_MANAGEMENT_SETTINGS(@UserSIDs) 

) as tSet
order by CONVERT(bigint, SettingIndex) 

else 
 select distinct ACSettingIndex0 AS SettingIndex, CONVERT(bigint, ACSettingIndex0)  AS SettingIndexDWORD from fn_rbac_GS_POWER_MANAGEMENT_SETTINGS(@UserSIDs)  p 
 WHERE p.ACSettingIndex0 like @filterwildcard 
 order by p.ACSettingIndex0 
end</CommandText>
