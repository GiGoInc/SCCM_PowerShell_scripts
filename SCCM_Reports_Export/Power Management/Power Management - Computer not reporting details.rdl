<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <rdl:Language xmlns:rdl="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition">=User!Language</rdl:Language>
  <Description>Displays a list of computers not reporting any power activity for a specified  date and time.</Description>
  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>default_reference</DataSourceReference>
      <rd:DataSourceID>55b5ce5c-0d20-42f9-8366-163d9f47edf2</rd:DataSourceID>
      <rd:SecurityType>None</rd:SecurityType>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet3">
      <Fields>
        <Field Name="reportDate">
          <DataField>date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>EXEC [SCCM_Rpt].[rptspPowerManagementGetDayRange] 31, - 1, 0</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="DataSetGetTotalComputersForCollection">
      <Fields>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select count(*) as Total from fn_rbac_ClientCollectionMembers(@UserSIDs)  where CollectionID = @collectionId and IsClient=1</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@collectionId">
            <Value>=Parameters!collectionId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSetGetCollectionName">
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LocName">
          <Value>=SrsResources.Localization.GetDBString(Fields!Name.Value, User!Language)</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select v_Collection_Alias.Name from fn_rbac_Collection(@UserSIDs) v_Collection_Alias 
 WHERE v_Collection_Alias.CollectionID = @collectionId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@collectionId">
            <Value>=Parameters!collectionId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OS">
          <DataField>OS</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastPowerReport">
          <DataField>LastPowerReport</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="LastHWScan">
          <DataField>LastHWScan</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="LastHeartbeatTime">
          <DataField>LastHeartbeatTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="LastPolicyRequest">
          <DataField>LastPolicyRequest</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
SELECT  
        nr.Name,  
        tOS.Caption0 AS 'OS',
        nr.LastPowerReport,
        nr.LastHWScan,
        nr.LastHeartbeatTime,
        nr.LastPolicyRequest
FROM 
        SCCM_Rpt.rptfnPowerManagementGetNotReporting (@reportDate, @collectionId, @deviceType) nr
        LEFT OUTER JOIN fn_rbac_GS_OPERATING_SYSTEM(@UserSIDs)  AS tOS ON tOS.ResourceID = nr.MachineID
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@reportDate">
            <Value>=Parameters!reportDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@collectionId">
            <Value>=Parameters!collectionId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@deviceType">
            <Value>=Parameters!deviceType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSetCollection">
      <Fields>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
        </Field>
        <Field Name="LocName">
          <Value>=SrsResources.Localization.GetDBString(Fields!Name.Value, User!Language)</Value>
        </Field>
        <Field Name="CollectionID_f0edddb0aaacccd">
          <DataField>CollectionID</DataField>
        </Field>
        <Field Name="Name_bfee0cb0dfcdcf">
          <DataField>Name</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>begin 
 if (@filterwildcard = '') 
 select v_Collection_Alias.CollectionID, v_Collection_Alias.Name from fn_rbac_Collection(@UserSIDs) v_Collection_Alias 
 where v_Collection_Alias.CollectionType = 2
 order by v_Collection_Alias.Name 
else 
 select v_Collection_Alias.CollectionID, v_Collection_Alias.Name from fn_rbac_Collection(@UserSIDs) v_Collection_Alias 
 WHERE v_Collection_Alias.CollectionID like @filterwildcard 
               and
               v_Collection_Alias.CollectionType = 2 
order by v_Collection_Alias.Name 
end</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="GetTotalPhyMachineForCollectionData">
      <Fields>
        <Field Name="TotalPhyMachines">
          <DataField>TotalPhyMachines</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
          select COUNT(*)  as TotalPhyMachines
          from fn_rbac_ClientCollectionMembers(@UserSIDs)  c
          LEFT JOIN fn_rbac_R_System(@UserSIDs)  v on v.ResourceID = c.ResourceID
          WHERE
          c.CollectionID = @collectionId
          AND c.IsClient = 1
          AND ISNULL(v.Is_Virtual_Machine0,0) = 0
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@collectionId">
            <Value>=Parameters!collectionId.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSetAdminID">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserTokenSIDs">
            <Value>=Parameters!UserTokenSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select dbo.fn_rbac_GetAdminIDsfromUserSIDs(@UserTokenSIDs) as UserSIDs</CommandText>
      </Query>
      <Fields>
        <Field Name="UserSIDs">
          <DataField>UserSIDs</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
