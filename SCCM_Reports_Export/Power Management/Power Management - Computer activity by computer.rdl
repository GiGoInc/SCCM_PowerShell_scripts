  <Description>Displays a graph showing monitor, computer and user activity for a specified computer on a specified date.</Description>
  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>default_reference</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>55b5ce5c-0d20-42f9-8366-163d9f47edf2</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@machineName">
            <Value>=Parameters!machineName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@reportDate">
            <Value>=Parameters!reportDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @machineId as int

select @machineId = ResourceID from fn_rbac_R_System(@UserSIDs) v_R_System_Alias
where v_R_System_Alias.Name0 = @machineName


EXEC SCCM_Rpt.rptspPowerManagementGetDailyReportPerMachine @reportDate = @reportDate, @machineId = @machineId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Hours">
          <DataField>Hours</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ComputerOn">
          <DataField>ComputerOn</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="MonitorOn">
          <DataField>MonitorOn</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="ComputerActive">
          <DataField>ComputerActive</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet3">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>EXEC [SCCM_Rpt].[rptspPowerManagementGetDayRange] 31, - 1, 0</CommandText>
      </Query>
      <Fields>
        <Field Name="reportDate">
          <DataField>date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@filterwildcard">
            <Value>=Parameters!ComputerFilter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>begin 
 if (@filterwildcard = '') 
    SELECT v_R_System_Valid_Alias.Netbios_Name0 as "Computer Name" FROM fn_rbac_R_System_Valid(@UserSIDs) v_R_System_Valid_Alias 
    WHERE ISNULL(v_R_System_Valid_Alias.Is_Virtual_Machine0,0) = 0
     order by v_R_System_Valid_Alias.Netbios_Name0 
 else 
    SELECT v_R_System_Valid_Alias.Netbios_Name0 as "Computer Name" FROM fn_rbac_R_System_Valid(@UserSIDs) v_R_System_Valid_Alias 
     WHERE v_R_System_Valid_Alias.Netbios_Name0 like @filterwildcard 
     AND ISNULL(v_R_System_Valid_Alias.Is_Virtual_Machine0,0) = 0
      order by v_R_System_Valid_Alias.Netbios_Name0 
end</CommandText>
      </Query>
      <Fields>
        <Field Name="CompName">
          <DataField>Computer Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSetAdminID">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserTokenSIDs">
            <Value>=Parameters!UserTokenSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select dbo.fn_rbac_GetAdminIDsfromUserSIDs(@UserTokenSIDs) as UserSIDs</CommandText>
      </Query>
      <Fields>
        <Field Name="UserSIDs">
          <DataField>UserSIDs</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
