  <Code>
    public function TimespanFormat( hours as long) as string
    Dim elapsed as timespan
    elapsed = Timespan.FromHours (hours)

    Dim dt As DateTime = DateTime.MinValue.Add(elapsed)

    Dim s As String


    s = dt.ToString("h tt")

    return s


    end function
