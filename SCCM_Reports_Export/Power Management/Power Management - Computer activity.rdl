    <DataSet Name="DataSetTotalSystemNotCapableOfSleep">
      <Fields>
        <Field Name="TotalSystemNotCapableOfSleep">
          <DataField>TotalSystemNotCapableOfSleep</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select
    count(CollectionTable.ResourceID) as TotalSystemNotCapableOfSleep 
from 
    fn_rbac_ClientCollectionMembers(@UserSIDs)  CollectionTable
    JOIN fn_rbac_GS_POWER_MANAGEMENT_CAPABILITIES(@UserSIDs)  tCapab ON CollectionTable.ResourceID = tCapab.ResourceID 
    LEFT JOIN fn_rbac_r_System_valid(@UserSIDs)  v ON v.ResourceID =  CollectionTable.ResourceID
where 
    tCapab.SystemS30 = 0 AND
    (
            @deviceType = 0 OR 
            (@deviceType = 1 AND tCapab.PreferredPMProfile0 != 2) OR 
            (@deviceType = 2 AND tCapab.PreferredPMProfile0 = 2) 
    ) AND
    CollectionTable.CollectionID = @collectionId 
    AND CollectionTable.IsClient = 1
    AND ISNULL(v.Is_Virtual_Machine0,0) = 0</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@deviceType">
            <Value>=Parameters!deviceType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@collectionId">
            <Value>=Parameters!collectionId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
