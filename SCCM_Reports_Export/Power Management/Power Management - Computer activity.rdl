    <DataSet Name="GetTotalPhyMachineForCollectionData">
      <Fields>
        <Field Name="TotalPhyMachines">
          <DataField>TotalPhyMachines</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
          select COUNT(*)  as TotalPhyMachines
          from fn_rbac_ClientCollectionMembers(@UserSIDs)  c
          LEFT JOIN fn_rbac_R_System(@UserSIDs)  v on v.ResourceID = c.ResourceID
          WHERE
          c.CollectionID = @collectionId
          AND c.IsClient = 1
          AND ISNULL(v.Is_Virtual_Machine0,0) = 0
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@collectionId">
            <Value>=Parameters!collectionId.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
