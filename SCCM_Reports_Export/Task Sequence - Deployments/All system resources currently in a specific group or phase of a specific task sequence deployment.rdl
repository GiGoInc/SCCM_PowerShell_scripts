          <DataField>LastStatusTime</DataField>
        </Field>
        <Field Name="SMS_Installed_Sites0">
          <DataField>SMS_Installed_Sites0</DataField>
        </Field>
        <Field Name="AdvertisementID">
          <DataField>AdvertisementID</DataField>
        </Field>
        <Field Name="ResourceID">
          <DataField>ResourceID</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>="SELECT  
 sys.Netbios_Name0 AS MachineName, 
 tslast.Step+1 AS TSRPT_C004, 
 tslast.ActionName AS TSRPT_C005, 
 tslast.GroupName AS TSRPT_C006, 
 stat.LastStatusMessageID, 
 stat.LastStatusMessageIDName, 
 LastStatusTime, 
 site.SMS_Installed_Sites0, 
 stat.AdvertisementID, 
 stat.ResourceID 
FROM fn_rbac_ClientAdvertisementStatus(@UserSIDs)  stat 
INNER JOIN fn_rbac_R_System(@UserSIDs)  sys ON stat.ResourceID=sys.ResourceID 
LEFT OUTER JOIN 
( 
 SELECT tse1.* FROM fn_rbac_TaskExecutionStatus(@UserSIDs)  tse1 
 INNER JOIN 
 ( 
  SELECT 
   AdvertisementID, 
   ResourceID, 
   MAX(ExecutionTime) AS Time 
  FROM fn_rbac_TaskExecutionStatus(@UserSIDs)  
  GROUP BY AdvertisementID, ResourceID 
 ) tse2 
 ON 
  tse1.AdvertisementID = tse2.AdvertisementID AND 
  tse1.ResourceID = tse2.ResourceID AND 
  tse1.ExecutionTime = tse2.Time 
) tslast 
ON 
 stat.AdvertisementID = tslast.AdvertisementID AND 
 stat.ResourceID = tslast.ResourceID 
LEFT JOIN fn_rbac_RA_System_SMSInstalledSites(@UserSIDs)  site ON stat.ResourceID=site.ResourceID 
WHERE  
 stat.LastState IN (9,12) AND 
 stat.AdvertisementID = @AdvertID AND 
 (ISNULL(tslast.GroupName, '') LIKE ISNULL(@GroupName, '') OR @GroupName = N'" &amp;SrsResources.Localization.GetString("Total_Plus_Colon", User!Language)&amp; "')"</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="AdvertID">
            <Value>=Parameters!AdvertID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="GroupName">
            <Value>=Parameters!GroupName.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
