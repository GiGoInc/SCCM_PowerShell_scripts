        </Field>
        <Field Name="AdvertisementName_abadbee0cbafc00ced">
          <DataField>AdvertisementName</DataField>
        </Field>
        <Field Name="Comment_cb0bbcc0facfa0daace">
          <DataField>Comment</DataField>
        </Field>
        <Field Name="AdvertisementIDName">
          <DataField>AdvertisementIDName</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          BEGIN
          IF (@filterwildcard = '')
          SELECT adv.AdvertisementName + ' (' + adv.AdvertisementID + ')' AS AdvertisementIDName, adv.AdvertisementID, adv.AdvertisementID, adv.AdvertisementName, adv.Comment FROM fn_rbac_Advertisement(@UserSIDs)  adv
          JOIN fn_rbac_TaskSequencePackage(@UserSIDs)  ts ON adv.PackageID=ts.PackageID
          ORDER BY adv.AdvertisementName
          ELSE
          SELECT adv.AdvertisementName + ' (' + adv.AdvertisementID + ')' AS AdvertisementIDName, adv.AdvertisementID, adv.AdvertisementID, adv.AdvertisementName, adv.Comment FROM fn_rbac_Advertisement(@UserSIDs)  adv
          JOIN fn_rbac_TaskSequencePackage(@UserSIDs)  ts ON adv.PackageID=ts.PackageID
          WHERE adv.AdvertisementID LIKE @filterwildcard
          ORDER BY adv.AdvertisementName
          END
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
