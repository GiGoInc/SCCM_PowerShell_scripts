        )
        And DATEDIFF("d", uss.LastScanTime, getdate()) &lt;= @DaysScanned
        AND rs.Obsolete0 = 0
)
Select
    Case
        When w.OS Like 'Microsoft Windows NT%Server 5.2%' Then 'Server 2003'
        When w.OS Like 'Microsoft Windows NT%Server 6.0%' Then 'Server 2008'
        When w.OS Like 'Microsoft Windows NT%Server 6.1%' Then 'Server 2008 R2'
        When w.OS Like 'Microsoft Windows NT%Server 6.2%' Then 'Server 2012'
