    <DataSet Name="Server_Required_Updates_Group">
      <Query>
        <DataSourceName>CM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DaysScanned">
            <Value>=Parameters!DaysScanned.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Category">
            <Value>=Parameters!Category.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH Required_Updates (ResourceID, Required) AS (
    Select
        ucs.resourceid,
        count(ucs.ResourceID) [Required]
    From
        v_Update_ComplianceStatus ucs
        JOIN v_UpdateInfo ui on ui.CI_ID = ucs.CI_ID
        and ucs.Status = 2
        AND ui.IsSuperseded = 0
        AND ui.DateRevised &lt; @StartDate
        and InfoURL NOT LIKE 'http%microsoft%'
        JOIN v_CICategoryInfo_All cica
        JOIN v_CategoryInfo ci on cica.CategoryInstanceID = ci.CategoryInstanceID
        AND cica.CategoryInstanceName IN (@Category) on ucs.CI_ID = cica.CI_ID
    Where
        ucs.ResourceID IN (
            Select
                rs.ResourceID
            from
                v_r_system rs
                JOIN V_FullCollectionMembership AS FCM on rs.ResourceID = FCM.ResourceID
                and FCM.CollectionID = @CollectionID
                JOIN v_UpdateScanStatus uss on uss.ResourceID = rs.ResourceID
            where
                (
                    rs.Operating_System_Name_and0 like 'Microsoft Windows NT%Server%'
                    AND rs.Operating_System_Name_and0 NOT like 'Microsoft Windows NT%Server 5%'
                )
                And datediff("d", uss.LastScanTime, getdate()) &lt;= @DaysScanned
                AND rs.Obsolete0 = 0
        )
    group by
        ucs.resourceid
)
select
    (
        select
            count(resourceid)
        from
            Required_Updates ru
        where
            ru.Required BETWEEN 1
            AND 10
    ) [1-10],
    (
        select
            count(resourceid)
        from
            Required_Updates ru
        where
            ru.Required BETWEEN 11
            AND 25
    ) [11-25],
    (
        select
            count(resourceid)
        from
            Required_Updates ru
        where
            ru.Required BETWEEN 26
            AND 50
    ) [26-50],
    (
        select
            count(resourceid)
        from
            Required_Updates ru
        where
            ru.Required &gt;= 51
    ) [51]</CommandText>
