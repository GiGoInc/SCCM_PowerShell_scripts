  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="SiteCode">
          <DataField>SiteCode</DataField>
        </Field>
        <Field Name="SiteName">
          <DataField>SiteName</DataField>
        </Field>
        <Field Name="Status">
          <DataField>Status</DataField>
        </Field>
        <Field Name="NumberOfComputers">
          <DataField>NumberOfComputers</DataField>
        </Field>
        <Field Name="UpdateSourceID">
          <DataField>UpdateSourceID</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
              select
              sass.SMS_Assigned_Sites0 as SiteCode,
              site.SiteName as SiteName,
              sn.StateName as Status,
              count(*) as NumberOfComputers,
              sus.UpdateSource_UniqueID as UpdateSourceID
              from fn_rbac_UpdateScanStatus(@UserSIDs)  uss
              join fn_rbac_RA_System_SMSAssignedSites(@UserSIDs)  sass on uss.ResourceID = sass.ResourceID
              join fn_rbac_R_System(@UserSIDs)  rsys on rsys.ResourceID = uss.ResourceID and isnull(rsys.Obsolete0,0)&lt;&gt;1
              join fn_rbac_Site(@UserSIDs)  site on sass.SMS_Assigned_Sites0 = site.SiteCode
              join fn_rbac_StateNames(@UserSIDs)  sn on sn.TopicType = 501 and
              sn.StateID = (case when (isnull(uss.LastScanState, 0)=0 and Left(isnull(rsys.Client_Version0, '4.0'), 1)&lt;'4') then 7 else isnull(uss.LastScanState, 0) end)
              join fn_rbac_SoftwareUpdateSource(@UserSIDs)  sus on uss.UpdateSource_ID = sus.UpdateSource_ID
              where (@SiteCode='' or sass.SMS_Assigned_Sites0=@SiteCode)
              and (@UpdateSourceID='' or sus.UpdateSource_UniqueID =@UpdateSourceID)
              group by sass.SMS_Assigned_Sites0, site.SiteName, sn.StateName, sus.UpdateSource_UniqueID, sus.UpdateSourceName
              order by sus.UpdateSource_UniqueID,sass.SMS_Assigned_Sites0, sn.StateName
            </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UpdateSourceID">
            <Value>=Parameters!UpdateSourceID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SiteCode">
            <Value>=Parameters!SiteCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet0">
      <Fields>
        <Field Name="UpdateSource">
          <DataField>UpdateSource</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Version">
          <DataField>Version</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SourceSite">
          <DataField>SourceSite</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UpdateSourceID">
          <DataField>UpdateSourceID</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
              select us.UpdateSourceName as UpdateSource,
              us.UpdateSourceDescription as Description,
              us.UpdateSourceVersion as Version,
              us.SourceSite as SourceSite,
              @UpdateSourceID as UpdateSourceID
              from fn_rbac_SoftwareUpdateSource(@UserSIDs)  us
              where us.UpdateSource_UniqueID = @UpdateSourceID
              or (@UpdateSourceID =''
              and (us.UpdateSourceName != 'WSUS Enterprise Server'
              or (us.UpdateSourceName  = 'WSUS Enterprise Server' and IsPublishingEnabled =1)))

              select
              sass.SMS_Assigned_Sites0 as SiteCode,
              site.SiteName as SiteName,
              sn.StateName as Status,
              count(*) as NumberOfComputers,
              sus.UpdateSource_UniqueID as UpdateSourceID
              from fn_rbac_UpdateScanStatus(@UserSIDs)  uss
              join fn_rbac_RA_System_SMSAssignedSites(@UserSIDs)  sass on uss.ResourceID = sass.ResourceID
              join fn_rbac_R_System(@UserSIDs)  rsys on rsys.ResourceID = uss.ResourceID and isnull(rsys.Obsolete0,0)&lt;&gt;1
              join fn_rbac_Site(@UserSIDs)  site on sass.SMS_Assigned_Sites0 = site.SiteCode
              join fn_rbac_StateNames(@UserSIDs)  sn on sn.TopicType = 501 and
              sn.StateID = (case when (isnull(uss.LastScanState, 0)=0 and Left(isnull(rsys.Client_Version0, '4.0'), 1)&lt;'4') then 7 else isnull(uss.LastScanState, 0) end)
              join fn_rbac_SoftwareUpdateSource(@UserSIDs)  sus on uss.UpdateSource_ID = sus.UpdateSource_ID
              where (@SiteCode='' or sass.SMS_Assigned_Sites0=@SiteCode)
              and (@UpdateSourceID='' or sus.UpdateSource_UniqueID =@UpdateSourceID)
              group by sass.SMS_Assigned_Sites0, site.SiteName, sn.StateName, sus.UpdateSource_UniqueID, sus.UpdateSourceName
              order by sus.UpdateSource_UniqueID,sass.SMS_Assigned_Sites0, sn.StateName
            </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UpdateSourceID">
            <Value>=Parameters!UpdateSourceID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SiteCode">
            <Value>=Parameters!SiteCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_DataSet_UpdateSourceID">
      <Fields>
        <Field Name="UpdateSourceID">
          <DataField>UpdateSourceID</DataField>
        </Field>
        <Field Name="UpdateSourceName">
          <DataField>UpdateSourceName</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          select sus.UpdateSource_UniqueID UpdateSourceID,
          sus.UpdateSourceName as UpdateSourceName
          from fn_rbac_SoftwareUpdateSource(@UserSIDs)  sus
          where (@filterwildcard='' or sus.UpdateSourceName like @filterwildcard or sus.UpdateSource_UniqueID like @filterwildcard)
            and sus.IsExpired=0
          order by sus.UpdateSourceName
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_DataSet_SiteCode">
      <Fields>
        <Field Name="SiteCode">
          <DataField>SiteCode</DataField>
        </Field>
        <Field Name="SiteName">
          <DataField>SiteName</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          select SiteCode, SiteName=SiteCode + ' - ' + SiteName 
          from fn_rbac_Site(@UserSIDs) 
          where (@filterwildcard='' or SiteCode like @filterwildcard or SiteName like @filterwildcard)
          order by SiteCode
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
