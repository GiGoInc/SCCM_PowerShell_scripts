  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
        </Field>
        <Field Name="StatusID">
          <DataField>StatusID</DataField>
        </Field>
        <Field Name="StatusName">
          <DataField>StatusName</DataField>
        </Field>
        <Field Name="Status">
          <Value>=SrsResources.Localization.GetDBString(Fields!StatusName.Value, User!Language)</Value>
        </Field>
        <Field Name="NumberOfComputers">
          <DataField>NumberOfComputers</DataField>
        </Field>
        <Field Name="UpdateSourceID">
          <DataField>UpdateSourceID</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          declare @SrcID int = case when @UpdateSourceID is not null then (select UpdateSource_ID from fn_rbac_SoftwareUpdateSource(@UserSIDs)  where UpdateSource_UniqueID=@UpdateSourceID) end

          select
          coll.CollectionID as CollectionID,
          coll.Name as Name,
          isnull(uss.LastScanState, 0) as StatusID,
          sn.StateName as StatusName,
          count(*) as NumberOfComputers,
          @UpdateSourceID as UpdateSourceID
          from fn_rbac_Collection(@UserSIDs)  coll
          join v_FullCollectionMembership fcm on fcm.CollectionID = coll.CollectionID and fcm.IsClient=1
          join v_R_System rsys on rsys.ResourceID = fcm.ResourceID and isnull(rsys.Obsolete0,0)=0
          join fn_rbac_UpdateScanStatus(@UserSIDs)  uss on uss.ResourceID = fcm.ResourceID and (@SrcID is null or @SrcID=uss.UpdateSource_ID)
          left join fn_rbac_StateNames(@UserSIDs)  sn on sn.StateID= uss.LastScanState and sn.TopicType=501
          where (@CollID = '' or coll.CollectionID = @CollID)
          group by coll.CollectionID, coll.Name, isnull(uss.LastScanState, 0), sn.StateName
          order by 1,3
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UpdateSourceID">
            <Value>=Parameters!UpdateSourceID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollID">
            <Value>=Parameters!CollID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet0">
      <Fields>
