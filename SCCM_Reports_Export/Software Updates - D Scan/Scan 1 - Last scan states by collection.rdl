        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          select UpdateSource_UniqueID, UpdateSourceName
          from fn_rbac_SoftwareUpdateSource(@UserSIDs) 
          where IsPublishingEnabled=1
        </CommandText>
