      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          select us.UpdateSourceName as UpdateSource,
            us.UpdateSourceDescription as Description,
            us.UpdateSourceVersion as Version,
            us.SourceSite as SourceSite
          from fn_rbac_SoftwareUpdateSource(@UserSIDs)  us
          where us.UpdateSource_UniqueID = @UpdateSourceID
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UpdateSourceID">
            <Value>=Parameters!UpdateSourceID.Value</Value>
          </QueryParameter>
        </QueryParameters>
