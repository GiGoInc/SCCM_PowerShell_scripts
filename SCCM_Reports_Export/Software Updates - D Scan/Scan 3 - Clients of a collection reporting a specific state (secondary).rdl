        </Field>
        <Field Name="LastScanTime">
          <DataField>LastScanTime</DataField>
        </Field>
        <Field Name="LastScanPackageLocation">
          <DataField>LastScanPackageLocation</DataField>
        </Field>
        <Field Name="LastScanPackageVersion">
          <DataField>LastScanPackageVersion</DataField>
        </Field>
        <Field Name="ErrorStatusName">
          <DataField>ErrorStatusName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>  if @CollID='' set @CollID='SMS00001'

          select
            rsys.Name0 as MachineName,
            rsys.Client_Version0 as SMSClientVersion,
            uss.LastWUAVersion as WUAVersion,
            rsys.User_Domain0+'\'+rsys.User_Name0 as LastLoggedOnUser,
            uss.LastStatusMessageID&amp;0x0000FFFF as ErrorStatusID,
          uss.LastErrorCode as LastErrorCode,
          sn.StateName as Status,
          uss.LastScanTime as LastScanTime,
          uss.LastScanPackageLocation as LastScanPackageLocation,
          uss.LastScanPackageVersion as LastScanPackageVersion,
          statusinfo.MessageStateName as ErrorStatusName
          from fn_rbac_UpdateScanStatus(@UserSIDs)  uss
          join fn_rbac_ClientCollectionMembers(@UserSIDs)  cm on cm.ResourceID=uss.ResourceID and cm.CollectionID=@CollID
          join v_R_System rsys on rsys.ResourceID = cm.ResourceID
          join fn_rbac_SoftwareUpdateSource(@UserSIDs)  sus on uss.UpdateSource_ID = sus.UpdateSource_ID and (@UpdateSourceID = '' or sus.UpdateSource_UniqueID = @UpdateSourceID)
          join fn_rbac_StateNames(@UserSIDs)  sn on sn.TopicType = 501 and sn.StateID = isnull(uss.LastScanState, 0) and (@StateName='' or sn.StateName=@StateName)
          left join fn_rbac_AdvertisementStatusInformation(@UserSIDs)  statusinfo on statusinfo.MessageID=nullif(uss.LastStatusMessageID&amp;0x0000FFFF, 0)
          order by rsys.Name0
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollID">
            <Value>=Parameters!CollID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StateName">
            <Value>=Parameters!StateName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UpdateSourceID">
            <Value>=Parameters!UpdateSourceID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet0">
