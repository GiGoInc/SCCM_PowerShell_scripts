    <DataSet Name="Parameter_DataSet_StateName">
      <Fields>
        <Field Name="StateID">
          <DataField>StateID</DataField>
        </Field>
        <Field Name="UnlocStateName">
          <DataField>StateName</DataField>
        </Field>
        <Field Name="StateName">
          <Value>=SrsResources.Localization.GetDBString(Fields!UnlocStateName.Value, User!Language)</Value>
        </Field>
        <Field Name="StateDisplayName">
          <Value>=Fields!StateID.Value &amp; " - "&amp; SrsResources.Localization.GetDBString(Fields!UnlocStateName.Value, User!Language)</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          select StateID, StateName
          from fn_rbac_StateNames(@UserSIDs) 
          where TopicType = 501
          order by StateID
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
