    <DataSet Name="Parameter_DataSet_UpdateSourceID">
      <Fields>
        <Field Name="UpdateSourceID">
          <DataField>UpdateSourceID</DataField>
        </Field>
        <Field Name="UpdateSourceName">
          <DataField>UpdateSourceName</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
              begin
              if (@filterwildcard = '')
              select sus.UpdateSource_UniqueID UpdateSourceID,
              sus.UpdateSourceName as UpdateSourceName
              from fn_rbac_SoftwareUpdateSource(@UserSIDs)  sus
              where sus.UpdateSourceName != 'WSUS Enterprise Server'
              or (sus.UpdateSourceName  = 'WSUS Enterprise Server' and IsPublishingEnabled =1) order by sus.UpdateSourceName
              else
              select sus.UpdateSource_UniqueID UpdateSourceID,
              sus.UpdateSourceName as UpdateSourceName
              from fn_rbac_SoftwareUpdateSource(@UserSIDs)  sus
              where (sus.UpdateSourceName != 'WSUS Enterprise Server'
              or (sus.UpdateSourceName  = 'WSUS Enterprise Server' and IsPublishingEnabled =1))
              and (sus.UpdateSourceName like @filterwildcard or sus.UpdateSource_UniqueID like @filterwildcard)
              order by sus.UpdateSourceName
              end
            </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
