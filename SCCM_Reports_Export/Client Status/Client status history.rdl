    <DataSet Name="CollectionsInfo">
      <Fields>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CollectionName">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <Value>=SrsResources.Localization.GetDBString(Fields!CollectionName.Value, User!Language)</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT
  v_Collection_Alias.CollectionID
  ,v_Collection_Alias.Name
FROM
  fn_rbac_Collection(@UserSIDs) v_Collection_Alias
WHERE
  v_Collection_Alias.CollectionType = 2</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="ClientSummaryHistory">
      <Fields>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SLACheck">
          <DataField>SLAActiveHealthyOrActiveNoResultsClients</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalClientsActiveHealthyOrActiveNoResults">
          <DataField>TotalClientsActiveHealthyOrActiveNoResults</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="TotalClientsActive">
          <DataField>TotalClientsActive</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="SLAActivity">
          <DataField>SLAActive</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>DECLARE @checkSla int 
DECLARE @activitySla int 
DECLARE @variablesXml XML 

-- 21 is the type id of check SLA alert 
SET @variablesXml = (SELECT ParameterValues 
                     FROM   fn_rbac_SMS_Alert(@UserSIDs)  
                     WHERE  TypeID = 21 
		     AND TypeInstanceID = @CollID) 

SET @checkSla = (SELECT T.X.value('.', 'int') 
                 FROM   @variablesXml.nodes('/Parameters/Parameter') AS T(X) 
                 WHERE  T.X.value('(./@index)', 'int') = 3)  

-- 22 is the type id of activity SLA alert 
SET @variablesXml = (SELECT ParameterValues 
                     FROM   fn_rbac_SMS_Alert(@UserSIDs)  
                     WHERE  TypeID = 22 
		     AND TypeInstanceID = @CollID) 

SET @activitySla = (SELECT T.X.value('.', 'int') 
                    FROM   @variablesXml.nodes('/Parameters/Parameter') AS T(X) 
                    WHERE  T.X.value('(./@index)', 'int') = 3)  

SELECT DATEADD(dd, 0, DATEDIFF(dd, 0, Date)) AS Date, 
       CollectionID, 
       ISNULL(@checkSla, -1)                       AS SLAActiveHealthyOrActiveNoResultsClients, 
       ISNULL(@activitySla, -1)                    AS SLAActive, 
       CASE 
         WHEN SUM(RealClientsTotal) != 0 THEN 
         100 * SUM(ClientsActive) / SUM(RealClientsTotal) 
         ELSE 0 
       END                                   AS TotalClientsActive, 
       CASE 
         WHEN SUM(ClientsActive) != 0 THEN 
         100 * SUM(ClientsActiveHealthyOrActiveNoResults) / SUM(ClientsActive) 
         ELSE 0 
       END                                   AS TotalClientsActiveHealthyOrActiveNoResults 
FROM   (SELECT SiteCode, 
               RealClientsTotal, 
               ClientsActive, 
               CollectionID, 
               ClientsActiveHealthyOrActiveNoResults, 
               Date 
        FROM   (SELECT SiteCode, 
                       (ClientsActive + ClientsInactive) AS RealClientsTotal, 
                       ClientsActive, 
                       CollectionID, 
                       ClientsActiveHealthyOrActiveNoResults, 
                       Date 
                FROM   fn_rbac_CH_ClientSummaryHistory(@UserSIDs)  
                WHERE  CollectionID = @CollID 
                       AND DATEADD(dd, 0, DATEDIFF(dd, 0, Date)) BETWEEN DATEADD(dd, -@Days, DATEDIFF(dd, 0, GETUTCDATE())) AND DATEADD(dd, -1, DATEDIFF(dd, 0, GETUTCDATE()))) a 
               INNER JOIN (SELECT MAX(Date) AS LatestDate 
                           FROM   fn_rbac_CH_ClientSummaryHistory(@UserSIDs)  
                           WHERE  CollectionID = @CollID 
                                  AND DATEADD(dd, 0, DATEDIFF(dd, 0, Date)) BETWEEN DATEADD(dd, -@Days, DATEDIFF(dd, 0, GETUTCDATE())) AND DATEADD(dd, -1, DATEDIFF(dd, 0, GETUTCDATE()))
                           GROUP  BY DATEADD(dd, 0, DATEDIFF(dd, 0, Date)), 
                                     SiteCode) DayLatestBySite 
                 ON DayLatestBySite.LatestDate = a.Date) DaySummaryAll 
GROUP  BY DATEADD(dd, 0, DATEDIFF(dd, 0, Date)), 
          CollectionID </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Days">
            <Value>=Parameters!Days.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@CollID">
            <Value>=Parameters!CollID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
