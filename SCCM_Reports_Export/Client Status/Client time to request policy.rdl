          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
SET NOCOUNT ON 

DECLARE @total INT -- total active clients   
DECLARE @max DATETIME -- end date   
DECLARE @min DATETIME -- begin date   
DECLARE @cur DATETIME -- current date   
DECLARE @curDay INT -- current day in evaluation   
DECLARE @clients INT -- count of clients for current date   
DECLARE @dayRange INT -- day range for the report

DECLARE @PolicyRequests TABLE ( 
  CollectionID NVARCHAR(20), 
  Day          INT, 
  Clients      INT, 
  Percentage   FLOAT, 
  Date         DATETIME) 

SELECT @total = COUNT(*) 
FROM fn_rbac_FullCollectionMembership(@UserSIDs)  cm
     INNER JOIN fn_rbac_CH_ClientSummary(@UserSIDs)  cs ON cs.ResourceID = cm.ResourceID AND cm.ResourceType = 5
WHERE cm.CollectionID = @CollID

IF @total = 0
    RETURN

SELECT @max = MAX(RequestTime) 
FROM   fn_rbac_FullCollectionMembership(@UserSIDs)  cm 
       LEFT OUTER JOIN fn_rbac_CH_PolicyRequestHistory(@UserSIDs)  pq ON pq.ResourceID = cm.ResourceID 
WHERE cm.CollectionID = @CollID

SELECT @min = MIN(RequestTime) 
FROM   fn_rbac_FullCollectionMembership(@UserSIDs)  cm 
       LEFT OUTER JOIN fn_rbac_CH_PolicyRequestHistory(@UserSIDs)  pq ON pq.ResourceID = cm.ResourceID 
WHERE cm.CollectionID = @CollID

SET @dayRange = DATEDIFF(dd, @min, @max)

IF @dayRange IS NULL
    RETURN
    
IF @dayRange &gt;= 30
    SET @dayRange = 29;

SET @cur = @max -- @cur is actually our end date for evaluation (last current request)   
SET @curDay = @dayRange + 1

WHILE @curDay &gt; 0
BEGIN 
    SELECT @clients = COUNT(DISTINCT pq.ResourceID) 
    FROM   fn_rbac_FullCollectionMembership(@UserSIDs)  cm 
           LEFT OUTER JOIN fn_rbac_CH_PolicyRequestHistory(@UserSIDs)  pq ON pq.ResourceID = cm.ResourceID
    WHERE cm.CollectionID = @CollID AND RequestTime &lt;= @cur 

    INSERT @PolicyRequests (CollectionID, Day, Clients, Percentage, Date) 
    VALUES (@CollID, @curDay, @clients, ROUND(100.00 * @clients / @total, 1), @cur ) 

    SET @cur = DATEADD(dd, -1, @cur) 
    SET @curDay = @curDay - 1
END 

SELECT CollectionID, Day, Clients, Percentage, Date 
FROM   @PolicyRequests 
ORDER  BY Day </CommandText>
        <QueryParameters>
