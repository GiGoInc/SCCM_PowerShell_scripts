          <DataField>InstallStatus</DataField>
        </Field>
        <Field Name="PackageID">
          <DataField>PackageID</DataField>
        </Field>
        <Field Name="LocInstallStatus">
          <Value>=SrsResources.Localization.GetDBString(Fields!InstallStatus.Value, User!Language, "SMS_DPStatus-StateName")</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select 
SUBSTRING(dp.ServerNALPath, CHARINDEX('\\', dp.ServerNALPath) + 2, CHARINDEX('"]', dp.ServerNALPath) - CHARINDEX('\\', dp.ServerNALPath) - 3 ) AS C070,  
dp.SiteCode, 
stat.SourceVersion, 
pstat.UpdateTime, 
stat.InstallStatus, 
dp.PackageID
from fn_rbac_DistributionPoint(@UserSIDs)  dp 
left join fn_rbac_PackageStatusDistPointsSumm(@UserSIDs)  stat on dp.ServerNALPath=stat.ServerNALPath 
                                             and dp.PackageID=stat.PackageID 
left join fn_rbac_PackageStatus(@UserSIDs)  pstat on dp.ServerNALPath=pstat.PkgServer  
                                  and dp.PackageID=pstat.PackageID 
where dp.PackageID=@PkgID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="PkgID">
            <Value>=Parameters!PkgID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="PackageID">
          <DataField>PackageID</DataField>
        </Field>
        <Field Name="PackageName">
          <DataField>PackageName</DataField>
        </Field>
        <Field Name="PackageID_abede0eeeaa0e">
          <DataField>PackageID</DataField>
        </Field>
        <Field Name="LocalizedPackageName">
          <Value>=SrsResources.Localization.GetDBString(Fields!PackageName.Value, User!Language)</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>begin 
 if (@filterwildcard = '') 
  SELECT DISTINCT PackageID, Name AS PackageName FROM fn_rbac_Package2(@UserSIDs)  order by Name 
 else 
  SELECT DISTINCT PackageID, Name AS PackageName FROM fn_rbac_Package2(@UserSIDs)  WHERE PackageID like @filterwildcard 
  order by Name 
end</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSetAdminID">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserTokenSIDs">
            <Value>=Parameters!UserTokenSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select dbo.fn_rbac_GetAdminIDsfromUserSIDs(@UserTokenSIDs) as UserSIDs</CommandText>
      </Query>
      <Fields>
        <Field Name="UserSIDs">
          <DataField>UserSIDs</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
