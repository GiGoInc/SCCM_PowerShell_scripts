<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesignerdrill" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition">
  <Description>Displays the number of peer cache source rejections per boundary group.</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>default_reference</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>cfca05f1-e074-4f44-a821-7898a6fedf25</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSetAdminID">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserTokenSIDs">
            <Value>=Parameters!UserTokenSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select dbo.fn_rbac_GetAdminIDsfromUserSIDs(@UserTokenSIDs) as UserSIDs</CommandText>
      </Query>
      <Fields>
        <Field Name="UserSIDs">
          <DataField>UserSIDs</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
WITH RejectionType AS (
	SELECT 1 AS Type, 'MaxDiskIO' AS RejectionName
	UNION ALL 
	SELECT 2 AS Type, 'MaxCPULoad' AS RejectionName
	UNION ALL 
	SELECT 8 AS Type, 'ConnectionServerOS' AS RejectionName
	UNION ALL 
	SELECT 16 AS Type, 'ConnectionClientOS' AS RejectionName
)
          SELECT PSBG.BoundaryGroupName as 'BoundaryGroupName',
          RT.RejectionName AS Type_of_Rejection,
          sum (PCRD.RejectionCount) as TotalPeerCacheSourceRejections
          FROM v_PeerSourceRejectionData AS PCRD
          JOIN v_PeerSourceBoundaryGroup AS PSBG
          ON PSBG.ResourceID = PCRD.ResourceID
          JOIN RejectionType AS RT
          ON RT.Type = PCRD.RejectionType
          JOIN fn_rbac_ClientMachines(@UserSIDs) cm on           cm.ResourceID=PSBG.ResourceID
	  where PCRD.ReportTime &gt;= @StartDate
            AND DATEADD(day,-1,PCRD.ReportTime) &lt;= @EndDate
          GROUP BY PSBG.BoundaryGroupName, RT.RejectionName
          ORDER BY PSBG.BoundaryGroupName, RT.RejectionName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="BoundaryGroupName">
          <DataField>BoundaryGroupName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Type_of_Rejection">
          <DataField>Type_of_Rejection</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalPeerCacheSourceRejections">
          <DataField>TotalPeerCacheSourceRejections</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
