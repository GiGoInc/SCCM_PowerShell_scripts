<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition">
  <Description>Displays the peer cache sources that rejected to serve content based on a condition.</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>default_reference</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>41e69b2c-1abc-46f3-b8fc-8493b82ee4b7</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSetAdminID">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserTokenSIDs">
            <Value>=Parameters!UserTokenSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select dbo.fn_rbac_GetAdminIDsfromUserSIDs(@UserTokenSIDs) as UserSIDs</CommandText>
      </Query>
      <Fields>
        <Field Name="UserSIDs">
          <DataField>UserSIDs</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RejectionType">
            <Value>=Parameters!RejectionType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BoundaryGroupName">
            <Value>=Parameters!BoundaryGroupName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH RejectionType AS (
	SELECT 1 AS Type, 'MaxDiskIO' AS RejectionName
	UNION ALL 
	SELECT 2 AS Type, 'MaxCPULoad' AS RejectionName
	UNION ALL 
	SELECT 8 AS Type, 'ConnectionServerOS' AS RejectionName
	UNION ALL 
	SELECT 16 AS Type, 'ConnectionClientOS' AS RejectionName
)
SELECT PCRD.ResourceID,
PCRD.ResourceName as PeerCacheSource, 
	sum(PCRD.RejectionCount) AS Total
FROM v_PeerSourceRejectionData AS PCRD
	JOIN v_PeerSourceBoundaryGroup AS PSBG
		ON PSBG.ResourceID = PCRD.ResourceID
	LEFT JOIN RejectionType AS RT
		ON RT.Type = PCRD.RejectionType
    JOIN fn_rbac_ClientMachines(@UserSIDs) cm on cm.ResourceID=PSBG.ResourceID
WHERE PSBG.BoundaryGroupName = @BoundaryGroupName
AND ISNULL(RT.RejectionName, 'Other/Unknown') = @RejectionType
GROUP BY PCRD.ResourceID, PCRD.ResourceName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="PeerCacheSource">
          <DataField>PeerCacheSource</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ResourceID">
          <DataField>ResourceID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
