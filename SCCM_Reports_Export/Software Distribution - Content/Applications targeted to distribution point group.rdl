  <DataSets>
    <DataSet Name="DPGroupApps">
      <Fields>
        <Field Name="ApplicationName">
          <DataField>ApplicationName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CI_ID">
          <DataField>CI_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select loc.DisplayName as ApplicationName,
loc.CI_ID
from 
fn_rbac_ListApplicationCIs(1033, @UserSIDs) loc
inner join fn_rbac_CIContentPackage(@UserSIDs)  cp on loc.CI_ID=cp.CI_ID
inner join fn_rbac_DPGroupPackages(@UserSIDs)  dpgp on dpgp.PkgID=cp.PkgID
where dpgp.GroupID = @GID and loc.IsExpired != 1</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="GID">
            <Value>=Parameters!GID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DPGs">
      <Fields>
        <Field Name="GroupID">
          <DataField>GroupID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
