  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="TotalBytes">
          <DataField>TotalBytes</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Rgroup">
          <DataField>Rgroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TargetSite">
          <DataField>TargetSite</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SourceSite">
          <DataField>SourceSite</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>WITH T(SyncDataSize, Rgroup, SourceSite, TargetSite)
        AS
        (
	select hst.SyncDataSize, 
	CASE 
		WHEN 
		vrd.ReplicationGroup LIKE 'Hardware[_]Inventory%' THEN 'Hardware Inventory'
		ELSE 
		CASE 
		    WHEN 
		    vrd.ReplicationPattern = 'global' OR vrd.ReplicationPattern = 'global_proxy' THEN 'Global Data'
		    ELSE 
		    CASE 
		        WHEN 
		        vrd.ReplicationGroup = 'Software_Inventory_and_Metering' THEN 'Software Inventory/Metering'
		        ELSE 
		        CASE 
		            WHEN 
		            vrd.ReplicationGroup = 'Status_Messages' THEN 'Status Messages'
		            ELSE 'Other Site Data'
    	        END 
    	    END 
    	END
    END AS Rgroup,
    hst.SourceSite, 
    hst.TargetSite
from fn_rbac_DrsSendHistorySummary(@UserSIDs)  hst join fn_rbac_replicationdata(@UserSIDs)  as vrd
  on hst.ReplicationGroupID = vrd.ID
  where hst.SummarizationTime &gt; dateadd(day,-CONVERT(INT, STR(@Days)),GETUTCDATE()) AND
 hst.TargetSite = @TargetSite AND 
 hst.SourceSite = @SourceSite)
select sum(SyncDataSize) AS TotalBytes, Rgroup, SourceSite, TargetSite from T
group by SourceSite, TargetSite, Rgroup
order by    Rgroup</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Days">
            <Value>=Parameters!Days.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TargetSite">
            <Value>=Parameters!TargetSite.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SourceSite">
            <Value>=Parameters!SourceSite.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
