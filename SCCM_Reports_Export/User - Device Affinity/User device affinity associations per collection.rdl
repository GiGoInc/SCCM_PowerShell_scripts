  <rdl:Language xmlns:rdl="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition">=User!Language</rdl:Language>
  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>default_reference</DataSourceReference>
      <rd:DataSourceID>d373c8dd-5069-4bfe-a82d-ca9b4965ab5e</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="UniqueUserName">
          <DataField>UniqueUserName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UDRCount">
          <DataField>UDRCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MachineResourceName">
          <DataField>MachineResourceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CreationTime">
          <DataField>CreationTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CreatedBy">
          <DataField>CreatedBy</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AggregateID">
          <DataField>AggregateID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select distinct
  temp.UDRCount,
  umr.MachineResourceName,
  umr.UniqueUserName,
  umr.CreationTime,
  temp3.AggregateID,
  case when temp3.AggregateID &amp; 2 = 2 THEN umsr.CreatedBy ELSE '' END as CreatedBy
from
(
  select 
    umr.UniqueUserName as ResourceName,
    COUNT(umr.MachineResourceName) as UDRCount
  from fn_rbac_UserMachineRelationship(@UserSIDs)  umr
  join v_UserMachineTypeRelation umtr on umr.RelationshipResourceID = umtr.RelationshipResourceID
  where umr.RelationActive = 1
  group by umr.UniqueUserName
) temp
join fn_rbac_UserMachineRelationship(@UserSIDs)  umr on umr.UniqueUserName = temp.ResourceName and umr.RelationActive = 1 
join v_UserMachineTypeRelation  umtr on umr.RelationshipResourceID = umtr.RelationshipResourceID
join v_FullCollectionMembership  fcm on umr.UniqueUserName = fcm.SMSID and fcm.CollectionID = @Collection
join v_Collection  co on co.CollectionID = fcm.CollectionID
join v_UserMachineSourceRelation  umsr on umsr.RelationshipResourceID = umr.RelationshipResourceID
join
(
   select
    SUM(temp2.AggregateID) as AggregateID,
    temp2.RelationshipResourceID
  from
  (
    select
    CASE umsr2.SourceID 
      WHEN 1 THEN 1
      WHEN 2 THEN 2
      WHEN 3 THEN 4
      WHEN 4 THEN 8
      WHEN 5 THEN 16
    END as AggregateID,
    umsr2.RelationshipResourceID as RelationshipResourceID
  from fn_rbac_UserMachineSourceRelation(@UserSIDs)  umsr2
  ) temp2
  group by temp2.RelationshipResourceID
) temp3 on temp3.RelationshipResourceID = umsr.RelationshipResourceID
where
  (temp3.AggregateID &amp; 2 = 0 or (temp3.AggregateID &amp; 2 = 2 and CreatedBy != '')) and
  ((@Operator = 0 and temp.UDRCount = @RelationshipCount) or
  (@Operator = 1 and temp.UDRCount &lt;&gt; @RelationshipCount) or
  (@Operator = 2 and temp.UDRCount &gt; @RelationshipCount) or
  (@Operator = 3 and temp.UDRCount &lt; @RelationshipCount) or
  (@Operator = 4 and temp.UDRCount &gt;= @RelationshipCount) or
  (@Operator = 5 and temp.UDRCount &lt;= @RelationshipCount)) and
  @CollectionType = 1

------------------------------------------------------------------------------------------------------------------------------
union all
------------------------------------------------------------------------------------------------------------------------------

select distinct
  temp.UDRCount,
  umr.MachineResourceName,
  umr.UniqueUserName,
  umr.CreationTime,
  temp3.AggregateID,
  case when temp3.AggregateID &amp; 2 = 2 THEN umsr.CreatedBy ELSE '' END as CreatedBy
from
(
  select
    umr.MachineResourceName ResourceName,
    COUNT(umr.UniqueUserName) as UDRCount
  from fn_rbac_UserMachineRelationship(@UserSIDs)  umr
  join v_UserMachineTypeRelation  umtr on umr.RelationshipResourceID = umtr.RelationshipResourceID
  where umr.RelationActive = 1
  group by umr.MachineResourceName
) temp
join v_UserMachineRelationship  umr on umr.MachineResourceName = temp.ResourceName and umr.RelationActive = 1
join v_UserMachineTypeRelation  umtr on umr.RelationshipResourceID = umtr.RelationshipResourceID
join v_FullCollectionMembership  fcm on umr.MachineResourceID = fcm.ResourceID and fcm.CollectionID = @Collection
join v_Collection  co on co.CollectionID = fcm.CollectionID
join v_UserMachineSourceRelation  umsr on umsr.RelationshipResourceID = umr.RelationshipResourceID
join
(
   select
    SUM(temp2.AggregateID) as AggregateID,
    temp2.RelationshipResourceID
  from
  (
    select
    CASE umsr2.SourceID 
      WHEN 1 THEN 1
      WHEN 2 THEN 2
      WHEN 3 THEN 4
      WHEN 4 THEN 8
      WHEN 5 THEN 16
    END as AggregateID,
    umsr2.RelationshipResourceID as RelationshipResourceID
  from fn_rbac_UserMachineSourceRelation(@UserSIDs)  umsr2
  ) temp2
  group by temp2.RelationshipResourceID
) temp3 on temp3.RelationshipResourceID = umsr.RelationshipResourceID
where
  (temp3.AggregateID &amp; 2 = 0 or (temp3.AggregateID &amp; 2 = 2 and CreatedBy != '')) and
  ((@Operator = 0 and temp.UDRCount = @RelationshipCount) or
  (@Operator = 1 and temp.UDRCount &lt;&gt; @RelationshipCount) or
  (@Operator = 2 and temp.UDRCount &gt; @RelationshipCount) or
  (@Operator = 3 and temp.UDRCount &lt; @RelationshipCount) or
  (@Operator = 4 and temp.UDRCount &gt;= @RelationshipCount) or
  (@Operator = 5 and temp.UDRCount &lt;= @RelationshipCount)) and
  @CollectionType = 2
  
------------------------------------------------------------------------------------------------------------------------------
union all
------------------------------------------------------------------------------------------------------------------------------

-- Get users in the selected collection that have no UDA relationships
select distinct 
  0 as UDRCount,
  NULL as MachineResourceName,
  temp.SMSID as UniqueUserName,
  '' as CreationTime,
  0 as AggregateID,
  NULL as CreatedBy
from 
(
select fcm.SMSID, fcm.CollectionID, MAX(umtr.CreationTime) as CreationTime from fn_rbac_FullCollectionMembership(@UserSIDs)  fcm
left join v_UserMachineRelation  umr on fcm.SMSID = umr.UniqueUserName and umr.RelationActive = 1
left join fn_rbac_UserMachineTypeRelation(@UserSIDs)  umtr on umtr.RelationshipResourceID = umr.RelationshipResourceID
where fcm.ResourceType = 4 
group by SMSID, CollectionID  
) temp
where temp.CollectionID = @Collection and temp.CreationTime IS NULL and
  ((@Operator = 0 and @RelationshipCount = 0) or
  (@Operator = 1 and @RelationshipCount &lt;&gt; 0) or
  (@Operator = 3 and @RelationshipCount &gt; 0) or
  (@Operator = 4 and @RelationshipCount = 0) or
  (@Operator = 5 and @RelationshipCount &gt;= 0))
 
------------------------------------------------------------------------------------------------------------------------------
union all
------------------------------------------------------------------------------------------------------------------------------

-- Get machines in the selected collection that have no UDA relationships
select distinct
  0 as UDRCount,
  temp.Name as MachineResourceName,
  NULL as UniqueUserName,
  '' as CreationTime,
  0 as AggregateID,
  NULL as CreatedBy
from 
(
select fcm.Name, fcm.CollectionID, MAX(umtr.CreationTime) as CreationTime from fn_rbac_FullCollectionMembership(@UserSIDs)  fcm
left join v_UserMachineRelationship  umr on fcm.Name = umr.MachineResourceName and umr.RelationActive = 1
left join v_UserMachineTypeRelation  umtr on umtr.RelationshipResourceID = umr.RelationshipResourceID
where fcm.ResourceType = 5
group by Name, CollectionID  
) temp
where temp.CollectionID = @Collection and temp.CreationTime IS NULL and
  ((@Operator = 0 and @RelationshipCount = 0) or
  (@Operator = 1 and @RelationshipCount &lt;&gt; 0) or
  (@Operator = 3 and @RelationshipCount &gt; 0) or
  (@Operator = 4 and @RelationshipCount = 0) or
  (@Operator = 5 and @RelationshipCount &gt;= 0))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Collection">
            <Value>=Parameters!Collection.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollectionType">
            <Value>=Parameters!CollectionType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Operator">
            <Value>=Parameters!Operator.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RelationshipCount">
            <Value>=Parameters!RelationshipCount.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
