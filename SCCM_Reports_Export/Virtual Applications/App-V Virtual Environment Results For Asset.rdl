<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <rdl:Language xmlns:rdl="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition">=User!Language</rdl:Language>
  <Description>This report shows summary information for all software deployments</Description>
  <DataSources>
    <DataSource Name="ID5C6358F24BB64a1bA16E8D96795D8602">
      <DataSourceReference>default_reference</DataSourceReference>
      <rd:DataSourceID>feaac5ad-a180-4202-bea2-e69bf51b73fb</rd:DataSourceID>
      <rd:SecurityType>None</rd:SecurityType>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="VEStateID">
          <DataField>VEStateID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ErrorCode">
          <DataField>ErrorCode</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="HexErrorCode">
          <DataField>HexErrorCode</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>
	declare @VEStateID as int
	declare @ComplianceStatusID as bigint
	declare @ErrorCode as bigint
	
	declare @UserIDs  table (UserID int)
	insert into @UserIDs(UserID) select users.UserID from fn_rbac_Users(@UserSIDs)  users where users.FullName = @UserName
	
	if (not exists (select 1 from @UserIDs))
	begin
		set @VEStateID = 6 -- Unknown because no data was ever reported for that user	
	end
	else
	begin
		Select @ComplianceStatusID = ccs.CI_CurrentComplianceStatusID from fn_rbac_CICurrentComplianceStatus(@UserSIDs)  ccs
		where ccs.ResourceID = @ResourceID
		and ccs.UserID in (select UserID from @UserIDs)
		and ccs.CI_ID = @VECIID
		
		if @ComplianceStatusID is NULL
		begin
			set @VEStateID = 6 -- Unknown because no compliance data was reported for that user	
		end
		else
		begin
			select @VEStateID = case when ccs.ComplianceState = 3 or ccs.EnforcementState between 5000 and 5010 then 5 -- Error
							         when ccs.EnforcementState in (2019, 2020) then 4 -- In Progress
									 when ccs.ComplianceState = 1 and ccs.IsApplicable = 1 then 3 -- Installed
									 when ccs.ComplianceState = 1 and ccs.IsApplicable = 0 then 2 -- Retained
									 when ccs.ComplianceState &lt;&gt; 1 and ccs.IsApplicable = 0 then 1 -- Not Applicable
									 end,
				@ErrorCode = ced.ErrorCode
			from fn_rbac_CICurrentComplianceStatus(@UserSIDs)  ccs 
			left join fn_rbac_CI_CurrentErrorDetails(@UserSIDs)  ced
			on ccs.CI_CurrentComplianceStatusID = ced.CurrentComplianceStatusID
			where ccs.CI_CurrentComplianceStatusID = @ComplianceStatusID
		end
	end
	
	Select @VEStateID as VEStateID, @ErrorCode as ErrorCode, master.dbo.fn_varbintohexstr(CONVERT(VARBINARY(8),CONVERT(int,@ErrorCode))) as HexErrorCode

	</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@VECIID">
            <Value>=Parameters!VirtualEnvironment.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserName">
            <Value>=Parameters!UserName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ResourceID">
            <Value>=Parameters!computer.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet_VirtualEnvironment">
      <Fields>
        <Field Name="VECIID">
          <DataField>VECIID</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="AppCIID">
          <DataField>AppCIID</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>
			Select distinct ve.DisplayName, ve.CI_ID as VECIID
			from fn_rbac_CIRelation(@UserSIDs)  cirAppToDT  
			join fn_rbac_CIRelation(@UserSIDs)  cirVEToDT on cirVEToDT.ToCIID = cirAppToDT.ToCIID
			join fn_rbac_AssignmentTargetedCIs(@UserSIDs)  cit on cit.CI_ID = cirAppToDT.FromCIID
			join fn_rbac_ListVirtualEnvironments(dbo.fn_LShortNameToLCID(@locale), @UserSIDs) ve on ve.CI_ID = cirVEToDT.FromCIID
			where cirAppToDT.RelationType = 9 and cirVEToDT.RelationType = 20
			order by ve.DisplayName
		</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet_User">
      <Fields>
        <Field Name="UserName">
          <DataField>UserName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>
		Select disc_users.Unique_User_Name0 as UserName from fn_rbac_R_User(@UserSIDs)  disc_users 
		UNION
		Select users.FullName as UserName from fn_rbac_Users(@UserSIDs)  users
		where UserName like @filterwildcard or @filterwildcard = '' or @filterwildcard is NULL
		order by UserName
		</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value>=Parameters!UserFilter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet_Machine">
      <Fields>
        <Field Name="MachineDisplay">
          <DataField>MachineDisplay</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ResourceID">
          <DataField>ResourceID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>
		select distinct SYS.Netbios_Name0 as MachineDisplay, SYS.ResourceID as ResourceID from fn_rbac_R_System_Valid(@UserSIDs)  sys 
		where sys.Netbios_Name0 like @filterwildcard or @filterwildcard = '' or @filterwildcard is NULL
		order by MachineDisplay
		</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value>=Parameters!DeviceFilter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet2">
      <Fields>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>Select distinct dtci.DisplayName from fn_rbac_CIRelation(@UserSIDs)  cir
	inner join fn_rbac_ConfigurationItems(@UserSIDs)  veci on veci.CI_ID = cir.FromCIID
	inner join fn_rbac_ListDeploymentTypeCIs(dbo.fn_LShortNameToLCID(@locale), @UserSIDs) dtci on dtci.CI_ID = cir.ToCIID
	where cir.RelationType = 20
	and veci.CI_ID =  @VECIID
	order by dtci.DisplayName</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@VECIID">
            <Value>=Parameters!VirtualEnvironment.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
