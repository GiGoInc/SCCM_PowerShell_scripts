          begin
          select * from
          (
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as FullDisplayName, '' as FullName
              UNION ALL
              select distinct usrs.FullName as FullDisplayName, usrs.FullName from fn_rbac_Users(@UserSIDs)  usrs
          ) temp
          end
          else
          begin
          select * from
          (
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as FullDisplayName, '' as FullName
              UNION ALL
              select distinct usrs.FullName as FullDisplayName, usrs.FullName from fn_rbac_Users(@UserSIDs)  usrs where usrs.FullName like @filterwildcard
          ) temp
          end
          "
        </CommandText>
        <QueryParameters>
