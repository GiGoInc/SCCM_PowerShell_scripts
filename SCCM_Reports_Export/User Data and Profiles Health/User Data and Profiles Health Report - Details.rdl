<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <rdl:Language xmlns:rdl="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition">=User!Language</rdl:Language>
  <Description>This report shows the details of either error or warning for folder redirection or roaming user profile when drilled down into the count from summary report.</Description>
  <DataSources>
    <DataSource Name="AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_">
      <DataSourceReference>default_reference</DataSourceReference>
      <rd:DataSourceID>ef53a39e-1505-4cc3-8c42-b067e2aa3672</rd:DataSourceID>
      <rd:SecurityType>None</rd:SecurityType>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet0">
      <Fields>
        <Field Name="UserName">
          <DataField>UserName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="DeviceName">
          <DataField>DeviceName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="HealthState">
          <DataField>HealthState</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="HealthStateID">
          <DataField>HealthStateID</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          declare @UserSID    as nvarchar(max)
          if(@UserName is NULL OR @UserName= '')     set @UserSID = ''
          else                                       set @UserSID = (select top 1 SID0 from fn_rbac_R_User(@UserSIDs) where Unique_User_Name0 = @UserName)

          declare @MachineID as int
          if(@Machine is NULL OR @Machine= '')       set @MachineID = ''
          else                                       set @MachineID = (select ResourceID from v_R_System_Valid  where Netbios_Name0 = @Machine)

          if (@FeatureTypeID = 0)
          begin
          select users.Unique_User_Name0 as UserName, machines.Netbios_Name0 as DeviceName,case when st.HealthState = 1 then 'Warning' else 'Error' end as HealthState, st.HealthState as HealthStateID
          from v_UserHealthProfileState st
          join fn_rbac_R_User(@UserSIDs) users on users.SID0=st.UserSID
          join v_R_System_Valid  machines on machines.ResourceID = st.ItemKey
          where (users.SID0 = @UserSID or @UserSID = '') and (machines.ResourceID = @MachineID or @MachineID='') and st.HealthState = @HealthStateID
          GROUP BY users.Unique_User_Name0, machines.Netbios_Name0, st.HealthState
          end
          else
          begin
          select users.Unique_User_Name0 as UserName, machines.Netbios_Name0 as DeviceName,case when st.HealthState = 1 then 'Warning' else 'Error' end as HealthState, st.HealthState as HealthStateID
          from v_FolderRedirectionState st
          join fn_rbac_R_User(@UserSIDs) users on users.SID0=st.UserSID
          join v_R_System_Valid machines on machines.ResourceID = st.ItemKey
          where (users.SID0 = @UserSID or @UserSID = '') and (machines.ResourceID = @MachineID or @MachineID='') and st.HealthState = @HealthStateID
          GROUP BY users.Unique_User_Name0, machines.Netbios_Name0, st.HealthState
          end

        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserName">
            <Value>=Parameters!UserName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Machine">
            <Value>=Parameters!computer.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@FeatureTypeID">
            <Value>=Parameters!FeatureType.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@HealthStateID">
            <Value>=Parameters!HealthState.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="MachineDisplay">
          <DataField>MachineDisplay</DataField>
        </Field>
        <Field Name="Machine">
          <DataField>Machine</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          ="
          if (@filterwildcard = '')
          select * from
          (
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as MachineDisplay, '' as Machine
              UNION ALL
              select distinct SYS.Netbios_Name0 as MachineDisplay, SYS.Netbios_Name0 as Machine from  v_FullCollectionMembership_Valid  cm
              inner join v_R_System_Valid  sys on sys.ResourceID=cm.ResourceID
          ) temp
          order by MachineDisplay
          else
          select * from
          (
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as MachineDisplay, '' as Machine
              UNION ALL
              select distinct SYS.Netbios_Name0 as MachineDisplay, SYS.Netbios_Name0 as Machine from  v_FullCollectionMembership_Valid  cm
              inner join v_R_System_Valid  sys on sys.ResourceID=cm.ResourceID
              where SYS.Netbios_Name0 like @filterwildcard
          ) temp
          order by MachineDisplay
          "
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value>=Parameters!DeviceFilter.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
