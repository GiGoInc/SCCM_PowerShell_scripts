  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SCCM</DataSourceName>
        <CommandText>Select Manufacturer, Model, Count(ResourceID) as DeviceCount from (
Select cs.ResourceID, cs.SystemType0 as Architecture, RTRIM(LTRIM(cs.Manufacturer0)) as Manufacturer, 
RTRIM(LTRIM((Case when cs.Manufacturer0 like '%Lenovo%' or cs.Manufacturer0 like '%IBM%' 
then csp.Version0 else cs.Model0 end))) as Model
from v_GS_COMPUTER_SYSTEM cs
left join v_GS_COMPUTER_SYSTEM_PRODUCT csp on cs.ResourceID = csp.REsourceID
) ModelCount
group by Manufacturer, Model
order by DeviceCount DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="Manufacturer">
          <DataField>Manufacturer</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Model">
          <DataField>Model</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DeviceCount">
          <DataField>DeviceCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
