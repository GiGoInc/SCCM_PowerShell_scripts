from vCI_ConfigurationItems ci
where IsLatest = 1 and CIType_ID in (3) and CI_ID in (Select distinct CI_ID from v_CICurrentSettingsComplianceStatusDetail) and CI_ID = @CIID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
