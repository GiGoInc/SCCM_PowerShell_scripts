        <CommandText>;WITH XMLNAMESPACES ('http://schemas.microsoft.com/SystemsCenterConfigurationManager/2009/06/14/Rules' as r, 'http://schemas.microsoft.com/SystemsCenterConfigurationManager/2009/07/10/DesiredConfiguration' as dc)
