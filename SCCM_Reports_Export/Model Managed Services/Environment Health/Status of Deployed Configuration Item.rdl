  <DataSets>
    <DataSet Name="GetCIList">
      <Query>
        <DataSourceName>SCCM</DataSourceName>
        <CommandText> ;WITH XMLNAMESPACES ('http://schemas.microsoft.com/SystemsCenterConfigurationManager/2009/06/14/Rules' as r, 'http://schemas.microsoft.com/SystemsCenterConfigurationManager/2009/07/10/DesiredConfiguration' as dc)
Select CI_ID,
(Case when CIType_ID = 3 then 'ConfigurationItem' else 'Other' end) as Type,
SDMPackageDigest.value('(/dc:DesiredConfigurationDigest/dc:OperatingSystem/r:Annotation/r:DisplayName/@Text)[1]', 'varchar(max)') [Name]
from vCI_ConfigurationItems ci
where IsLatest = 1 and CIType_ID in (3) and CI_ID in (Select distinct CI_ID from v_CICurrentSettingsComplianceStatusDetail)
order by Name</CommandText>
      </Query>
      <Fields>
        <Field Name="CI_ID">
          <DataField>CI_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Type">
          <DataField>Type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="GetDeploymentDetails">
      <Query>
        <DataSourceName>SCCM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CIID">
            <Value>=Parameters!CIID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select s.Name0, data.*, 
(Case when curv.CurrentValue is not null then cast(curv.CurrentValue as varchar(max))
when curv.CurrentValue is null and Status = 'Non-Compliant' then 'Setting Instance Not Found'
when curv.CurrentValue is null and Status = 'Compliant' then (Select distinct Criteria   
from v_CIComplianceStatusDetail where (CI_ID = @CIID))
else Null end) as CurrentValue

 from (
Select REsourceID,
(Case when IsError = 1 then 'Error'
when IsDetected = 1 and IsCompliant = 0 then 'Non-Compliant'
when IsDetected = 1 and IsCompliant = 1 then 'Compliant'
when IsDetected = 0 then 'Not Detected' 

else 'Unknown' end) as Status

from v_CIComplianceHistory where (CI_ID = @CIID)
Union

Select ResourceID, 'Unknown' as Status from vDCMDeploymentUnknownResources where BL_ID in (Select FromCIID from v_CIRelation where (ToCIID = @CIID) and RelationType = 1)
) data 
left join v_R_System s on data.ResourceID = s.ResourceID
left join (
Select ResourceID, CurrentValue   
from v_CICurrentSettingsComplianceStatusDetail where (CI_ID = @CIID)
and CurrentValue &lt;&gt; 'NULL'
) curv on data.ResourceID = curv.ResourceID
where Status like @Status + '%'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="REsourceID">
          <DataField>REsourceID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name0">
          <DataField>Name0</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CurrentValue">
          <DataField>CurrentValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="GetDeploymentSummary">
      <Query>
        <DataSourceName>SCCM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CIID">
            <Value>=Parameters!CIID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select data.Status, count(data.ResourceID) as Clients from (
Select REsourceID,
(Case when IsError = 1 then 'Error'
when IsDetected = 1 and IsCompliant = 0 then 'Non-Compliant'
when IsDetected = 1 and IsCompliant = 1 then 'Compliant '
when IsDetected = 0 then 'Not Detected' 

else 'Unknown' end) as Status

from v_CIComplianceHistory where (CI_ID = @CIID )
Union

Select ResourceID, 'Unknown' as Status from vDCMDeploymentUnknownResources where BL_ID in (Select FromCIID from v_CIRelation where (ToCIID = @CIID ) and RelationType = 1)
) data 
left join v_R_System s on data.ResourceID = s.ResourceID
group by data.Status</CommandText>
      </Query>
      <Fields>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Clients">
          <DataField>Clients</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
