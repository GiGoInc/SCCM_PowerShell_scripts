        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select  
  @ID as CollectionID, 
  ErrorCode,  
  AssignedSiteCode as SiteCode,  
  count(1) as NumberOfComputers 
from ( 
  select distinct stat.ErrorCode, stat.AssignedSiteCode, coll.ResourceID  
  from fn_rbac_ClientHealthState(@UserSIDs)  stat 
  inner join fn_rbac_FullCollectionMembership(@UserSIDs)  coll on stat.SMSID=coll.SMSID  
  where  stat.HealthState=2 and coll.CollectionID=@ID 
) detail 
group by ErrorCode, AssignedSiteCode</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="ID">
            <Value>=Parameters!ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
