    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllSystems">
            <Value>=Parameters!GlobalAllSystems.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllUnknownComputers">
            <Value>=Parameters!GlobalAllUnknownComputers.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllMobileDevices">
            <Value>=Parameters!GlobalAllMobileDevices.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllDesktopandServerClients">
            <Value>=Parameters!GlobalAllDesktopandServerClients.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
         declare @TempCollectTable AS table(CollectionID nvarchar(64), [Name] nvarchar(255))
        Insert @TempCollectTable(CollectionID, [Name])
        VALUES  (N'SMS00001', @AllSystems),
                (N'SMS000US', @AllUnknownComputers),
                (N'SMSDM001', @AllMobileDevices),
                (N'SMSDM003', @AllDesktopandServerClients)
                
        SELECT DISTINCT v_Collection_Alias.CollectionID AS CollectionID, 
               ISNULL(TheTemp.Name, v_Collection_Alias.Name) AS [Name] 
               FROM fn_rbac_Collection(@UserSIDs) v_Collection_Alias 
               left join @TempCollectTable TheTemp ON TheTemp.CollectionID = v_Collection_Alias.CollectionID
        where v_Collection_Alias.CollectionType = 2 
        ORDER BY 2
        </CommandText>
      </Query>
