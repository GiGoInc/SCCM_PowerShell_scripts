  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@collection">
            <Value>=Parameters!collection.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select fcm1.name
Into #temp1
from v_fullcollectionmembership fcm1
where collectionid = @collection
group by name

Select fcm2.name, count(sw.name) as ServiceWindows
from #temp1
Inner Join v_fullcollectionmembership fcm2 ON #temp1.name = fcm2.name
Inner Join v_servicewindow sw ON fcm2.collectionid = sw.Collectionid
group by fcm2.name
having count(sw.name) = 1
Order by fcm2.name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ServiceWindows">
          <DataField>ServiceWindows</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Collection">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select name, collectionid from v_collection</CommandText>
      </Query>
