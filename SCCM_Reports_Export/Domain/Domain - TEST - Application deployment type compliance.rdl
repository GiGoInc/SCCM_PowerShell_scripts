    <DataSet Name="Application">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT CI_ID, DisplayName from fn_rbac_ListLatestApplicationCIs(dbo.fn_LShortNameToLCID(@locale), @UserSIDs)
order by DisplayName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CI_ID">
          <DataField>CI_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Collection">
            <Value>=Parameters!Collection.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Application">
            <Value>=Parameters!Application.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @CollectionType int

SELECT @CollectionType = CollectionType FROM fn_rbac_Collection(@UserSIDs)  WHERE CollectionID = @Collection

IF (@CollectionType = 2)
BEGIN

SELECT MAX( ApplicationName ) AS ApplicationName,
       MAX( DeploymentTypeName ) AS DeploymentTypeName,
       DTCI,
       SUM( CASE WHEN AppState/1000 = 1 THEN 1 ELSE 0 END) AS TotalSuccess,
       SUM( CASE WHEN AppState/1000 = 3 THEN 1 ELSE 0 END) AS TotalRNM,
       SUM( CASE WHEN AppState/1000 = 5 THEN 1 ELSE 0 END) AS TotalError,       
       SUM(1) AS TOTAL
FROM
(
    SELECT DISTINCT AppCI, ApplicationName, DTCI, DeploymentTypeName, AppState, MachineID, UserID
    FROM fn_appdtclientsummarizedstate(dbo.fn_LShortNameToLCID(@locale)) acs
    INNER JOIN fn_rbac_FullCollectionMembership(@UserSIDs)  fc ON fc.CollectionID = @Collection AND fc.ResourceID = acs.MachineID
    WHERE AppCI = @Application
) res
GROUP BY DTCI

END

IF (@CollectionType = 1)
BEGIN

SELECT MAX( ApplicationName ) AS ApplicationName,
       MAX( DeploymentTypeName ) AS DeploymentTypeName,
       DTCI,
       SUM( CASE WHEN AppState/1000 = 1 THEN 1 ELSE 0 END) AS TotalSuccess,
       SUM( CASE WHEN AppState/1000 = 3 THEN 1 ELSE 0 END) AS TotalRNM,
       SUM( CASE WHEN AppState/1000 = 5 THEN 1 ELSE 0 END) AS TotalError,       
       SUM(1) AS TOTAL
FROM
(
    SELECT DISTINCT AppCI, ApplicationName, DTCI, DeploymentTypeName, AppState, MachineID, UserID
    FROM fn_appdtclientsummarizedstate(dbo.fn_LShortNameToLCID(@locale)) acs
    INNER JOIN fn_rbac_CollectionExpandedUserMembers(@UserSIDs)  us on us.SiteID = @Collection
    INNER JOIN fn_rbac_R_User(@UserSIDs)  ud on ud.ResourceID = us.UserItemKey
    WHERE AppCI = @Application and acs.UserName = ud.Unique_User_Name0
) res
GROUP BY DTCI

END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
