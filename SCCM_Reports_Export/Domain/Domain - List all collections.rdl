  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>Select
	v_FullCollectionMembership.CollectionID [Collection ID],
	v_Collection.Name [Name],
	NameSort=v_FullCollectionMembership.CollectionID+' - '+v_Collection.Name, 
	count(case when v_FullCollectionMembership.IsActive=1 then '*' else NULL end) [Active Clients],
	count(case when v_FullCollectionMembership.IsClient= 1 then '*' else NULL end) [Client Installed],
	count(case when v_FullCollectionMembership.IsAssigned=1 then '*' else NULL end) [Assigned Clients],
	count(case when v_FullCollectionMembership.IsObsolete= 1 then '*' else NULL end) [Obsolete Clients]
From
	v_FullCollectionMembership,	v_Collection
Where v_FullCollectionMembership.ResourceType='5' and v_FullCollectionMembership.CollectionId = v_Collection.CollectionID
Group by v_FullCollectionMembership.CollectionID,v_Collection.Name
Order by v_FullCollectionMembership.CollectionID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
