  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>Select DISTINCT sys.Netbios_Name0 AS [System Name],
fcm.SiteCode AS [Site Code],
sys.User_Domain0 AS [User Domain],
sys.User_Name0 AS [User Name],
v_GS_OPERATING_SYSTEM.Caption0 AS [Operating System],
arp.DisplayName0 AS [Application Name],
arp.Version0 AS [Application Version]
FROM v_R_System  sys 
JOIN v_Add_Remove_Programs arp ON sys.ResourceID = arp.ResourceID  
JOIN v_FullCollectionMembership fcm on sys.ResourceID=fcm.ResourceID
JOIN v_GS_OPERATING_SYSTEM on sys.ResourceID = v_GS_OPERATING_SYSTEM.ResourceID
WHERE DisplayName0 = 'Adobe Reader XI (11.0.23)' and
sys.Name0 not in (
select distinct
v_R_System.Name0
from v_R_System
JOIN v_FullCollectionMembership on v_R_System.ResourceID=v_FullCollectionMembership.ResourceID
where v_FullCollectionMembership.CollectionID='DC100B0A')
Order by sys.Netbios_Name0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
