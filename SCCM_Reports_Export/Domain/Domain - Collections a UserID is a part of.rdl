  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserID">
            <Value>=Parameters!UserID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	C.CollectionID,
	C.Name,
	C.Comment
from 
	dbo.v_Collection C
	join dbo.v_FullCollectionMembership FCM on C.CollectionID = FCM.CollectionID
Where
	FCM.smsid like '%' + @UserID + '%'
Order By C.Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
