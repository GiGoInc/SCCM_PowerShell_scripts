  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>select v_R_SYSTEM.Name0 AS "PC Name",
	v_GS_OPERATING_SYSTEM.InstallDate0 AS "OS Install Date",
	v_R_System.Creation_Date0 AS "SCCM Record Creation Date",
	v_R_SYSTEM.Resource_Domain_OR_Workgr0 AS "Domain / Workgroup",
	v_R_SYSTEM.Client0 AS "Client Yes / No",
	v_R_SYSTEM.SMS_Unique_Identifier0 AS "SCCM GUID"
from v_R_System
inner join v_GS_OPERATING_SYSTEM on v_GS_OPERATING_SYSTEM.ResourceId = v_R_System.ResourceId
where DateDiff(day,v_GS_OPERATING_SYSTEM.InstallDate0, GetDate()) &lt;7
Order By v_GS_OPERATING_SYSTEM.InstallDate0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
