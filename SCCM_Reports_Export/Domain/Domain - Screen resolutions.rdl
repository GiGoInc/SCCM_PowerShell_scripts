    <DataSet Name="DataSet_CollID">
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>select
	CollectionID,
	CollectionName=Name,
	NameSort=CollectionID+' - '+Name 
from v_Collection
order by 2</CommandText>
