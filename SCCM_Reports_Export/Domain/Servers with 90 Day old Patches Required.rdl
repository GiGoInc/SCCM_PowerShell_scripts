  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
SELECT DISTINCT
	SYS.ResourceID, SYS.Name0 AS Name, SYS.AD_Site_Name0 ADSite
INTO #SUTEMP001
FROM
	dbo.v_FullCollectionMembership FCM JOIN
	dbo.v_R_System SYS ON SYS.ResourceID = FCM.ResourceID
WHERE  FCM.CollectionID = 'DC100B73'

SELECT
	SU.*, 
	max(DATEDIFF(DAY, ui.DatePosted,GETDATE())) as Days,
	UDS.AssignmentName SUAssignmentName,
	UI.Title SUTitle,
	CASE UCS.Status WHEN 2 THEN 'Required' WHEN 3 THEN 'Installed' END AS SUComplianceStatus,
	UI.BulletinID SUBulletinID, 
	CI.CategoryInstanceName SUCategoryInstanceName,
	CASE UI.Severity WHEN '10' THEN 'Critical' WHEN '8' THEN 'Important' WHEN '6' THEN 'Moderate' WHEN '2' THEN 'Low' ELSE 'Unknown' END SUSeverity,
	UI.ArticleID SUArticleID, 
	UI.InfoURL SUInfoURL, 
	UI.CI_ID SUCI_ID,
	CASE USS.LastScanState WHEN 3 THEN 'Successfull' ELSE 'Failed' END AS LastSUScanState, 
	USS.LastScanTime LastSUScanDate
FROM #SUTEMP001 SU JOIN
	dbo.v_R_System AS SYS ON SU.ResourceID = SYS.ResourceID JOIN
	dbo.v_UpdateScanStatus AS USS ON SU.ResourceID = USS.ResourceID JOIN
	dbo.v_UpdateAssignmentStatus UAS ON UAS.ResourceID = SU.ResourceID LEFT JOIN
	dbo.v_UpdateComplianceStatus UCS ON UCS.ResourceID = SU.ResourceID JOIN
	dbo.v_UpdateInfo AS UI ON UI.CI_ID = UCS.CI_ID AND UI.IsDeployed = 1 JOIN
	dbo.v_UpdateDeploymentSummary UDS ON UDS.AssignmentID = UAS.AssignmentID AND UCS.CI_ID = UDS.CI_ID JOIN
	dbo.v_CICategoryInfo AS CI ON UCS.CI_ID = CI.CI_ID AND CI.CategoryTypeName = 'UpdateClassification'
where 
	UCS.Status = '2'
	and 
	ui.DatePosted &lt; DATEADD(day,-90,getdate())
	
	
group by 
	SU.ResourceID,
	SU.Name,
	SU.ADSite,
	UDS.AssignmentName,
	UI.Title,
	UCS.Status,
	UI.Severity,
	USS.LastScanTime,
	UI.ArticleID,
	UI.InfoURL,
	UI.BulletinID,
	USS.LastScanState,
	UI.CI_ID,
	CI.CategoryInstanceName

DROP TABLE #SUTEMP001</CommandText>
