  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PCName">
            <Value>=Parameters!PCName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select v_CIRules.RuleName
	,v_CIRules.RuleDescription
	,v_CIComplianceStatusDetail.CIVersion
	,v_CIComplianceStatusDetail.ConfigurationItemName
	,v_CIComplianceStatusDetail.LastComplianceMessageTime
	,v_CIRules.Criteria AS [Rule Criteria]
	,v_CIComplianceStatusDetail.Criteria AS [Compliance Status Detail Criteria]
	,v_CIComplianceStatusDetail.CurrentValue
	,v_CIRules.RuleType
	,v_CIComplianceStatusDetail.SettingType
	,v_CIRules.Severity
	,v_CIComplianceStatusDetail.InstanceData
	,v_CIComplianceStatusDetail.CI_ID
	,v_CIRules.CI_ID AS [Rule CI ID]	

FROM v_CIComplianceStatusDetail
Inner Join v_CIRules on v_CIComplianceStatusDetail.CI_ID = v_CIRules.CI_ID
Where v_CIComplianceStatusDetail.Netbios_Name0 = @PCName
and 
v_CIComplianceStatusDetail.ConfigurationItemName LIKE 'HB%'
Order By ConfigurationItemName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
