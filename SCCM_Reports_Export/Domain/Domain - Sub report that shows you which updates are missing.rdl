      <DataSourceReference>/ConfigMgr_DC1/{5C6358F2-4BB6-4a1b-A16E-8D96795D8602}</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>f89d5e80-cdd1-499d-a97f-361482a3ee53</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ComputerName">
            <Value>=Parameters!ComputerName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UpdateList">
            <Value>=Parameters!UpdateList.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	TOP (100) PERCENT v_UpdateInfo.BulletinID,
	v_UpdateInfo.ArticleID,
	v_UpdateInfo.Title,
	v_StateNames.StateName AS State, 
	v_UpdateDeploymentSummary.AssignmentName AS [Update List]
FROM  v_R_System
INNER JOIN v_UpdateComplianceStatus ON v_R_System.ResourceID = v_UpdateComplianceStatus.ResourceID
INNER JOIN v_UpdateInfo ON v_UpdateComplianceStatus.CI_ID = v_UpdateInfo.CI_ID
INNER JOIN v_StateNames ON v_UpdateComplianceStatus.LastEnforcementMessageID = v_StateNames.StateID
INNER JOIN v_UpdateDeploymentSummary ON v_UpdateInfo.CI_ID = v_UpdateDeploymentSummary.CI_ID
WHERE
	(v_UpdateComplianceStatus.Status = 2)
	AND
	(v_StateNames.TopicType = 402)
	AND
	(v_R_System.Netbios_Name0 = @ComputerName)
	AND 
	(NOT (v_StateNames.StateName LIKE 'Successfully Installed update'))
	AND 
	v_UpdateDeploymentSummary.AssignmentName = @UpdateList
ORDER BY
	v_R_System.Netbios_Name0,
	v_UpdateComplianceStatus.Status</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="BulletinID">
          <DataField>BulletinID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ArticleID">
          <DataField>ArticleID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="State">
          <DataField>State</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Update_List">
          <DataField>Update List</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Update_List">
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>SELECT 
	AssignmentID,
	AssignmentName,
	CI_ID
FROM 
	v_UpdateDeploymentSummary
Where
	AssignmentName = 'ADR: Software Updates - Workstations - Test'
	OR
	AssignmentName = 'ADR: Software Updates - Workstations - Manual'
	OR
	AssignmentName = 'ADR: Software Updates - Workstations - Branch Test'
	OR
	AssignmentName = 'ADR: Software Updates - Workstations - Branch'
	OR
	AssignmentName = 'ADR: Software Updates - Workstations'
	OR
	AssignmentName = 'ADR: Software Updates - Servers'
	OR
	AssignmentName = 'ADR: Server Reporting - Critical patches'
Order By AssignmentName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="AssignmentName">
          <DataField>AssignmentName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AssignmentID">
          <DataField>AssignmentID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CI_ID">
          <DataField>CI_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
