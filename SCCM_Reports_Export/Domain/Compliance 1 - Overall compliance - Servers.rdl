<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <Description>This report returns the overall compliance data for a software update group.</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_">
      <DataSourceReference>/ConfigMgr_DC1/{5C6358F2-4BB6-4a1b-A16E-8D96795D8602}</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>2c7f8d0a-e235-434e-ad68-4b30bc071bc4</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet0">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AuthListID">
            <Value>=Parameters!AuthListID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollID">
            <Value>=Parameters!CollID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          declare @CI_ID int; select @CI_ID=CI_ID from fn_rbac_ConfigurationItems(@UserSIDs)  where CIType_ID=9 and CI_UniqueID=@AuthListID

          select
            CollectionID=@CollID,
            Status=sn.StateName,
            cs.NumberOfComputers,
            PComputers=convert(float, isnull(cs.NumberOfComputers, 0)*100.00) / isnull(nullif(cs.NumTotal, 0), 1),
            AuthListID=@AuthListID
          from (select CI_ID, NumTotal, [0]=NumUnknown, [1]=NumPresent+NumNotApplicable, [2]=NumMissing
                from fn_rbac_UpdateSummaryPerCollection(@UserSIDs)  
                where CI_ID=@CI_ID and CollectionID=@CollID
               ) cnt
          unpivot (NumberOfComputers for [Status] in ([0], [1], [2])) cs
          left join fn_rbac_StateNames(@UserSIDs)  sn on sn.TopicType=300 and sn.StateID=cs.Status
          where cs.NumberOfComputers&gt;0
          order by cs.NumberOfComputers desc
        </CommandText>
      </Query>
      <Fields>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
        </Field>
        <Field Name="Status">
          <DataField>Status</DataField>
        </Field>
        <Field Name="NumberOfComputers">
          <DataField>NumberOfComputers</DataField>
        </Field>
        <Field Name="PComputers">
          <DataField>PComputers</DataField>
        </Field>
        <Field Name="AuthListID">
          <DataField>AuthListID</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollID">
            <Value>=Parameters!CollID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          select
            CollectionName=vc.Name,
            NumberInCollection=NumTotal
          from fn_rbac_Collection(@UserSIDs)  vc
          cross apply(select NumTotal=count(*) from fn_rbac_ClientCollectionMembers(@UserSIDs)  where CollectionID=vc.CollectionID) cnt
          where vc.CollectionID=@CollID
        </CommandText>
      </Query>
      <Fields>
        <Field Name="UnlocCollectionName">
          <DataField>CollectionName</DataField>
        </Field>
        <Field Name="CollectionName">
          <Value>=SrsResources.Localization.GetDBString(Fields!UnlocCollectionName.Value, User!Language)</Value>
        </Field>
        <Field Name="NumberInCollection">
          <DataField>NumberInCollection</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Parameter_DataSet_AuthListID">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          select CI_UniqueID as AuthListID, Title
          from fn_rbac_AuthListInfo(@lcid, @UserSIDs)
          order by Title
        </CommandText>
      </Query>
      <Fields>
        <Field Name="AuthListID">
          <DataField>AuthListID</DataField>
        </Field>
        <Field Name="Title">
          <DataField>Title</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Parameter_DataSet_CollID">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          select CollectionID, CollectionName=Name, NameSort=CollectionID+' - '+Name 
          from fn_rbac_Collection(@UserSIDs)  
          order by 2
        </CommandText>
      </Query>
      <Fields>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
        </Field>
        <Field Name="UnlocCollectionName">
          <DataField>CollectionName</DataField>
        </Field>
        <Field Name="CollectionName">
          <Value>=Fields!CollectionID.Value &amp; " - "&amp;SrsResources.Localization.GetDBString(Fields!UnlocCollectionName.Value, User!Language)</Value>
        </Field>
      </Fields>
    </DataSet>
