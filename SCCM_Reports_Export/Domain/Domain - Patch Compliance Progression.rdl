        </Field>
        <Field Name="Caption0">
          <DataField>Caption0</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Last_HW_scan">
          <DataField>Last HW scan</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="collectionID">
          <DataField>collectionID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
