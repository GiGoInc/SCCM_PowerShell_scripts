      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select
  advert.AdvertisementID,
  advert.PresentTime
from fn_rbac_Advertisement(@UserSIDs)  advert
where advert.PackageID = @PackageProgram and
            advert.CollectionID = @Collection</CommandText>
