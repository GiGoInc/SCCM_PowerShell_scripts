        </Field>
        <Field Name="LocLastStatusMessageIDName">
          <Value>=SrsResources.Localization.GetDBString(Fields!LastStatusMessageIDName.Value, User!Language, "_OfferStatusType-Messages")</Value>
        </Field>
        <Field Name="LocLastAcceptanceMessageIDName">
          <Value>=SrsResources.Localization.GetDBString(Fields!LastAcceptanceMessageIDName.Value, User!Language, "_OfferStatusType-Messages")</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>="
              declare @State int

              select @State=MessageState
              from fn_rbac_AdvertisementStatusInformation(@UserSIDs) 
              where MessageStateName=@StateName and MessageState &lt; 100

              if @State in (0,1,2,3)
              select sys.Netbios_Name0, sys.User_Domain0, sys.User_Name0, site.SMS_Installed_Sites0, Client_Type0,
              LastAcceptanceMessageID, LastAcceptanceMessageIDName,
              LastAcceptanceStatusTime,
              stat.AdvertisementID, advert.PackageID, advert.CollectionID
              from v_ClientAdvertisementStatus  stat
              join fn_rbac_Advertisement(@UserSIDs)  advert on stat.AdvertisementID = advert.AdvertisementID
              join fn_rbac_R_System(@UserSIDs)  sys on stat.ResourceID=sys.ResourceID
              left join v_RA_System_SMSInstalledSites site on stat.ResourceID=site.ResourceID
              where stat.LastAcceptanceState=@State and stat.AdvertisementID=@Deployment
              else if @State = 11
              select sys.Netbios_Name0, sys.User_Domain0, sys.User_Name0,site.SMS_Installed_Sites0, Client_Type0,
              LastStatusMessageID, LastStatusMessageIDName,
              LastStatusTime,
              stat.AdvertisementID, LastExecutionResult, LastExecutionContext, advert.PackageID, advert.CollectionID
              from v_ClientAdvertisementStatus stat
              join fn_rbac_Advertisement(@UserSIDs)  advert on stat.AdvertisementID = advert.AdvertisementID
              join fn_rbac_R_System(@UserSIDs)  sys on stat.ResourceID=sys.ResourceID
              left join v_RA_System_SMSInstalledSites site on stat.ResourceID=site.ResourceID
              where stat.LastState=@State and stat.AdvertisementID=@Deployment
              else if @State in (9,12,13)
              select sys.Netbios_Name0, sys.User_Domain0, sys.User_Name0, site.SMS_Installed_Sites0, Client_Type0,
              LastStatusMessageID, LastStatusMessageIDName,
              LastStatusTime,
              stat.AdvertisementID, LastExecutionContext, advert.PackageID, advert.CollectionID
              from v_ClientAdvertisementStatus stat
              join fn_rbac_Advertisement(@UserSIDs)  advert on stat.AdvertisementID = advert.AdvertisementID
              join fn_rbac_R_System(@UserSIDs)  sys on stat.ResourceID=sys.ResourceID
              left join v_RA_System_SMSInstalledSites site on stat.ResourceID=site.ResourceID
              where stat.LastState=@State and stat.AdvertisementID=@Deployment
              else
              select sys.Netbios_Name0, sys.User_Domain0, sys.User_Name0, site.SMS_Installed_Sites0, Client_Type0,
              CASE LastStatusMessageID WHEN 65535 THEN 10002 ELSE LastStatusMessageID END as LastStatusMessageID,
              LastStatusMessageIDName,
              LastStatusTime,
              stat.AdvertisementID, advert.PackageID, advert.CollectionID
              from v_ClientAdvertisementStatus stat
              join fn_rbac_Advertisement(@UserSIDs)  advert on stat.AdvertisementID = advert.AdvertisementID
              join fn_rbac_R_System(@UserSIDs)  sys on stat.ResourceID=sys.ResourceID
              left join v_RA_System_SMSInstalledSites site on stat.ResourceID=site.ResourceID
              where stat.LastState=@State and stat.AdvertisementID=@Deployment
            "</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StateName">
            <Value>=Parameters!StateName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Deployment">
            <Value>=Parameters!Deployment.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_DataSet_StateName">
      <Fields>
        <Field Name="MessageStateName">
          <DataField>MessageStateName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LocMessageStateName">
          <Value>=SrsResources.Localization.GetDBString(Fields!MessageStateName.Value, User!Language, "_LastDistributionStatus-Name")</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select MessageStateName from (
                       select
                       statinfo.MessageStateName
                       from fn_rbac_Advertisement(@UserSIDs)  advert
                       join fn_rbac_ClientAdvertisementStatus(@UserSIDs)  cas on advert.AdvertisementID = @Deployment AND advert.AdvertisementID = cas.AdvertisementID
                       join fn_rbac_AdvertisementStatusInformation(@UserSIDs)  statinfo on cas.LastAcceptanceMessageID = statinfo.MessageID
                       where statinfo.MessageState &lt;= 13 
                       union
                       select
                       statinfo.MessageStateName
                       from fn_rbac_Advertisement(@UserSIDs)  advert
                       join fn_rbac_ClientAdvertisementStatus(@UserSIDs)  cas on advert.AdvertisementID = @Deployment AND advert.AdvertisementID = cas.AdvertisementID
                       join fn_rbac_AdvertisementStatusInformation(@UserSIDs)  statinfo on cas.LastStatusMessageID = statinfo.MessageID
                       where statinfo.MessageState &lt;= 13
                     ) as results
                     order by MessageStateName
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Deployment">
            <Value>=Parameters!Deployment.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="PackageProgramDataset">
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select distinct
  pkg.Name + '(' + advert.ProgramName + ')' as name,
  pkg.PackageID as id
from fn_rbac_Advertisement(@UserSIDs)  advert
join fn_rbac_Package2(@UserSIDs)  pkg on advert.PackageID = pkg.PackageID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
