    <DataSet Name="CollectionDataset">
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LocName">
          <Value>=SrsResources.Localization.GetDBString(Fields!Name.Value, User!Language, "SMS_Collection-Name")</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select distinct
  co.Name,
  co.CollectionID
from fn_rbac_Collection(@UserSIDs)  co
join fn_rbac_Advertisement(@UserSIDs)  advert on co.CollectionID = advert.CollectionID
where advert.PackageID = @PackageProgram
order by Name</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PackageProgram">
            <Value>=Parameters!PackageProgram.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
