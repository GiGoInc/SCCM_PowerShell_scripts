      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>declare @Total int
              declare @Accepted int

              select @Total=count(*), @Accepted=sum(case LastState when 0 then 0 else 1 end)
              from fn_rbac_ClientAdvertisementStatus(@UserSIDs) 
              where AdvertisementID=@Deployment

              select stat.LastStateName as C017, count(*) as C015,
              ROUND(100.0*count(*)/@Accepted,1)  as C016,
              stat.AdvertisementID,
               advert.PackageID,
              advert.CollectionID
              from fn_rbac_ClientAdvertisementStatus(@UserSIDs)  stat
              join fn_rbac_Advertisement(@UserSIDs)  advert on stat.AdvertisementID = advert.AdvertisementID
              where stat.AdvertisementID=@Deployment and LastState!=0
              group by LastStateName, stat.AdvertisementID, advert.PackageID, advert.CollectionID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Deployment">
            <Value>=Parameters!Deployment.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="PackageProgramDataset">
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select distinct
  pkg.Name + '(' + advert.ProgramName + ')' as name,
  pkg.PackageID + advert.ProgramName as id
from fn_rbac_Advertisement(@UserSIDs)  advert
join fn_rbac_Package2(@UserSIDs)  pkg on advert.PackageID = pkg.PackageID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="CollectionDataset">
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LocCollectionName">
          <Value>=SrsResources.Localization.GetDBString(Fields!Name.Value, User!Language, "SMS_Collection-Name")</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select distinct
  co.Name,
  co.CollectionID
from fn_rbac_Collection(@UserSIDs)  co
join fn_rbac_Advertisement(@UserSIDs)  advert on co.CollectionID = advert.CollectionID
where advert.PackageID + advert.ProgramName = @PackageProgram
order by Name</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PackageProgram">
            <Value>=Parameters!PackageProgram.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DeploymentDataset">
      <Fields>
        <Field Name="AdvertisementID">
          <DataField>AdvertisementID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PresentTime">
          <DataField>PresentTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select
  advert.AdvertisementID,
  advert.PresentTime
from fn_rbac_Advertisement(@UserSIDs)  advert
where advert.CollectionID = @Collection and
            advert.PackageID + advert.ProgramName = @PackageProgram</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Collection">
            <Value>=Parameters!Collection.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PackageProgram">
            <Value>=Parameters!PackageProgram.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
