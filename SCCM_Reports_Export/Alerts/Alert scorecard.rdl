      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT
   v_StatusMessagesAlerts_Alias.Username
  ,v_StatusMessagesAlerts_Alias.Time
  ,v_StatusMessagesAlerts_Alias.TypeID
  ,v_StatusMessagesAlerts_Alias.FeatureArea
  ,v_StatusMessagesAlerts_Alias.RecordID
FROM
  fn_rbac_StatusMessagesAlerts(@UserSIDs) v_StatusMessagesAlerts_Alias
WHERE
  v_StatusMessagesAlerts_Alias.MessageID = 30244</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
