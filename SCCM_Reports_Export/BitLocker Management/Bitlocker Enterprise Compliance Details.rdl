    <DataSet Name="InventoryViewExistence">
      <Query>
        <DataSourceName>SCCM_DB</DataSourceName>
        <CommandText>
          DECLARE @query nvarchar(70)
          SET @query = N'SELECT COUNT(EncodedComputerName0) AS Status FROM V_GS_MBAM_POLICY'
          BEGIN TRY
            EXEC(@query)
          END TRY
          BEGIN CATCH
            SELECT 0 AS Status
          END CATCH
        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ComplianceDetails">
      <Query>
        <DataSourceName>SCCM_DB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@collectionIdParameter">
            <Value>=Parameters!collectionIdParameter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          DECLARE @baseline_CI_UniqueID varchar(512)
          DECLARE @CICompliant int
          DECLARE @CINotCompliant int
          
          -- Set CI compliant value
          SET @CICompliant = 1
          
          -- Set CI non compliant value
          SET @CINotCompliant = 2
          
          -- Set the Baseline CI_UniqueID (AuthoringScopeId + LogicalName) based on a predefined value matching MBAM proprietary baseline
          SET @baseline_CI_UniqueID = N'ScopeId_MBAM_97199067-B6C4-408C-A742-7D3CB731A297/REPORT_PARAMETER_BASELINE_LOGICALNAME_TOKEN'
          
          SELECT MBAMPolicy.EncodedComputerName0 [Computer Name],
            ComputerSystem.Domain0 [Domain Name], 
            RSystem.User_Domain0 + N'\' + RSystem.User_Name0 [Device Users],
            CompStat.ComplianceState [Compliance Status],
            CASE
              WHEN MBAMPolicy.MBAMPolicyEnforced0 IS NULL THEN -1 -- (when outer join returns null : N/A)
              WHEN CompStat.ComplianceState = @CINotCompliant AND MBAMPolicy.MBAMPolicyEnforced0 = 3 THEN 1 -- tmp user exempt
              WHEN CompStat.ComplianceState = @CICompliant AND MBAMPolicy.MBAMPolicyEnforced0 = 2 THEN 2 -- user exempt
              ELSE 0 -- not exempted
            END AS Exemption,
            CASE
              WHEN CompStat.ComplianceState = @CICompliant AND MBAMPolicy.MBAMPolicyEnforced0 = 0 THEN 50 -- policy not enforced
              WHEN CompStat.ComplianceState = @CICompliant AND MBAMPolicy.MBAMPolicyEnforced0 = 1 THEN 0 -- No Error
              WHEN CompStat.ComplianceState = @CINotCompliant AND MBAMPolicy.MBAMMachineError0 IS NOT NULL THEN MBAMPolicy.MBAMMachineError0 -- MBAM agent error status
              ELSE -1 -- No Details \ Errors
            END AS [Compliance Status Details],
            MAX(AssignmentStatus.LastEvaluationMessageTime) [Last Evaluation Date]
          FROM v_FullCollectionMembership FullCollection
            INNER JOIN v_R_System_Valid RSystem
              ON FullCollection.ResourceID = RSystem.ResourceID
            INNER JOIN v_GS_COMPUTER_SYSTEM ComputerSystem
              ON RSystem.ResourceID = ComputerSystem.ResourceID
            INNER JOIN v_SMSCICurrentComplianceStatus CompStat
              ON CompStat.ItemKey = RSystem.ResourceID
	          INNER JOIN v_ConfigurationItems ConfigItems
			        ON ConfigItems.ModelID = CompStat.ModelID
            LEFT OUTER JOIN v_GS_MBAM_POLICY MBAMPolicy
              ON MBAMPolicy.ResourceID = FullCollection.ResourceID
            LEFT OUTER JOIN v_CIAssignmentToCI AssignmentCI
              ON AssignmentCI.CI_ID  = ConfigItems.CI_ID
            LEFT OUTER JOIN v_CIAssignmentStatus AssignmentStatus
              ON AssignmentStatus.AssignmentID = AssignmentCI.AssignmentID
              AND AssignmentStatus.ResourceID = RSystem.ResourceID
          WHERE MBAMPolicy.EncodedComputerName0 IS NOT NULL
            AND FullCollection.IsClient = 1
            AND FullCollection.CollectionID = @collectionIdParameter
            AND (ConfigItems.CI_ID IN (select v.CI_ID from v_BLM_CI_ID_AND_COLL_ID v where v.CollectionID = @collectionIdParameter ))
          GROUP BY RSystem.ResourceID,
            ComputerSystem.Domain0,
            MBAMPolicy.EncodedComputerName0,
            RSystem.User_Domain0,
            RSystem.User_Name0,
            CompStat.ComplianceState,
            MBAMPolicy.MBAMPolicyEnforced0,
            MBAMPolicy.MBAMMachineError0
        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Computer_Name">
          <DataField>Computer Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Domain_Name">
          <DataField>Domain Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Device_Users">
          <DataField>Device Users</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Compliance_Status">
          <DataField>Compliance Status</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Exemption">
          <DataField>Exemption</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Compliance_Status_Details">
          <DataField>Compliance Status Details</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Last_Evaluation_Date">
          <DataField>Last Evaluation Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=IIF((IIF(Parameters!complianceStatusParameter.Value = 1, CStr(Fields!Compliance_Status.Value) &amp; "," &amp; CStr(IIF(Fields!Exemption.Value &lt;= 0, 0, Fields!Exemption.Value)) &amp; "," &amp; CStr(Fields!Compliance_Status_Details.Value \ 50), "-1000") = "1,0,0") Or
(IIF(Parameters!complianceStatusParameter.Value = 2 And Fields!Exemption.Value = 0, Fields!Compliance_Status.Value, -1000) = 2) Or
((IIF(Parameters!complianceStatusParameter.Value = 3, Fields!Compliance_Status.Value, -1000) = 0) Or (IIF(Parameters!complianceStatusParameter.Value = 3, Fields!Compliance_Status.Value, -1000) = 4)) Or
(IIF(Parameters!complianceStatusParameter.Value = 4, CStr(Fields!Exemption.Value) &amp; "," &amp; CStr(Fields!Compliance_Status.Value), "-1000") = "1,2") Or
(IIF(Parameters!complianceStatusParameter.Value = 5, CStr(Fields!Exemption.Value) &amp; "," &amp; CStr(Fields!Compliance_Status.Value), "-1000") = "2,1") Or
(IIF(Parameters!complianceStatusParameter.Value = 6, CStr(Fields!Compliance_Status_Details.Value) &amp; "," &amp; CStr(Fields!Compliance_Status.Value), "-1000") = "50,1") Or
(IIF(Parameters!complianceStatusParameter.Value = 7, 1, -1000) = 1), True, False)</FilterExpression>
          <Operator>Equal</Operator>
          <FilterValues>
            <FilterValue>=CBool(True)</FilterValue>
          </FilterValues>
        </Filter>
        <Filter>
          <FilterExpression>=IIF((IIF(Parameters!complianceStatusDetailsParameter.Value = 1, Fields!Compliance_Status_Details.Value, -1000) = 1) Or
(IIF(Parameters!complianceStatusDetailsParameter.Value = 2, Fields!Compliance_Status_Details.Value, -1000) = 5) Or
(IIF(Parameters!complianceStatusDetailsParameter.Value = 3, Fields!Compliance_Status_Details.Value, -1000) = 6) Or
(IIF(Parameters!complianceStatusDetailsParameter.Value = 4, Fields!Compliance_Status_Details.Value, -1000) = 7) Or
(IIF(Parameters!complianceStatusDetailsParameter.Value = 5, Fields!Compliance_Status_Details.Value, -1000) = 8) Or
(IIF(Parameters!complianceStatusDetailsParameter.Value = 6, Fields!Compliance_Status_Details.Value, -1000) = 9) Or
(IIF(Parameters!complianceStatusDetailsParameter.Value = 7, Fields!Compliance_Status_Details.Value, -1000) = 10) Or
(IIF(Parameters!complianceStatusDetailsParameter.Value = 8, Fields!Compliance_Status_Details.Value, -1000) = -1) Or
(IIF(Parameters!complianceStatusDetailsParameter.Value = 10, 1, -1000) = 1), True, False)</FilterExpression>
          <Operator>Equal</Operator>
          <FilterValues>
            <FilterValue>=CBool(True)</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
