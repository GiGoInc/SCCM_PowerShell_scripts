Public Function DecodeString(ByVal toDecode As String) As String
        On Error GoTo ErrorExit
        Dim decodedBytes As Byte() = System.Convert.FromBase64String(toDecode)
        Dim decodedValue As String = System.Text.UnicodeEncoding.Unicode.GetString(decodedBytes)
        Return decodedValue
ErrorExit:
        Return ""
End Function

Public Function ToLocalTime(ByVal utcTime As DateTime) As DateTime
        On Error GoTo ErrorExit
        Return utcTime.ToLocalTime()
ErrorExit:
        Return utcTime
End Function
