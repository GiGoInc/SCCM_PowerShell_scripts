    <DataSet Name="DataSet0">
      <Fields>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NumberOfComputers">
          <DataField>NumberOfComputers</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="PComputers">
          <DataField>PComputers</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DeploymentID">
          <DataField>DeploymentID</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="UniqueUpdateID">
          <DataField>UniqueUpdateID</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="DeploymentStateID">
          <DataField>DeploymentStateID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DeploymentName">
          <DataField>DeploymentName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
              declare @DeploymentLocalID int = (select AssignmentID from fn_rbac_CIAssignment(@UserSIDs)  where Assignment_UniqueID = @DEPLOYMENTID)
              declare @COLLCOUNT as int = (select count(*) from fn_rbac_CIAssignmentTargetedMachines(@UserSIDs)  where AssignmentID=@DeploymentLocalID)
              declare @LOCALUPDATEID int = (select CI_ID from fn_rbac_UpdateCIs(@UserSIDs)  where CI_UniqueID=@UPDATEID and CI_ID in (select CI_ID from fn_rbac_CIAssignmentToCI(@UserSIDs)  where AssignmentID=@DeploymentLocalID))

              select
                sn.StateName as Status,
                count(*) as NumberOfComputers,
                PComputers=convert(float, isnull(count(*), 0)*100.0) / isnull(nullif(@COLLCOUNT, 0), 1),
                @DEPLOYMENTID as DeploymentID,
                @UPDATEID as UniqueUpdateID,
                sn.TopicType*10000 + sn.StateID as DeploymentStateID
              from fn_rbac_UpdateState_Combined(@UserSIDs)  uc
              join fn_rbac_CIAssignmentTargetedMachines(@UserSIDs)  atm on uc.ResourceID = atm.ResourceID and atm.AssignmentID=@DeploymentLocalID 
              join fn_rbac_StateNames(@UserSIDs)  sn on uc.StateType = sn.TopicType and sn.StateID=isnull(uc.StateID,0)
              where uc.CI_ID=@LOCALUPDATEID
              group by sn.StateName, sn.TopicType, sn.StateID
              order by NumberOfComputers desc, sn.StateName
            </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DEPLOYMENTID">
            <Value>=Parameters!DEPLOYMENTID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UPDATEID">
            <Value>=Parameters!UpdateID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet1">
      <Fields>
