      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>IF (@SelectBy = 2)
BEGIN
SELECT ds.SoftwareName AS SoftwareName, 
       ds.CollectionID,
       ds.CollectionName,
       ds.CI_ID,
       ad.MachineName,
       ad.UserName,
       dbo.fn_GetAppState(ad.ComplianceState, ad.EnforcementState, cia.OfferTypeID, 1, ad.DesiredState, ad.IsApplicable) AS EnforcementState
FROM fn_rbac_R_System(@UserSIDs)  sd
INNER JOIN fn_rbac_FullCollectionMembership(@UserSIDs)  cm ON cm.ResourceID = sd.ResourceID
INNER JOIN fn_rbac_DeploymentSummary(dbo.fnLShortNameToLCID(@locale), @UserSIDs) ds ON ds.CollectionID = cm.CollectionID AND ds.FeatureType = 1
LEFT JOIN fn_rbac_AppIntentAssetData(@UserSIDs)  ad ON ad.MachineID = cm.ResourceID AND ad.AssignmentID = ds.AssignmentID
INNER JOIN fn_rbac_CIAssignment(@UserSIDs)  cia ON cia.AssignmentID = ds.AssignmentID
WHERE Client0 = 1 AND sd.Netbios_Name0 = @Name
END

IF (@SelectBy = 1)
BEGIN
SELECT ds.SoftwareName AS SoftwareName, 
       ds.CollectionID,
       ds.CollectionName,
       ds.CI_ID,
       ad.MachineName,
       ad.UserName,
       dbo.fn_GetAppState(ad.ComplianceState, ad.EnforcementState, cia.OfferTypeID, 1, ad.DesiredState, ad.IsApplicable) AS EnforcementState
FROM fn_rbac_CollectionExpandedUserMembers(@UserSIDs)  cm
INNER JOIN fn_rbac_R_User(@UserSIDs)  ud ON ud.ResourceID= cm.UserItemKey
INNER JOIN fn_rbac_DeploymentSummary(dbo.fnLShortNameToLCID(@locale), @UserSIDs) ds ON ds.CollectionID = cm.SiteID
LEFT JOIN fn_rbac_AppIntentAssetData(@UserSIDs)  ad ON ad.UserName = @Name AND ad.AssignmentID = ds.AssignmentID
INNER JOIN fn_rbac_CIAssignment(@UserSIDs)  cia ON cia.AssignmentID = ds.AssignmentID
WHERE ud.Unique_User_Name0 = @Name AND ds.FeatureType = 1
END</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SelectBy">
            <Value>=Parameters!SelectBy.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Name">
            <Value>=Parameters!Name.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
