        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>DECLARE @PolicyModelID int

SELECT
  @PolicyModelID = PolicyModelID
FROM fn_rbac_DeploymentSummary([dbo].[fn_LShortNameToLCID](@locale), @UserSIDs)
WHERE AssignmentID = @AssignmentID

begin
if (@StatusType = 1 or @StatusType = 2)

SELECT
    AppState.CollectionID,
    AppState.CollectionName,
    AppState.AssignmentID,
    AppState.PolicyModelID,
    AppState.AppCI,
    AppState.AppName,
    AppState.StatusType,
    AppState.AppStatusType,
    AppState.EnforcementState,
    '' as RequirementName,
    0 as RequirementType,
    NULL as ErrorCode,
    AppState.DTCI,
    AppState.DTName,
    AppState.Total,
    0 as RuleID
FROM
    fn_rbac_AppDeploymentStatus([dbo].[fn_LShortNameToLCID](@locale), @UserSIDs) AppState
WHERE
    (AppState.AssignmentID = @AssignmentID) AND
    (AppState.PolicyModelID = @PolicyModelID) AND
    (@DT = 0 OR AppState.DTCI = @DT) AND
    (AppState.StatusType = @StatusType)
ELSE IF (@StatusType = 3)
SELECT
    AppState.CollectionID,
    AppState.CollectionName,
    AppState.AssignmentID,
    AppState.PolicyModelID,
    AppState.AppCI,
    AppState.AppName,
    AppState.StatusType,
    AppState.AppStatusType,
    AppState.EnforcementState,
    AppState.RequirementName,
    AppState.RequirementType,
    NULL as ErrorCode,
    AppState.DTCI,
    AppState.DTName,
    AppState.Total,
    AppState.RuleID
FROM
    fn_rbac_AppDeploymentRNMStatus([dbo].[fn_LShortNameToLCID](@locale), @UserSIDs) AppState
INNER JOIN fn_rbac_DeploymentSummary([dbo].[fn_LShortNameToLCID](@locale), @UserSIDs) DS on AppState.PolicyModelID = DS.PolicyModelID
WHERE
    (AppState.AssignmentID = @AssignmentID) AND
    (AppState.PolicyModelID = @PolicyModelID) AND
    (@DT = 0 OR AppState.DTCI = @DT) AND
    AppState.StatusType = @StatusType
ELSE IF (@StatusType = 4)
SELECT
    AppState.CollectionID,
    AppState.CollectionName,
    AppState.AssignmentID,
    AppState.PolicyModelID,
    AppState.CI_ID as AppCI,
    AppState.SoftwareName as AppName,
    4 as StatusType,
    4 as AppStatusType,
    4000 as EnforcementState,
    '' as RequirementName,
    0 as RequirementType,
    NULL as ErrorCode,
    0 as DTCI,
    '' as DTName,
    AppState.Total,
    0 as RuleID
FROM
    fn_rbac_CIDeploymentUnknownStatus([dbo].[fn_LShortNameToLCID](@locale), @UserSIDs) AppState
INNER JOIN fn_rbac_DeploymentSummary([dbo].[fn_LShortNameToLCID](@locale), @UserSIDs) DS on AppState.PolicyModelID = DS.PolicyModelID
WHERE
    (AppState.AssignmentID = @AssignmentID) AND
    (AppState.PolicyModelID = @PolicyModelID)
ELSE IF (@StatusType = 5)
SELECT
    AppState.CollectionID,
    AppState.CollectionName,
    AppState.AssignmentID,
    AppState.PolicyModelID,
    AppState.AppCI,
    AppState.AppName,
    AppState.StatusType,
    AppState.AppStatusType,
    AppState.EnforcementState,
    '' as RequirementName,
    0 as RequirementType,
    AppState.ErrorCode,
    AppState.DTCI,
    AppState.DTName,
    AppState.Total,
    0 as RuleID
FROM
    fn_rbac_AppDeploymentErrorStatus([dbo].[fn_LShortNameToLCID](@locale), @UserSIDs) AppState
INNER JOIN fn_rbac_DeploymentSummary([dbo].[fn_LShortNameToLCID](@locale), @UserSIDs) DS on AppState.PolicyModelID = DS.PolicyModelID
WHERE
    (AppState.AssignmentID = @AssignmentID) AND
    (AppState.PolicyModelID = @PolicyModelID) AND
    (@DT = 0 OR AppState.DTCI = @DT) AND
    AppState.StatusType = @StatusType
end</CommandText>
