        </Field>
        <Field Name="DeploymentTypeName">
          <DataField>DeploymentTypeName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DTCI">
          <DataField>DTCI</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ErrorCode">
          <DataField>ErrorCode</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>DECLARE @CollectionType int

SELECT @CollectionType = CollectionType FROM fn_rbac_Collection(@UserSIDs)  WHERE CollectionID = @Collection

IF (@CollectionType = 2)
BEGIN
    SELECT MachineName, UserName, AppCI, ApplicationName, DTCI, DeploymentTypeName, MAX(Date) AS Date, ErrorCode
    FROM
    (
    SELECT DISTINCT MachineName, UserName, AppCI, ApplicationName, DTCI, DeploymentTypeName, Date, ErrorCode 
    FROM 
    fn_appevalerrors(dbo.fn_LShortNameToLCID(@locale)) aee
    INNER JOIN fn_rbac_FullCollectionMembership(@UserSIDs)  fc on fc.CollectionID = @Collection and fc.ResourceID = aee.MachineID
    WHERE AppCI = @Application and ((@ErrorCode is NULL and ErrorCode is NULL) or (ErrorCode = @ErrorCode))
    ) res
    GROUP BY MachineName, UserName, AppCI, ApplicationName, DTCI, DeploymentTypeName, ErrorCode 
END

IF (@CollectionType = 1)
BEGIN
    SELECT DISTINCT MachineName, UserName, AppCI, ApplicationName, DTCI, DeploymentTypeName, Date, ErrorCode 
    FROM 
    fn_appevalerrors(dbo.fn_LShortNameToLCID(@locale)) aee
    INNER JOIN fn_rbac_CollectionExpandedUserMembers(@UserSIDs)  cm on cm.SiteID = @Collection
    INNER JOIN fn_rbac_R_User(@UserSIDs)  ud on ud.ResourceID = cm.UserItemKey
    WHERE AppCI = @Application and aee.UserName = ud.Unique_User_Name0 and ((@ErrorCode is NULL and ErrorCode is NULL) or (ErrorCode = @ErrorCode))
END</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Collection">
            <Value>=Parameters!Collection.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Application">
            <Value>=Parameters!Application.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ErrorCode">
            <Value>=Parameters!ErrorCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
