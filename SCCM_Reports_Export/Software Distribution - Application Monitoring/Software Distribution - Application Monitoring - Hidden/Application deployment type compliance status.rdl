    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SubCategory">
          <DataField>SubCategory</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="EnforcementState">
          <DataField>EnforcementState</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>DECLARE @CollectionType int

SELECT @CollectionType = CollectionType FROM fn_rbac_Collection(@UserSIDs)  WHERE CollectionID = @Collection

IF (@StatusType = 1) AND (@CollectionType = 2)
BEGIN
SELECT AppState AS EnforcementState, SUM(1) AS Total, NULL AS SubCategory
FROM
(
SELECT DISTINCT MachineID, UserID, AppState
FROM fn_appdtclientsummarizedstate(dbo.fn_LShortNameToLCID(@locale)) acs
INNER JOIN fn_rbac_FullCollectionMembership(@UserSIDs)  fc ON fc.CollectionID = @Collection AND fc.ResourceID = acs.MachineID
WHERE AppState/1000 = 1 and AppCI = @Application and DTCI = @DeploymentType
) res
GROUP BY AppState
END

IF (@StatusType = 1) AND (@CollectionType = 1)
BEGIN
SELECT AppState AS EnforcementState, SUM(1) AS Total, NULL AS SubCategory
FROM
(
SELECT DISTINCT MachineID, UserID, AppState
FROM fn_appdtclientsummarizedstate(dbo.fn_LShortNameToLCID(@locale)) acs
INNER JOIN fn_rbac_R_User(@UserSIDs)  ud on ud.Unique_User_Name0 = acs.UserName
INNER JOIN fn_rbac_CollectionExpandedUserMembers(@UserSIDs)  cm on cm.SiteID = @Collection and cm.UserItemKey = ud.ResourceID
WHERE AppCI = @Application AND DTCI = @DeploymentType AND AppState/1000=1 
) res
GROUP BY AppState
END

IF (@StatusType = 3) AND (@CollectionType = 2)
BEGIN
SELECT AppState AS EnforcementState, SUM(1) AS Total, RequirementName AS SubCategory
FROM
(
SELECT DISTINCT MachineID, UserID, AppState, RequirementName
FROM fn_appdtclientsummarizedstate(dbo.fn_LShortNameToLCID(@locale)) acs
INNER JOIN fn_rbac_FullCollectionMembership(@UserSIDs)  fc ON fc.CollectionID = @Collection AND fc.ResourceID = acs.MachineID
WHERE AppState/1000 = 3 and AppCI = @Application and DTCI = @DeploymentType
) res
GROUP BY AppState, RequirementName
END

IF (@StatusType = 3) AND (@CollectionType = 1)
BEGIN
SELECT AppState AS EnforcementState, SUM(1) AS Total, RequirementName AS SubCategory
FROM
(
SELECT DISTINCT MachineID, UserID, AppState, RequirementName
FROM fn_appdtclientsummarizedstate(dbo.fn_LShortNameToLCID(@locale)) acs
INNER JOIN fn_rbac_R_User(@UserSIDs)  ud on ud.Unique_User_Name0 = acs.UserName
INNER JOIN fn_rbac_CollectionExpandedUserMembers(@UserSIDs)  cm on cm.SiteID = @Collection and cm.UserItemKey = ud.ResourceID
WHERE AppCI = @Application  AND DTCI = @DeploymentType AND AppState/1000=3
) res
GROUP BY AppState, RequirementName
END

IF (@StatusType = 5) AND (@CollectionType = 2)
BEGIN
SELECT AppState AS EnforcementState, SUM(1) AS Total, ErrorCode AS SubCategory
FROM
(
SELECT DISTINCT MachineID, UserID, AppState, ErrorCode
FROM fn_appdtclientsummarizedstate(dbo.fn_LShortNameToLCID(@locale)) acs
INNER JOIN fn_rbac_FullCollectionMembership(@UserSIDs)  fc ON fc.CollectionID = @Collection AND fc.ResourceID = acs.MachineID
WHERE AppState/1000 = 5 and AppCI = @Application and DTCI = @DeploymentType
) res
GROUP BY AppState, ErrorCode
END

IF (@StatusType = 5) AND (@CollectionType = 1)
BEGIN
SELECT AppState AS EnforcementState, SUM(1) AS Total, ErrorCode AS SubCategory
FROM
(
SELECT DISTINCT MachineID, UserID, AppState, ErrorCode
FROM fn_appdtclientsummarizedstate(dbo.fn_LShortNameToLCID(@locale)) acs
INNER JOIN fn_rbac_R_User(@UserSIDs)  ud on ud.Unique_User_Name0 = acs.UserName
INNER JOIN fn_rbac_CollectionExpandedUserMembers(@UserSIDs)  cm on cm.SiteID = @Collection and cm.UserItemKey = ud.ResourceID
WHERE AppCI = @Application AND DTCI = @DeploymentType AND AppState/1000=5
) res
GROUP BY AppState, ErrorCode
END</CommandText>
