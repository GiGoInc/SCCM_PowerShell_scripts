  <DataSets>
    <DataSet Name="GlobalExpression">
      <Fields>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CI_UniqueID">
          <DataField>CI_UniqueID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select loc.DisplayName, 
           ci.CI_UniqueID  
from fn_rbac_ConfigurationItems(@UserSIDs)  ci 
inner join fn_rbac_LocalizedCIProperties(dbo.fn_LShortNameToLCID(@locale), @UserSIDs) loc on loc.CI_ID = ci.CI_ID 
where ci.CIType_ID = 13</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Setting">
          <DataField>Setting</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PreReifiedValue">
          <DataField>PreReifiedValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CI_UniqueID">
          <DataField>CI_UniqueID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UniqueRequirementName">
          <DataField>UniqueRequirementName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>select loc.DisplayName as Setting,
       crd.PreReifiedValue,
       ciset.CI_UniqueID,
       cis.CI_UniqueID as UniqueRequirementName
from fn_rbac_ConfigurationItems(@UserSIDs)  ci
inner join fn_rbac_CICurrentComplianceStatus(@UserSIDs)  ccs on ccs.CI_ID = ci.CI_ID
inner join fn_rbac_R_System(@UserSIDs)  sd on ccs.ResourceID = sd.ResourceID
inner join fn_rbac_Users(@UserSIDs)  us on us.UserID = ccs.UserID
inner join fn_rbac_CICurrentRuleDetail(@UserSIDs)  crd on crd.CurrentComplianceStatusID = ccs.CI_CurrentComplianceStatusID
left join fn_rbac_ConfigurationItems(@UserSIDs)  ciset on ciset.CI_ID = crd.Setting_CI_ID and ciset.CIType_ID = 13
left join fn_rbac_LocalizedCIProperties(dbo.fn_LShortNameToLCID(@locale), @UserSIDs) loc on loc.CI_ID = crd.Setting_CI_ID
left join fn_rbac_CISettings(@UserSIDs)  sett on sett.Setting_ID = crd.SettingID
left join fn_rbac_ConfigurationItems(@UserSIDs)  cis on cis.CI_ID = sett.CI_ID
where ci.CI_UniqueID = @GlobalExpression and sd.Netbios_Name0 = @Device and ((us.FullName = @User) or (us.UserID = 0 and @User = ''))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@GlobalExpression">
            <Value>=Parameters!GlobalExpression.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Device">
            <Value>=Parameters!Device.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@User">
            <Value>=Parameters!User.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
