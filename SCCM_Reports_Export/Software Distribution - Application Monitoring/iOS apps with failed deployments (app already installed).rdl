            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT CollectionID, Name AS CollectionName FROM fn_rbac_Collection(@UserSIDs)  ORDER BY Name</CommandText>
