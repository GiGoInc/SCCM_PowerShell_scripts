    <DataSet Name="Application">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Distinct AppCIs.CI_ID, AppCIs.DisplayName from fn_rbac_ListLatestApplicationCIs(dbo.fn_LShortNameToLCID(@locale), 'disabled') AppCIs
		inner join fn_rbac_ListDeploymentTypeCIs(dbo.fn_LShortNameToLCID(@locale), 'disabled') DTCIs on 
		AppCIs.ModelName = DTCIs.AppModelName
	 	where Technology = 'iOSDeepLink' AND AppCIs.IsLatest =1 ORDER BY DisplayName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CI_ID">
          <DataField>CI_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Collection">
            <Value>=Parameters!Collection.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Application">
            <Value>=Parameters!Application.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @CollectionType int

SELECT @CollectionType = CollectionType FROM fn_rbac_Collection(@UserSIDs)  WHERE CollectionID = @Collection

IF (@CollectionType = 2)
BEGIN

SELECT ApplicationName AS ApplicationName, UserName AS UserName ,srl.Netbios_Name0 AS DeviceName
FROM dbo.fn_AppClientSummarizedState(dbo.fn_LShortNameToLCID(@locale)) acs
 INNER JOIN fn_rbac_FullCollectionMembership(@UserSIDs) fc ON fc.CollectionID = @Collection AND fc.ResourceID = acs.MachineID
 INNER JOIN fn_rbac_R_System(@UserSIDs) srl ON acs.MachineID = srl.ResourceID
  WHERE AppCI = @Application and ErrorCode = 12025

  END

IF (@CollectionType = 1)
BEGIN

SELECT ApplicationName AS ApplicationName,Netbios_Name0 AS DeviceName,ud.Name0 AS UserName
FROM fn_appclientsummarizedstate(dbo.fn_LShortNameToLCID(@locale)) acs
INNER JOIN fn_rbac_CollectionExpandedUserMembers(@UserSIDs)  us ON us.SiteID = @Collection
INNER JOIN fn_rbac_R_User(@UserSIDs)  ud ON ud.ResourceID = us.UserItemKey
INNER JOIN fn_rbac_R_System(@UserSIDs) srl ON acs.MachineID = srl.ResourceID
WHERE AppCI = @Application and acs.UserName = ud.Unique_User_Name0 and acs.ErrorCode = 12025

END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ApplicationName">
          <DataField>ApplicationName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="DeviceName">
          <DataField>DeviceName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="UserName">
          <DataField>UserName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
