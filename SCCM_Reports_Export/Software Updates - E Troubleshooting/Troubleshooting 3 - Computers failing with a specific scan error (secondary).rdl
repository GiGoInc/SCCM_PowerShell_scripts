        </Field>
        <Field Name="LastScanTime">
          <DataField>LastScanTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="LastScanPackageLocation">
          <DataField>LastScanPackageLocation</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastScanPackageVersion">
          <DataField>LastScanPackageVersion</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          select
            rsys.ResourceID,
            rsys.Name0 as MachineName,
            rsys.Client_Version0 as SMSClientVersion,
            uss.LastWUAVersion as WUAVersion,
            rsys.User_Domain0+'\'+rsys.User_Name0 as LastLoggedOnUser,
            uss.LastStatusMessageID&amp;0x0000FFFF as ErrorStatusID,
            uss.LastErrorCode as LastErrorCode,
            uss.LastScanTime as LastScanTime,
            uss.LastScanPackageLocation as LastScanPackageLocation,
            uss.LastScanPackageVersion as LastScanPackageVersion
          from fn_rbac_SoftwareUpdateSource(@UserSIDs)  sus  
          join fn_rbac_UpdateScanStatus(@UserSIDs)  uss 
            on (sus.UpdateSource_ID=uss.UpdateSource_ID) 
            and (uss.LastStatusMessageID&amp;0x0000FFFF=@ErrorStatusID)
            and (isnull(uss.LastErrorCode,0)=@ErrorCode)
          join fn_rbac_ClientCollectionMembers(@UserSIDs)  ccm  on (uss.ResourceID=ccm.ResourceID) and (ccm.CollectionID=@CollID)
          join fn_rbac_R_System(@UserSIDs)  rsys  on rsys.ResourceID = uss.ResourceID
          where (sus.UpdateSource_UniqueID = @UpdateSourceID) 
          order by rsys.Name0
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UpdateSourceID">
            <Value>=Parameters!UpdateSourceID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollID">
            <Value>=Parameters!CollID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ErrorStatusID">
            <Value>=Parameters!ErrorStatusID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ErrorCode">
            <Value>=Parameters!ErrorCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet0">
