        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
              select us.UpdateSourceName as UpdateSource,
              us.UpdateSourceDescription as Description,
              us.UpdateSourceVersion as Version,
              us.SourceSite as SourceSite,
              @UpdateSourceID as UpdateSourceID
              from fn_rbac_SoftwareUpdateSource(@UserSIDs)  us 
              where us.UpdateSource_UniqueID = @UpdateSourceID
            </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UpdateSourceID">
            <Value>=Parameters!UpdateSourceID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_DataSet_UpdateSourceID">
      <Fields>
        <Field Name="UpdateSourceID">
          <DataField>UpdateSourceID</DataField>
        </Field>
        <Field Name="UpdateSourceName">
          <DataField>UpdateSourceName</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          select sus.UpdateSource_UniqueID UpdateSourceID,
            sus.UpdateSourceName as UpdateSourceName
          from fn_rbac_SoftwareUpdateSource(@UserSIDs)  sus
          where sus.IsExpired=0
          order by sus.UpdateSourceName
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_DataSet_CollID">
      <Fields>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
        </Field>
        <Field Name="UnlocCollectionName">
          <DataField>CollectionName</DataField>
        </Field>
        <Field Name="CollectionName">
          <Value>=Fields!CollectionID.Value &amp; " - "&amp;SrsResources.Localization.GetDBString(Fields!UnlocCollectionName.Value, User!Language)</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          select CollectionID, CollectionName=Name, NameSort=CollectionID+' - '+Name 
          from fn_rbac_Collection(@UserSIDs)  
          order by 2
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_DataSet_ErrorStatusID">
      <Fields>
        <Field Name="StatusID">
          <DataField>StatusID</DataField>
        </Field>
        <Field Name="UnlocStatusName">
          <DataField>StatusName</DataField>
        </Field>
        <Field Name="StatusName">
          <Value>=SrsResources.Localization.GetDBString(Fields!UnlocStatusName.Value, User!Language)</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          select distinct
            uss.LastStatusMessageID&amp;0x0000FFFF as StatusID,
            asi.MessageName as StatusName
          from fn_rbac_UpdateScanStatus(@UserSIDs)  uss  
          join fn_rbac_SoftwareUpdateSource(@UserSIDs)  sus  on sus.UpdateSource_ID=uss.UpdateSource_ID and sus.UpdateSource_UniqueID=@UpdateSourceID
          join fn_rbac_ClientCollectionMembers(@UserSIDs)  ccm  on ccm.ResourceID=uss.ResourceID and ccm.CollectionID=@CollID
          left join fn_rbac_AdvertisementStatusInformation(@UserSIDs)  asi on asi.MessageID=uss.LastStatusMessageID&amp;0x0000FFFF
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UpdateSourceID">
            <Value>=Parameters!UpdateSourceID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollID">
            <Value>=Parameters!CollID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_DataSet_ErrorCode">
      <Fields>
        <Field Name="ErrorCode">
          <DataField>ErrorCode</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          select distinct isnull(uss.LastErrorCode,0) as ErrorCode
          from fn_rbac_SoftwareUpdateSource(@UserSIDs)  sus  
          join fn_rbac_UpdateScanStatus(@UserSIDs)  uss  on (sus.UpdateSource_ID=uss.UpdateSource_ID) and (uss.LastStatusMessageID&amp;0x0000FFFF=@ErrorStatusID)
          join fn_rbac_ClientCollectionMembers(@UserSIDs)  ccm  on (uss.ResourceID=ccm.ResourceID) and (ccm.CollectionID=@CollID)
          where (sus.UpdateSource_UniqueID = @UpdateSourceID) 
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
