        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Parameter_DataSet_UpdateSourceID">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          select sus.UpdateSource_UniqueID UpdateSourceID,
                 sus.UpdateSourceName as UpdateSourceName
          from fn_rbac_SoftwareUpdateSource(@UserSIDs)  sus
          where sus.IsExpired=0
          order by sus.UpdateSourceName
        </CommandText>
      </Query>
      <Fields>
        <Field Name="UpdateSourceID">
          <DataField>UpdateSourceID</DataField>
        </Field>
        <Field Name="UpdateSourceName">
