        </Field>
        <Field Name="ErrorCodeInt">
          <DataField>ErrorCodeInt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DeploymentID">
          <DataField>DeploymentID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          select
            assc.ResourceID,
            MachineName = sys.Name0,
            SMSClientVersion = sys.Client_Version0,
            LastLoggedOnUser = sys.User_Domain0+'\'+sys.User_Name0, 
            LastEnforcementTime = assc.LastEnforcementMessageTime, 
            ErrorStatusID = assc.LastEnforcementErrorID&amp;0x0000FFFF,  
            ErrorCode = isnull(master.dbo.fn_varbintohexstr(CONVERT(VARBINARY(8),CONVERT(int,assc.LastEnforcementErrorCode))), 0),
            ErrorCodeInt = assc.LastEnforcementErrorCode,
            DeploymentID = cia.Assignment_UniqueID
            from fn_rbac_CIAssignment(@UserSIDs)  cia   
            join fn_rbac_UpdateAssignmentStatus_Live(@UserSIDs)  assc  
                on assc.AssignmentID = cia.AssignmentID 
                and isnull(assc.IsCompliant, 0)=0
                and assc.LastEnforcementMessageID in (6,9) 
                and isnull(assc.LastEnforcementErrorCode,0)=@ErrorCode
                and assc.LastEnforcementErrorID&amp;0x0000FFFF=@ErrorStatusID  
            join fn_rbac_R_System(@UserSIDs)  sys  on assc.ResourceID=sys.ResourceID and isnull(sys.Obsolete0,0)=0
            where cia.Assignment_UniqueID=@AssignmentID  
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ErrorCode">
            <Value>=Parameters!ErrorCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ErrorStatusID">
            <Value>=Parameters!ErrorStatusID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AssignmentID">
            <Value>=Parameters!AssignmentID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="DeploymentID">
          <DataField>DeploymentID</DataField>
        </Field>
        <Field Name="DeploymentName">
          <DataField>DeploymentName</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          select DeploymentID = cia.Assignment_UniqueID
               , DeploymentName = isnull(grp.Title+' - ', '') + cia.AssignmentName
          from fn_rbac_CIAssignment(@UserSIDs)  cia
          left join fn_rbac_CIAssignmentToGroup(@UserSIDs)  atg on cia.AssignmentType=5 and atg.AssignmentID=cia.AssignmentID
          left join fn_rbac_AuthListInfo(@lcid, @UserSIDs) grp on cia.AssignmentType=5 and grp.CI_ID=atg.AssignedUpdateGroup
          where cia.AssignmentType in (1,5)
            and (@filterwildcard='' or cia.Assignment_UniqueID like @filterwildcard or cia.AssignmentName like @filterwildcard or grp.Title like @filterwildcard)
          order by 2
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet2">
      <Fields>
        <Field Name="State">
          <DataField>State</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="StatusNameUnloc">
          <DataField>DisplayText</DataField>
        </Field>
        <Field Name="DisplayText">
          <Value>=Fields!State.Value &amp; " - "&amp;SrsResources.Localization.GetDBString(Fields!StatusNameUnloc.Value, User!Language)</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
            select State = sm.MessageID, DisplayText = isnull(asi.MessageName, '')
            from (
                select distinct MessageID=isnull(assc.LastEnforcementErrorID&amp;0x0000FFFF,0)
                from fn_rbac_CIAssignment(@UserSIDs)  cia   
                join fn_rbac_UpdateAssignmentStatus_Live(@UserSIDs)  assc  on assc.AssignmentID = cia.AssignmentID and isnull(assc.IsCompliant, 0)=0 and assc.LastEnforcementMessageID in (6,9)
                where cia.Assignment_UniqueID=@AssignmentID
                ) sm
          left join fn_rbac_AdvertisementStatusInformation(@UserSIDs)  asi on sm.MessageID = asi.MessageID 
          order by sm.MessageID 
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AssignmentID">
            <Value>=Parameters!AssignmentID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet3">
      <Fields>
        <Field Name="ErrorCode">
          <DataField>ErrorCode</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DisplayText">
          <DataField>DisplayText</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
            select ErrorCode, DisplayText = convert(varchar, ErrorCode) + ' ('+dbo.fn_ConvertBinaryToHexString(convert(VARBINARY(8), CONVERT(int,ErrorCode))) + ')'
            from (
                select distinct ErrorCode=isnull(LastEnforcementErrorCode, 0)
                from fn_rbac_CIAssignment(@UserSIDs)  cia   
                join fn_rbac_UpdateAssignmentStatus_Live(@UserSIDs)  assc  
                    on assc.AssignmentID = cia.AssignmentID 
                    and isnull(assc.IsCompliant, 0)=0
                    and assc.LastEnforcementMessageID in (6,9)
                    and assc.LastEnforcementErrorID&amp;0x0000FFFF=@ErrorStatusID  
                where cia.Assignment_UniqueID=@AssignmentID
                ) errc
            order by 1
        
        
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ErrorStatusID">
            <Value>=Parameters!ErrorStatusID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AssignmentID">
            <Value>=Parameters!AssignmentID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
