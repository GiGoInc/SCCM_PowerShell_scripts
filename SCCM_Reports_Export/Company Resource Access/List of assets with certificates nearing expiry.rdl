    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@filterwildcard_Device">
            <Value>=Parameters!DeviceFilter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllValueParameter">
            <Value>=Parameters!AllValueParameter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          begin
          if (@filterwildcard_Device = '')
          SELECT @AllValueParameter AS "Computer Name"
          UNION ALL
          SELECT distinct Netbios_Name0 as "Computer Name" FROM v_CertificateStatus
          else
          SELECT @AllValueParameter AS "Computer Name"
          UNION ALL
          SELECT distinct Netbios_Name0 as "Computer Name" FROM v_CertificateStatus where Netbios_Name0 like '%' + @filterwildcard_Device + '%'
          end</CommandText>
      </Query>
      <Fields>
        <Field Name="Computer_Name">
          <DataField>Computer Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
