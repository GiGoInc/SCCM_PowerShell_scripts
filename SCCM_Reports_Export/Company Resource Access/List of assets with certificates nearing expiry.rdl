  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@DeviceName">
            <Value>=Parameters!DeviceName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserName">
            <Value>=Parameters!UserName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllValueParameter">
            <Value>=Parameters!AllValueParameter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ExpiryDate">
            <Value>=Parameters!ExpiryDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)

          select LOC.DisplayName, c.* from v_CertificateStatus c left join fn_LocalizedCIProperties(@lcid) as LOC on c.CI_ID=LOC.CI_ID
          where (@DeviceName=  @AllValueParameter OR Netbios_Name0 = @DeviceName) and (@UserName = @AllValueParameter OR User_Name0=@UserName) and CertExpirationDate &lt;= @ExpiryDate
        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
